            SuperscriptBox["u", "2"], ")"}]}]},
         {
          RowBox[{"-", 
           RowBox[{"sin", "(", 
            SuperscriptBox["u", "1"], ")"}]}]}
        }], ")"}]}]}]}], ",", " ", 
   RowBox[{
    SubscriptBox["e", "2"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", "x"}], 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["u", "2"]}]], "=", 
     RowBox[{"a", " ", 
      RowBox[{"(", GridBox[{
         {
          RowBox[{
           RowBox[{"-", 
            RowBox[{"sin", "(", 
             SuperscriptBox["u", "1"], ")"}]}], " ", 
           RowBox[{"sin", "(", 
            SuperscriptBox["u", "2"], ")"}]}]},
         {
          RowBox[{
           RowBox[{"sin", "(", 
            SuperscriptBox["u", "1"], ")"}], " ", 
           RowBox[{"cos", "(", 
            SuperscriptBox["u", "2"], ")"}]}]},
         {"0"}
        }], ")"}]}]}]}]}], TraditionalForm]], "Equation",
