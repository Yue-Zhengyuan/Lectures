 FormBox[
  RowBox[{
   RowBox[{
    SuperscriptBox["e", "i"], 
    SubscriptBox["e", "j"]}], "=", 
   RowBox[{
    SubscriptBox[
     RowBox[{
      SubscriptBox[
       RowBox[{"[", 
        RowBox[{"\[DifferentialD]", 
