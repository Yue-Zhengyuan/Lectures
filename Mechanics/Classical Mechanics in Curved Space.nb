Cell[BoxData[
 FormBox[GridBox[{
    {
     RowBox[{
      SuperscriptBox["x", "1"], "="}], 
     RowBox[{"a", " ", "sin", " ", 
      SuperscriptBox["u", "1"], "cos", " ", 
      SuperscriptBox["u", "2"]}]},
    {
     RowBox[{
      SuperscriptBox["x", "2"], "="}], 
