Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["e", "1"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", "x"}], 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["u", "1"]}]], "=", 
     RowBox[{"a", " ", 
      RowBox[{"(", GridBox[{
         {
          RowBox[{
           RowBox[{"cos", "(", 
