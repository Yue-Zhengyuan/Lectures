Cell[BoxData[
 FormBox[
  RowBox[{"L", "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[Integral]", "curve"], 
     SqrtBox[
      RowBox[{
       RowBox[{
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"\[DifferentialD]", 
            SuperscriptBox["x", "1"]}], ")"}], "2"], "+"}], "..."}], "+", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"\[DifferentialD]", 
          SuperscriptBox["x", "n"]}], ")"}], "2"]}]]}], "=", 
    RowBox[{
     SubscriptBox["\[Integral]", "curve"], 
     SqrtBox[
      RowBox[{
       RowBox[{"\[DifferentialD]", "x"}], "\[CenterDot]", 
