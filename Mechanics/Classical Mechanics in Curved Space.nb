       SubscriptBox["g", 
        RowBox[{"i", " ", "j"}]], "(", "q", ")"}], 
      SuperscriptBox[
       OverscriptBox["q", "."], "j"]}], "+", 
     RowBox[{
      FractionBox["1", "\[ScriptCapitalL]"], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         FractionBox[
          RowBox[{"\[DifferentialD]", 
           SubscriptBox["g", 
            RowBox[{"i", " ", "j"}]]}], 
          RowBox[{"\[DifferentialD]", "\[Lambda]"}]], 
         SuperscriptBox[
          OverscriptBox["q", "."], "j"]}], "+", 
        RowBox[{
         RowBox[{
          SubscriptBox["g", 
           RowBox[{"i", " ", "j"}]], "(", "q", ")"}], 
         SuperscriptBox[
          OverscriptBox["q", "\[DoubleDot]"], "j"]}]}], ")"}]}]}], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"\[DifferentialD]", 
        RowBox[{"(", 
         SuperscriptBox["\[ScriptCapitalL]", 
          RowBox[{"-", "1"}]], ")"}]}], 
       RowBox[{"\[DifferentialD]", "\[Lambda]"}]], 
      RowBox[{
       SubscriptBox["g", 
        RowBox[{"i", " ", "j"}]], "(", "q", ")"}], 
      SuperscriptBox[
       OverscriptBox["q", "."], "j"]}], "+", 
     RowBox[{
      FractionBox["1", "\[ScriptCapitalL]"], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{
