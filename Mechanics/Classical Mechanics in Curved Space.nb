 FormBox[GridBox[{
    {
     RowBox[{
      SuperscriptBox["x", "1"], "=", 
      RowBox[{
       SuperscriptBox["x", "1"], "(", 
       RowBox[{
        SuperscriptBox["q", "1"], ",", 
        SuperscriptBox["q", "2"]}], ")"}]}]},
    {
     RowBox[{
      SuperscriptBox["x", "2"], "=", 
      RowBox[{
       SuperscriptBox["x", "2"], "(", 
       RowBox[{
        SuperscriptBox["q", "1"], ",", 
        SuperscriptBox["q", "2"]}], ")"}]}]},
    {
     RowBox[{
      SuperscriptBox["x", "3"], "=", 
      RowBox[{
       SuperscriptBox["x", "3"], "(", 
       RowBox[{
        SuperscriptBox["q", "1"], ",", 
        SuperscriptBox["q", "2"]}], ")"}]}]}
   }], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.7841490608664503`*^9, 3.7841491060618353`*^9}, 
