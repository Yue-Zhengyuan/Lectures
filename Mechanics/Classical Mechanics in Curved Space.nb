Cell[BoxData[
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"\[DifferentialD]", 
        RowBox[{"(", 
         SuperscriptBox["\[ScriptCapitalL]", 
          RowBox[{"-", "1"}]], ")"}]}], 
