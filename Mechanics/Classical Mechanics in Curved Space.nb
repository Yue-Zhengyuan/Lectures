Cell[CellGroupData[{

Cell["The Acceleration", "Section",
 CellChangeTimes->{{3.7841582091593723`*^9, 3.7841582238503847`*^9}, {
   3.784158895836636*^9, 3.78415889981037*^9}, {3.784160365374674*^9, 
   3.7841603751939363`*^9}, {3.784161031090016*^9, 3.784161032184779*^9}, 
   3.7841894094450207`*^9, {3.7843308461880074`*^9, 
   3.784330852567952*^9}},ExpressionUUID->"50dbd402-655f-4160-b861-\
524d2e006441"],

Cell["\<\
Now we analyze the motion in the curvilinear coordinate system. We must \
consider the contribution by the changing of the basis vector. \
\>", "Text",
 CellChangeTimes->{{3.784151214533886*^9, 3.784151244857833*^9}, {
   3.784151308323916*^9, 3.7841513681943007`*^9}, {3.784158309692087*^9, 
   3.784158397408676*^9}, {3.7841591481698723`*^9, 3.7841593441768293`*^9}, {
   3.784160392803487*^9, 3.784160395265752*^9}, 3.78416079837329*^9, {
   3.784170302036487*^9, 
   3.7841703033258953`*^9}},ExpressionUUID->"2b83f5aa-c54d-4b59-af59-\
773e64b02b25"],

Cell["\<\
By definition of the acceleration, we take the time derivative of (1.7):\
\>", "Text",
 CellChangeTimes->{{3.7841604144694242`*^9, 
  3.7841604490829277`*^9}},ExpressionUUID->"4cf5b6d3-d6e9-4f4b-8657-\
7e8dd96a449b"],

Cell[BoxData[
 FormBox[
  RowBox[{"a", "=", 
   RowBox[{
    FractionBox[
     RowBox[{"\[DifferentialD]", "v"}], 
     RowBox[{"\[DifferentialD]", "t"}]], "=", 
    RowBox[{
     RowBox[{
      FractionBox["\[DifferentialD]", 
       RowBox[{"\[DifferentialD]", "t"}]], 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["v", "i"], 
        SubscriptBox["e", "i"]}], ")"}]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        FractionBox[
         RowBox[{"\[DifferentialD]", 
          SuperscriptBox["v", "i"]}], 
         RowBox[{"\[DifferentialD]", "t"}]], 
        SubscriptBox["e", "i"]}], "+", 
       RowBox[{
        SuperscriptBox["v", "i"], 
        FractionBox[
         RowBox[{"\[DifferentialD]", 
          SubscriptBox["e", "i"]}], 
         RowBox[{"\[DifferentialD]", "t"}]]}]}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{
         FractionBox[
          RowBox[{"\[DifferentialD]", 
           SuperscriptBox["v", "i"]}], 
          RowBox[{"\[DifferentialD]", "t"}]], 
         SubscriptBox["e", "i"]}], "+", 
        RowBox[{
         SuperscriptBox["v", "i"], 
         FractionBox[
          RowBox[{"\[DifferentialD]", 
           SuperscriptBox["q", "j"]}], 
          RowBox[{"\[DifferentialD]", "t"}]], 
         FractionBox[
          RowBox[{"\[PartialD]", 
           SubscriptBox["e", "i"]}], 
          RowBox[{"\[PartialD]", 
           SuperscriptBox["q", "j"]}]]}]}], "=", 
       RowBox[{
        RowBox[{
         FractionBox[
          RowBox[{"\[DifferentialD]", 
           SuperscriptBox["v", "i"]}], 
          RowBox[{"\[DifferentialD]", "t"}]], 
         SubscriptBox["e", "i"]}], "+", 
        RowBox[{
         SuperscriptBox["v", "i"], 
         SuperscriptBox["v", "j"], 
         FractionBox[
          RowBox[{"\[PartialD]", 
           SubscriptBox["e", "i"]}], 
          RowBox[{"\[PartialD]", 
           SubscriptBox["q", "j"]}]]}]}]}]}]}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.7841604144694242`*^9, 3.784160465313398*^9}, {
  3.7841605166954107`*^9, 3.78416063788623*^9}, {3.784160674096552*^9, 
  3.784160680868876*^9}, {3.784717063718752*^9, 
  3.784717063719974*^9}},ExpressionUUID->"976094fd-3aff-453f-b880-\
09facd71bd71"],

Cell[TextData[{
 "Here comes the key step: the derivative ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[PartialD]", 
     SubscriptBox["e", "i"]}], "/", 
    RowBox[{"\[PartialD]", 
     SubscriptBox["q", "j"]}]}], TraditionalForm]],ExpressionUUID->
  "71a5b33d-54c4-4dc3-97ba-f770d47246c2"],
 " can be expressed as a linear combination of the basis vectors ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Epsilon]", "k"], " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"k", "=", "1"}], ",", "2"}], ")"}]}], TraditionalForm]],
  ExpressionUUID->"61e597cb-1d65-48cf-b60e-4bdceb239fc4"],
 ":"
}], "Text",
 CellChangeTimes->{{3.784152389051642*^9, 3.784152506775627*^9}, {
  3.784152752330781*^9, 3.784152787342527*^9}, {3.784160555240904*^9, 
  3.7841605553490458`*^9}, {3.784160765226864*^9, 3.784160765606783*^9}, {
  3.784717063720896*^9, 3.784717063720932*^9}, {3.784892664365119*^9, 
  3.784892664802533*^9}},ExpressionUUID->"3177cc23-014e-461e-9dc2-\
3a6ecd0f899c"],

Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    RowBox[{"\[PartialD]", 
     SubscriptBox["e", "i"]}], 
    RowBox[{"\[PartialD]", 
     SuperscriptBox["q", "j"]}]], "=", 
   RowBox[{
    SubsuperscriptBox["\[CapitalGamma]", 
     RowBox[{"i", " ", "j"}], "k"], 
    SubscriptBox["e", "k"], "   ", 
    RowBox[{"(", 
     RowBox[{"i", ",", 
      RowBox[{"j", "=", "1"}], ",", "2"}], ")"}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.784152389051642*^9, 3.784152556898985*^9}, {
   3.784152658139307*^9, 3.7841526605888157`*^9}, {3.7841606521509438`*^9, 
   3.784160653440379*^9}, {3.784160696202601*^9, 3.7841606963282824`*^9}, {
   3.784160748812931*^9, 3.784160771819935*^9}, 
   3.7847170637214317`*^9},ExpressionUUID->"43086839-b708-4758-bdd4-\
7610e72fdfb5"],

Cell[TextData[{
 "The to-be-determined coefficients ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["\[CapitalGamma]", 
    RowBox[{"i", " ", "j"}], "k"], TraditionalForm]],ExpressionUUID->
  "d74c7275-2a92-40b3-8429-3363db6d95dc"],
 " are called the ",
 StyleBox["connection coefficients",
  FontWeight->"Bold"],
 " (or the ",
 StyleBox["Christoffel symbol",
  FontWeight->"Bold"],
 "). "
}], "Text",
 CellChangeTimes->{{3.784152706793432*^9, 3.784152707631447*^9}, {
   3.784152794838921*^9, 3.784152864558737*^9}, {3.78416065650103*^9, 
   3.78416065650154*^9}, 3.78416096169553*^9, {3.784172010835106*^9, 
   3.7841720453325253`*^9}, {3.784181638290636*^9, 3.7841816415388937`*^9}, {
   3.784188738043414*^9, 3.7841887706141*^9}, 3.7842464056169233`*^9, {
   3.784246448985579*^9, 3.784246458720512*^9}, {3.784892677005013*^9, 
   3.78489267744125*^9}},ExpressionUUID->"352252c7-473f-4321-acf0-\
9b844fc5f673"],

Cell[TextData[{
 "We point out that the connection coefficients are ",
 StyleBox["symmetric with respect to the two subscripts",
  FontSlant->"Italic"],
 ", i.e."
}], "Text",
 CellChangeTimes->{{3.784152706793432*^9, 3.784152707631447*^9}, {
   3.784152794838921*^9, 3.784152864558737*^9}, {3.78416065650103*^9, 
   3.78416065650154*^9}, 3.78416096169553*^9, {3.784172010835106*^9, 
   3.7841720453325253`*^9}, {3.784181638290636*^9, 3.7841816415388937`*^9}, {
   3.784188738043414*^9, 3.784188744688734*^9}, {3.784188777069759*^9, 
   3.784188777822603*^9}, 3.784246405669561*^9, {3.784246476030789*^9, 
   3.7842464762720757`*^9}},ExpressionUUID->"2e6ca632-e124-485d-b334-\
baa1e8e0118d"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubsuperscriptBox["\[CapitalGamma]", 
    RowBox[{"i", " ", "j"}], "k"], "=", 
   SubsuperscriptBox["\[CapitalGamma]", 
    RowBox[{"j", " ", "i"}], "k"]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.784188501608801*^9, 
  3.784188548852366*^9}},ExpressionUUID->"1717e887-7dbc-49b1-8ef1-\
cfb1288f23e8"],

Cell[TextData[{
 "This is because ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[PartialD]", 
      SubscriptBox["e", "i"]}], "/", 
     RowBox[{"\[PartialD]", 
      SuperscriptBox["q", "j"]}]}], "=", 
    RowBox[{
     RowBox[{"\[PartialD]", 
      SubscriptBox["e", "j"]}], "/", 
     RowBox[{"\[PartialD]", 
      SuperscriptBox["q", "i"]}]}]}], TraditionalForm]],ExpressionUUID->
  "1ee70cfa-2f5b-4a25-89b3-caeb0bdf4b09"],
 ", which can be proved using ",
 StyleBox["the symmetry of mixed partial derivatives",
  FontSlant->"Italic"],
 ":"
}], "Text",
 CellChangeTimes->{{3.784188555272112*^9, 3.784188592698594*^9}, {
  3.7843295804028482`*^9, 3.7843295938307962`*^9}, {3.784717063722446*^9, 
  3.7847170637230186`*^9}},ExpressionUUID->"eda6af2d-a3b6-43c2-a76c-\
fce861f38ad5"],

Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    RowBox[{"\[PartialD]", 
     SubscriptBox["e", "i"]}], 
    RowBox[{"\[PartialD]", 
     SuperscriptBox["q", "j"]}]], "=", 
   RowBox[{
    RowBox[{
     FractionBox["\[PartialD]", 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["q", "j"]}]], 
     RowBox[{"(", 
      FractionBox[
       RowBox[{"\[PartialD]", "x"}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["q", "i"]}]], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      FractionBox["\[PartialD]", 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["q", "i"]}]], 
      RowBox[{"(", 
       FractionBox[
        RowBox[{"\[PartialD]", "x"}], 
        RowBox[{"\[PartialD]", 
         SuperscriptBox["q", "j"]}]], ")"}]}], "=", 
     FractionBox[
      RowBox[{"\[PartialD]", 
       SubscriptBox["e", "j"]}], 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["q", "i"]}]]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.784188555272112*^9, 3.784188655631311*^9}, 
   3.78471701855689*^9, {3.784717063723527*^9, 
   3.784717063725436*^9}},ExpressionUUID->"894a1c05-6a16-4d95-95c8-\
bcdbcbd5cf19"],

Cell[TextData[{
 "The acceleration can now be expressed as (recall that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["v", "i"], "=", 
    RowBox[{
     RowBox[{"\[DifferentialD]", 
      SuperscriptBox["q", "i"]}], "/", 
     RowBox[{"\[DifferentialD]", "t"}]}]}], TraditionalForm]],ExpressionUUID->
  "823ccbe1-9db2-4cc9-8141-69daecf1fc15"],
 " from eq. (1.7))"
}], "Text",
 CellChangeTimes->{{3.784152706793432*^9, 3.784152707631447*^9}, {
   3.784152794838921*^9, 3.784152864558737*^9}, {3.78416065650103*^9, 
   3.78416065650154*^9}, 3.78416096169553*^9, {3.784172010835106*^9, 
   3.7841720453325253`*^9}, {3.784181638290636*^9, 3.7841816415388937`*^9}, {
   3.7841887830714493`*^9, 3.78418879253432*^9}, {3.784717020046879*^9, 
   3.784717020046924*^9}},ExpressionUUID->"b2ba12c6-6229-454d-ad66-\
172061adf56a"],

Cell[BoxData[
 FormBox[
  RowBox[{"a", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       FractionBox[
        RowBox[{"\[DifferentialD]", 
         SuperscriptBox["v", "k"]}], 
        RowBox[{"\[DifferentialD]", "t"}]], "+", 
       RowBox[{
        SuperscriptBox["v", "i"], 
        SuperscriptBox["v", "j"], 
        SubsuperscriptBox["\[CapitalGamma]", 
         RowBox[{"i", " ", "j"}], "k"]}]}], ")"}], 
     SubscriptBox["e", "k"]}], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       FractionBox[
        RowBox[{
         SuperscriptBox["\[DifferentialD]", "2"], 
         SuperscriptBox["q", "k"]}], 
        RowBox[{"\[DifferentialD]", 
         SuperscriptBox["t", "2"]}]], "+", 
       RowBox[{
        FractionBox[
         RowBox[{"\[DifferentialD]", 
          SuperscriptBox["q", "i"]}], 
         RowBox[{"\[DifferentialD]", "t"}]], 
        FractionBox[
         RowBox[{"\[DifferentialD]", 
          SuperscriptBox["q", "j"]}], 
         RowBox[{"\[DifferentialD]", "t"}]], 
        SubsuperscriptBox["\[CapitalGamma]", 
         RowBox[{"i", " ", "j"}], "k"]}]}], ")"}], 
     SubscriptBox["e", "k"]}]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.784152706793432*^9, 3.784152707631447*^9}, {
  3.784152794838921*^9, 3.784152864558737*^9}, {3.784160661317609*^9, 
  3.78416071906666*^9}, {3.7841629954430447`*^9, 3.7841630220858393`*^9}, {
  3.78416306058162*^9, 3.784163060862358*^9}, {3.78417200444934*^9, 
