d88bb117a5bf"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["g", 
    RowBox[{"i", " ", "j"}]], "=", 
   RowBox[{"(", GridBox[{
      {
       SuperscriptBox["a", "2"], "0"},
      {"0", 
       RowBox[{
        SuperscriptBox["a", "2"], 
        SuperscriptBox["sin", "2"], 
        SuperscriptBox["u", "1"]}]}
     }], ")"}]}], TraditionalForm]], "Equation",
