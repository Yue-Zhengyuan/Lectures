Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["e", "1"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", "x"}], 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["q", "1"]}]], "=", 
     RowBox[{"a", " ", 
      RowBox[{"(", GridBox[{
         {
          RowBox[{
           RowBox[{"cos", "(", 
            SuperscriptBox["q", "1"], ")"}], " ", 
           RowBox[{"cos", "(", 
            SuperscriptBox["q", "2"], ")"}]}]},
         {
          RowBox[{
           RowBox[{"cos", "(", 
            SuperscriptBox["q", "1"], ")"}], " ", 
           RowBox[{"sin", "(", 
            SuperscriptBox["q", "2"], ")"}]}]},
         {
          RowBox[{"-", 
           RowBox[{"sin", "(", 
            SuperscriptBox["q", "1"], ")"}]}]}
        }], ")"}]}]}]}], ",", " ", 
   RowBox[{
    SubscriptBox["e", "2"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", "x"}], 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["q", "2"]}]], "=", 
     RowBox[{"a", " ", 
      RowBox[{"(", GridBox[{
         {
          RowBox[{
           RowBox[{"-", 
            RowBox[{"sin", "(", 
             SuperscriptBox["q", "1"], ")"}]}], " ", 
           RowBox[{"sin", "(", 
            SuperscriptBox["q", "2"], ")"}]}]},
         {
          RowBox[{
           RowBox[{"sin", "(", 
            SuperscriptBox["q", "1"], ")"}], " ", 
           RowBox[{"cos", "(", 
            SuperscriptBox["q", "2"], ")"}]}]},
         {"0"}
        }], ")"}]}]}]}]}], TraditionalForm]], "Equation",
