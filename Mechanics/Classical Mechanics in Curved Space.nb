 FormBox[
  RowBox[{
   RowBox[{
    SuperscriptBox["x", "i"], "=", 
    RowBox[{
     SuperscriptBox["x", "i"], "(", 
     RowBox[{
      SuperscriptBox["q", "1"], ",", "...", ",", 
      SuperscriptBox["q", "m"]}], ")"}]}], ",", "    ", 
   RowBox[{"i", "=", "1"}], ",", "...", ",", "n"}], 
  TraditionalForm]], "EquationNumbered",
