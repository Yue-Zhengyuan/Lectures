cd7ca45a441e"],

Cell[BoxData[
 FormBox[
  RowBox[{"L", "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[Integral]", "curve"], 
     SqrtBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"\[DifferentialD]", 
          SuperscriptBox["q", "i"]}], 
         FractionBox[
          RowBox[{"\[PartialD]", "x"}], 
          RowBox[{"\[PartialD]", 
           SuperscriptBox["q", "i"]}]]}], ")"}], "\[CenterDot]", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"\[DifferentialD]", 
          SuperscriptBox["q", "j"]}], 
         FractionBox[
          RowBox[{"\[PartialD]", "x"}], 
          RowBox[{"\[PartialD]", 
           SuperscriptBox["q", "j"]}]]}], ")"}]}]]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[Integral]", "curve"], 
      SqrtBox[
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["e", "i"], "\[CenterDot]", 
          SubscriptBox["e", "j"]}], ")"}], 
        RowBox[{"\[DifferentialD]", 
         SuperscriptBox["q", "i"]}], 
        RowBox[{"\[DifferentialD]", 
         SuperscriptBox["q", "j"]}]}]]}], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[Integral]", "curve"], 
       SqrtBox[
        RowBox[{
         RowBox[{
          SubscriptBox["g", 
           RowBox[{"i", " ", "j"}]], "(", "q", ")"}], 
         RowBox[{"\[DifferentialD]", 
          SuperscriptBox["q", "i"]}], 
         RowBox[{"\[DifferentialD]", 
          SuperscriptBox["q", "j"]}]}]]}], "=", 
      RowBox[{
       RowBox[{
        SubsuperscriptBox["\[Integral]", 
         SubscriptBox["\[Lambda]", "a"], 
         SubscriptBox["\[Lambda]", "b"]], 
        RowBox[{
         RowBox[{"\[DifferentialD]", "\[Lambda]"}], 
         SqrtBox[
          RowBox[{
           RowBox[{
            SubscriptBox["g", 
             RowBox[{"i", " ", "j"}]], "(", "q", ")"}], 
           FractionBox[
            RowBox[{"\[DifferentialD]", 
             SuperscriptBox["q", "i"]}], 
            RowBox[{"\[DifferentialD]", "\[Lambda]"}]], 
           FractionBox[
            RowBox[{"\[DifferentialD]", 
             SuperscriptBox["q", "j"]}], 
            RowBox[{"\[DifferentialD]", "\[Lambda]"}]]}]]}]}], "=", 
       RowBox[{
        SubsuperscriptBox["\[Integral]", 
         SubscriptBox["\[Lambda]", "a"], 
         SubscriptBox["\[Lambda]", "b"]], 
        RowBox[{
         RowBox[{"\[DifferentialD]", "\[Lambda]"}], " ", 
         RowBox[{"\[ScriptCapitalL]", "(", 
          RowBox[{"q", ",", 
           OverscriptBox["q", "."], ",", "\[Lambda]"}], ")"}]}]}]}]}]}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.784687176842078*^9, 3.7846874080071*^9}, {
  3.784687557739897*^9, 3.784687625713333*^9}, {3.78468834336007*^9, 
