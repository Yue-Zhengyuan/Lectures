          RowBox[{"\[PartialD]", "P"}], "/", 
          RowBox[{"\[PartialD]", "S"}]}]},
        {
         RowBox[{
          RowBox[{"\[PartialD]", "T"}], "/", 
          RowBox[{"\[PartialD]", "V"}]}], 
         RowBox[{
          RowBox[{"\[PartialD]", "P"}], "/", 
          RowBox[{"\[PartialD]", "V"}]}]}
       }], ")"}]}], "=", 
    RowBox[{
     RowBox[{"-", 
      FractionBox[
       RowBox[{"\[PartialD]", 
        RowBox[{"(", 
         RowBox[{"T", ",", "P"}], ")"}]}], 
       RowBox[{"\[PartialD]", 
        RowBox[{"(", 
         RowBox[{"S", ",", "V"}], ")"}]}]]}], ">", "0"}]}]}], 
