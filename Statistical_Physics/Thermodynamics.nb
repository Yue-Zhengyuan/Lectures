Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"det", "(", GridBox[{
      {
       RowBox[{
        RowBox[{"\[PartialD]", "T"}], "/", 
        RowBox[{"\[PartialD]", "S"}]}], 
       RowBox[{
        RowBox[{"-", 
         RowBox[{"\[PartialD]", "P"}]}], "/", 
