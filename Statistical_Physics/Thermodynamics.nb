        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"\[Delta]", " ", "S"}], ")"}], "2"]}], "+", 
       RowBox[{
        FractionBox[
         RowBox[{
          SuperscriptBox["\[PartialD]", "2"], "E"}], 
         RowBox[{
          RowBox[{"\[PartialD]", "S"}], 
          RowBox[{"\[PartialD]", "V"}]}]], 
        RowBox[{"(", 
         RowBox[{"\[Delta]", " ", "S", " ", "\[Delta]", " ", "V"}], ")"}]}], 
       "+", 
       RowBox[{
        FractionBox[
         RowBox[{
          SuperscriptBox["\[PartialD]", "2"], "E"}], 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["V", "2"]}]], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"\[Delta]", " ", "V"}], ")"}], "2"]}]}], ")"}]}]}]}], 
  TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{
     SubscriptBox["T", "0"], "\[Delta]", " ", "S"}], "-", 
    RowBox[{
     SubscriptBox["P", "0"], "\[Delta]", " ", "V"}], "+", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        FractionBox[
         RowBox[{
          SuperscriptBox["\[PartialD]", "2"], "E"}], 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["S", "2"]}]], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"\[Delta]", " ", "S"}], ")"}], "2"]}], "+", 
       RowBox[{
        FractionBox[
         RowBox[{
          SuperscriptBox["\[PartialD]", "2"], "E"}], 
         RowBox[{
          RowBox[{"\[PartialD]", "S"}], 
          RowBox[{"\[PartialD]", "V"}]}]], 
        RowBox[{"(", 
         RowBox[{"\[Delta]", " ", "S", " ", "\[Delta]", " ", "V"}], ")"}]}], 
       "+", 
       RowBox[{
        FractionBox[
         RowBox[{
          SuperscriptBox["\[PartialD]", "2"], "E"}], 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["V", "2"]}]], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"\[Delta]", " ", "V"}], ")"}], "2"]}]}], ")"}]}]}]}], 
  TraditionalForm]}], "Equation",
 CellChangeTimes->{{3.793588942982375*^9, 3.793589086902656*^9}, {
  3.793589147151855*^9, 
