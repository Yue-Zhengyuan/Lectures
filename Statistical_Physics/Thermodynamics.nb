Cell[BoxData[
 FormBox[
  RowBox[{"P", "\[Congruent]", 
   RowBox[{"-", 
    RowBox[{"\[LeftAngleBracket]", 
     FractionBox[
      RowBox[{"\[PartialD]", "H"}], 
      RowBox[{"\[PartialD]", "V"}]], "\[RightAngleBracket]"}]}]}], 
