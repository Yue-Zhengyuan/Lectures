        RowBox[{"\[PartialD]", "S"}]], ")"}], "V"], "\[Delta]", " ", "S"}], 
    "+", 
    RowBox[{
     SubscriptBox[
      RowBox[{"(", 
       FractionBox[
        RowBox[{"\[PartialD]", "E"}], 
        RowBox[{"\[PartialD]", "V"}]], ")"}], "S"], "\[Delta]", " ", "V"}], 
    "+", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        FractionBox[
         RowBox[{
          SuperscriptBox["\[PartialD]", "2"], "E"}], 
         RowBox[{"\[PartialD]", 
