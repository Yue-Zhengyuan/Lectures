 CellChangeTimes->{{3.7848109573259706`*^9, 
  3.784810961650693*^9}},ExpressionUUID->"08d103f6-89f1-464a-bb47-\
7a3322b3dc48"],

Cell[TextData[{
 "For a system ",
 StyleBox["symmetric under rigid transformations",
  FontSlant->"Italic"],
 ", we only need to consider the terms containing the derivative ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[PartialD]", "\[Mu]"], 
    SubscriptBox["\[Omega]", "a"]}], TraditionalForm]],ExpressionUUID->
  "fdf50715-3bc1-45cb-ad50-1b63e2cff338"],
 ": "
}], "Text",
 CellChangeTimes->{{3.784808515363016*^9, 3.784808544833037*^9}, {
   3.784808667421382*^9, 3.7848086892346*^9}, {3.7848087674626513`*^9, 
   3.784808911682624*^9}, {3.7848107517182293`*^9, 3.7848108219783297`*^9}, 
   3.784810965617167*^9, {3.785227899235031*^9, 3.78522789973764*^9}, {
   3.7865745858737087`*^9, 3.786574593846325*^9}, {3.788162659364481*^9, 
   3.788162661281529*^9}},ExpressionUUID->"0d54d61e-9ffe-4844-a62a-\
4f45cf712f2d"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[Delta]", " ", "S"}], "=", 
   RowBox[{"\[Integral]", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[DifferentialD]", "d"], "x"}], "  ", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["\[PartialD]", "\[Mu]"], 
        SubscriptBox["\[Omega]", "a"]}], ")"}], "[", 
      RowBox[{
       RowBox[{
        FractionBox[
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "\[Prime]\[Mu]"]}], 
         RowBox[{"\[PartialD]", 
          SubscriptBox["\[Omega]", "a"]}]], 
        RowBox[{"\[ScriptCapitalL]", "(", 
         RowBox[{"\[Phi]", ",", 
          RowBox[{"\[PartialD]", "\[Phi]"}]}], ")"}]}], "+", 
       RowBox[{
        FractionBox[
         RowBox[{"\[PartialD]", "\[ScriptCapitalF]"}], 
         RowBox[{"\[PartialD]", 
          SubscriptBox["\[Omega]", "a"]}]], 
        FractionBox[
         RowBox[{"\[PartialD]", "\[ScriptCapitalL]"}], 
         RowBox[{"\[PartialD]", 
          RowBox[{"(", 
           RowBox[{
            SubscriptBox["\[PartialD]", "\[Mu]"], "\[Phi]"}], ")"}]}]]}], "-", 
       RowBox[{
        FractionBox[
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "\[Prime]\[Nu]"]}], 
         RowBox[{"\[PartialD]", 
          SubscriptBox["\[Omega]", "a"]}]], 
        FractionBox[
         RowBox[{"\[PartialD]", "\[ScriptCapitalL]"}], 
         RowBox[{"\[PartialD]", 
          RowBox[{"(", 
           RowBox[{
            SubscriptBox["\[PartialD]", "\[Mu]"], "\[Phi]"}], ")"}]}]], 
        RowBox[{
         SubscriptBox["\[PartialD]", "\[Nu]"], "\[Phi]"}]}]}], "]"}]}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.784804471056096*^9, 3.784804472710073*^9}, {
  3.784811048207933*^9, 3.784811074858522*^9}, {3.784811260178898*^9, 
  3.784811260775114*^9}, {3.788137627852108*^9, 3.7881376278542233`*^9}, {
  3.7881598299530163`*^9, 
  3.788159831538076*^9}},ExpressionUUID->"0fc224d8-111b-4509-9619-\
d8cc530c4f00"],

Cell["Now we define the current", "Text",
 CellChangeTimes->{{3.788162556397881*^9, 
  3.78816256084968*^9}},ExpressionUUID->"50f22df0-1ed7-4527-8029-\
b9f8d8235fdb"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubsuperscriptBox["j", "a", "\[Mu]"], "=", 
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "\[Prime]\[Nu]"]}], 
      RowBox[{"\[PartialD]", 
       SubscriptBox["\[Omega]", "a"]}]], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        FractionBox[
         RowBox[{"\[PartialD]", "\[ScriptCapitalL]"}], 
         RowBox[{"\[PartialD]", 
          RowBox[{"(", 
           RowBox[{
            SubscriptBox["\[PartialD]", "\[Mu]"], "\[Phi]"}], ")"}]}]], 
        RowBox[{
         SubscriptBox["\[PartialD]", "\[Nu]"], "\[Phi]"}]}], "-", 
       RowBox[{
        SubsuperscriptBox["\[Delta]", "\[Nu]", "\[Mu]"], 
        "\[ScriptCapitalL]"}]}], ")"}]}], "-", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", "\[ScriptCapitalL]"}], 
      RowBox[{"\[PartialD]", 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["\[PartialD]", "\[Mu]"], "\[Phi]"}], ")"}]}]], 
     FractionBox[
      RowBox[{"\[PartialD]", "\[ScriptCapitalF]"}], 
      RowBox[{"\[PartialD]", 
       SubscriptBox["\[Omega]", "a"]}]]}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.78481119337908*^9, 3.7848112132754087`*^9}, {
  3.788137627860896*^9, 
  3.788137627862047*^9}},ExpressionUUID->"61cfc768-5c76-4f0b-aea6-\
05d4d77c0ad9"],

Cell["Then", "Text",
 CellChangeTimes->{{3.788162574613517*^9, 
  3.788162575639968*^9}},ExpressionUUID->"aeb623f4-72cf-4d31-a80f-\
92f5c8a2e462"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[Delta]", " ", "S"}], "=", 
   RowBox[{"-", 
    RowBox[{"\[Integral]", 
     RowBox[{
      RowBox[{
       SuperscriptBox["\[DifferentialD]", "d"], "x"}], " ", 
      SubsuperscriptBox["j", "a", "\[Mu]"], 
      RowBox[{
       SubscriptBox["\[PartialD]", "\[Mu]"], 
       SubscriptBox["\[Omega]", "a"]}]}]}]}]}], TraditionalForm]], "Equation",\

 CellChangeTimes->{{3.788162574613517*^9, 
  3.788162601645488*^9}},ExpressionUUID->"68b651f6-5eee-4bac-a08f-\
877a2ae3fc17"],

Cell["Integrating by parts gives", "Text",
 CellChangeTimes->{{3.784804471056096*^9, 3.784804472710073*^9}, {
  3.784811048207933*^9, 3.784811074858522*^9}, {3.784811260178898*^9, 
  3.784811260775114*^9}, {3.788137627852108*^9, 3.7881376278542233`*^9}, {
  3.7881598299530163`*^9, 3.788159837858882*^9}, {3.788162608216896*^9, 
  3.788162612857003*^9}},ExpressionUUID->"3e407265-82d4-4303-ad01-\
c4f6d76ae48c"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[Delta]", " ", "S"}], "=", 
   RowBox[{
    RowBox[{"\[Integral]", 
     RowBox[{
      RowBox[{
       SuperscriptBox["\[DifferentialD]", "d"], "x"}], "  ", 
      SubscriptBox["\[Omega]", "a"], 
      RowBox[{
       SubscriptBox["\[PartialD]", "\[Mu]"], 
       SubsuperscriptBox["j", "a", "\[Mu]"]}]}]}], 
    OverscriptBox["=", "!"], "0"}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.784811253914606*^9, 
  3.7848112757532253`*^9}},ExpressionUUID->"33e459bb-13ac-49f5-a089-\
4dc45aecc041"],

Cell[TextData[{
 "Because of the arbitrariness of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Omega]", "a"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "0ac57ab3-df8d-4f20-9bde-65365c862a59"],
 ", this current should also be conserved:"
}], "Text",
 CellChangeTimes->{{3.784811239087873*^9, 3.784811251231536*^9}, {
  3.786574405705759*^9, 
  3.786574422370647*^9}},ExpressionUUID->"2cfadc1f-f229-4a27-8413-\
52f4dba73719"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[PartialD]", "\[Mu]"], 
    SubsuperscriptBox["j", "a", "\[Mu]"]}], "=", "0"}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.784811239087873*^9, 3.784811250670401*^9}, {
  3.784811285467469*^9, 
  3.7848112868128138`*^9}},ExpressionUUID->"c0473cba-7185-405d-9373-\
fc7135a0d92d"],

Cell[TextData[{
 "In fact, this current is a generalization of the energy-momentum tensor: if \
we take ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Omega]", "a"], "\[Rule]", 
     SuperscriptBox["\[Omega]", "\[Rho]"]}], "=", 
    RowBox[{"\[Delta]", " ", 
     SuperscriptBox["x", "\[Rho]"]}]}], TraditionalForm]],ExpressionUUID->
  "7250eb5e-3a56-4fec-a91a-9415e7aac9aa"],
 ", since translations (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["x", "\[Prime]\[Nu]"], "=", 
    RowBox[{
     SuperscriptBox["x", "\[Nu]"], "+", 
     RowBox[{"\[Delta]", " ", 
      SuperscriptBox["x", "\[Nu]"]}]}]}], TraditionalForm]],ExpressionUUID->
  "104bceb3-0fce-4d5e-adce-e41fe49a55d6"],
 ") do not change the field (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[PartialD]", "\[ScriptCapitalF]"}], "/", 
     RowBox[{"\[PartialD]", 
      RowBox[{"(", 
       RowBox[{"\[Delta]", " ", 
        SuperscriptBox["x", "\[Rho]"]}], ")"}]}]}], "=", "0"}], 
   TraditionalForm]],
  CellChangeTimes->{{3.78481119337908*^9, 3.7848112132754087`*^9}},
  ExpressionUUID->"355e1db5-cc1b-4c1d-9c95-6f255feba997"],
 "), we obtain "
}], "Text",
 CellChangeTimes->{{3.784811307687242*^9, 3.784811406802149*^9}, {
  3.78481145245811*^9, 3.784811524566183*^9}, {3.7848660077676687`*^9, 
  3.784866027935095*^9}, {3.7865744670674133`*^9, 3.786574481854067*^9}, {
  3.786574512644947*^9, 
  3.786574518818334*^9}},ExpressionUUID->"6278b872-fb5a-4c6e-ab45-\
ecfd305937ca"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubsuperscriptBox["j", "\[Rho]", "\[Mu]"], "=", 
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Delta]", "\[Rho]", "\[Nu]"], "(", 
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{"\[PartialD]", "\[ScriptCapitalL]"}], 
        RowBox[{"\[PartialD]", 
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["\[PartialD]", "\[Mu]"], "\[Phi]"}], ")"}]}]], 
       RowBox[{
        SubscriptBox["\[PartialD]", "\[Nu]"], "\[Phi]"}]}], "-", 
      RowBox[{
       SubsuperscriptBox["\[Delta]", "\[Nu]", "\[Mu]"], 
       "\[ScriptCapitalL]"}]}], ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{"\[PartialD]", "\[ScriptCapitalL]"}], 
        RowBox[{"\[PartialD]", 
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["\[PartialD]", "\[Mu]"], "\[Phi]"}], ")"}]}]], 
       RowBox[{
        SubscriptBox["\[PartialD]", "\[Rho]"], "\[Phi]"}]}], "-", 
      RowBox[{
       SubsuperscriptBox["\[Delta]", "\[Rho]", "\[Mu]"], 
       "\[ScriptCapitalL]"}]}], "=", 
     SubsuperscriptBox["T", "\[Rho]", "\[Mu]"]}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.784811307687242*^9, 3.784811406802149*^9}, {
  3.78481145245811*^9, 3.784811585623447*^9}, {3.788137627864853*^9, 
  3.788137627866486*^9}},ExpressionUUID->"56b89f05-d745-4079-ba22-\
f6806969973d"],

Cell["To summarize:", "Text",
 CellChangeTimes->{{3.784812037316765*^9, 3.784812048932405*^9}, {
  3.784812093501395*^9, 3.784812102083735*^9}, {3.784812234806149*^9, 
  3.784812241333219*^9}},ExpressionUUID->"47923ecf-5f6d-4026-8d90-\
08dff950b24d"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   SubscriptBox["j", "a"], TraditionalForm]],ExpressionUUID->
  "436c0afc-6702-4a84-afcc-ef477592da9f"],
 " is the current",
 StyleBox[" ",
  FontWeight->"Bold"],
 "associated with any parameter ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     StyleBox["\[Omega]",
      FontSlant->"Plain"], "a"], "(", "x", ")"}], TraditionalForm]],
  ExpressionUUID->"a17d610e-3702-4165-a5ec-4145fc33e6c0"],
 " of continuous symmetry transformations."
}], "Theorem",
 CellChangeTimes->{{3.78481205870989*^9, 3.784812149353078*^9}, {
  3.78481223177684*^9, 3.784812247894554*^9}, {3.784812851224783*^9, 
  3.784812852340317*^9}, {3.786571239013958*^9, 
  3.7865712450285063`*^9}},ExpressionUUID->"0bd868c7-2911-47dc-9bda-\
c8a57753fb3b"]
}, Open  ]],

