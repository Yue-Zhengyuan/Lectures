        RowBox[{"\[Rho]", " ", "\[Sigma]"}]]}]], 
     RowBox[{"(", 
      RowBox[{"x", ",", "0"}], ")"}], " ", 
     RowBox[{
      SubscriptBox["\[PartialD]", "\[Mu]"], 
      RowBox[{"\[Phi]", "(", "x", ")"}]}]}], "=", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SuperscriptBox["\[Eta]", 
         RowBox[{"\[Mu]", " ", "\[Rho]"}]], 
        SuperscriptBox["x", "\[Sigma]"]}], "-", 
       RowBox[{
        SuperscriptBox["\[Eta]", 
         RowBox[{"\[Mu]", " ", "\[Sigma]"}]], 
        SuperscriptBox["x", "\[Rho]"]}]}], ")"}], 
     RowBox[{
      SubscriptBox["\[PartialD]", "\[Mu]"], 
      RowBox[{"\[Phi]", "(", "x", ")"}]}]}]}]}], 
  TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    FractionBox["1", "2"], 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["x", "\[Sigma]"], 
      RowBox[{
       SuperscriptBox["\[PartialD]", "\[Rho]"], 
       RowBox[{"-", 
        SuperscriptBox["x", "\[Rho]"]}]}], 
      SuperscriptBox["\[PartialD]", "\[Sigma]"]}], ")"}], 
    RowBox[{"\[Phi]", "(", "x", ")"}]}]}], TraditionalForm]}], "Equation",
 CellChangeTimes->{{3.786572269126306*^9, 3.786572295532464*^9}, 
   3.786572346273074*^9, 3.786572394500759*^9, {3.786572808647338*^9, 
   3.78657286384753*^9}, {3.786572899792811*^9, 3.786572900042871*^9}, {
   3.788137641870648*^9, 
   3.788137641872203*^9}},ExpressionUUID->"c3fc706a-9e6b-4bed-950c-\
2862eb6fe68a"],

Cell[TextData[{
