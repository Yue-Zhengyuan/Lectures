     RowBox[{
      RowBox[{
       RowBox[{"\[Phi]", "(", "x", ")"}], " ", "\[DoubleLongRightArrow]", " ", 
       FractionBox[
        RowBox[{"\[PartialD]", "\[ScriptCapitalF]"}], 
        RowBox[{"\[PartialD]", 
         SubscriptBox["\[Omega]", "a"]}]]}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"\[Phi]", "(", "x", ")"}], ",", "0"}], ")"}]}], "=", 
