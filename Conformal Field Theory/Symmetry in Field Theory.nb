Cell[CellGroupData[{

Cell["Conserved Charge", "Section",
 CellChangeTimes->{{3.784620029223323*^9, 3.784620045908188*^9}, 
   3.7846230130739307`*^9, 
   3.784867119745289*^9},ExpressionUUID->"ad33fe80-5bf0-4efb-b8a1-\
6418cb06472f"],

Cell[TextData[{
 "The ",
 StyleBox["conserved charge",
  FontWeight->"Bold"],
 " associated with the current ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["j", "a"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "bf198cc0-2a91-4421-ad9b-fbff10bad465"],
 " is defined as"
}], "Text",
 CellChangeTimes->{{3.784866803176156*^9, 
  3.784866824428774*^9}},ExpressionUUID->"c21ce7d4-c326-4d18-83ba-\
021983a6e1c8"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["Q", "a"], "=", 
   RowBox[{"\[Integral]", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[DifferentialD]", 
       RowBox[{"d", "-", "1"}]], "x"}], " ", 
     SubsuperscriptBox["j", "a", "0"]}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.784866803176156*^9, 
  3.784866850052226*^9}},ExpressionUUID->"35f83592-224c-4120-92eb-\
03f4a520e76b"],

Cell[TextData[{
 "The integration is carried in the spatial part only. We verify that its \
time (",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "0"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "12476948-ab49-4716-ae99-7c33e24bb67c"],
 ") derivative indeed vanishes:"
}], "Text",
 CellChangeTimes->{{3.784866855095923*^9, 
  3.784866900056718*^9}},ExpressionUUID->"bbbc1cb9-7217-434d-bea6-\
4b782fefb86e"],

Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    RowBox[{"\[DifferentialD]", 
     SubscriptBox["Q", "a"]}], 
    RowBox[{"\[DifferentialD]", 
     SuperscriptBox["x", "0"]}]], "=", 
   RowBox[{
    RowBox[{"\[Integral]", 
     RowBox[{
      RowBox[{
       SuperscriptBox["\[DifferentialD]", 
        RowBox[{"d", "-", "1"}]], "x"}], " ", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["\[PartialD]", "0"], 
        SubsuperscriptBox["j", "a", "0"]}], ")"}]}]}], "=", 
    RowBox[{"-", 
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{
        SuperscriptBox["\[DifferentialD]", 
         RowBox[{"d", "-", "1"}]], "x"}], " ", 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["\[PartialD]", "i"], 
         SubsuperscriptBox["j", "a", "i"]}], ")"}]}]}]}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.784866855095923*^9, 
  3.7848669544847*^9}},ExpressionUUID->"1852d8ed-be19-4adf-9dcc-a229d6a7832a"],

Cell[TextData[{
 "In the second equality we used ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[PartialD]", "\[Mu]"], 
     SubsuperscriptBox["j", "a", "\[Mu]"]}], "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "99e625e9-b47f-4fbc-85c1-758d3ab74dc9"],
 ". Gauss\[CloseCurlyQuote] Theorem then converts the integral to a surface \
integral at infinity, which gives zero provided that the current vanished \
sufficiently rapidly as ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "\[Rule]", "\[Infinity]"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d8183ae0-65e8-463c-b62f-6cfadf4b0bb6"],
 ". "
}], "Text",
 CellChangeTimes->{{3.7848669644285107`*^9, 
  3.784867075811695*^9}},ExpressionUUID->"dc135c08-1646-4dcc-9070-\
4bd53213151a"]
}, Open  ]],

Cell[CellGroupData[{

Cell["Correlation Functions and Ward Identity", "Section",
 CellChangeTimes->{{3.784620085659404*^9, 3.784620087893456*^9}, {
  3.784867108993825*^9, 
  3.784867115552904*^9}},ExpressionUUID->"ffe96d38-e913-493d-86a2-\
030458c1497d"],

Cell[TextData[{
 "The ",
 StyleBox["correlation function",
  FontWeight->"Bold"],
 " is defined by the \[OpenCurlyDoubleQuote]mean \
value\[CloseCurlyDoubleQuote] of a product of the field at different places"
}], "Text",
 CellChangeTimes->{{3.784882989187642*^9, 3.784883013733325*^9}, {
  3.784886723089067*^9, 
  3.7848867464836817`*^9}},ExpressionUUID->"29672ccd-c81d-4787-ac11-\
a951172e639d"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[LeftAngleBracket]", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Phi]", "(", 
       SubscriptBox["x", "1"], ")"}], "..."}], 
     RowBox[{"\[Phi]", "(", 
      SubscriptBox["x", "n"], ")"}]}], "\[RightAngleBracket]"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      FractionBox["1", "Z"], 
      RowBox[{"\[Integral]", 
       RowBox[{
        RowBox[{"[", 
         RowBox[{"\[DifferentialD]", "\[Phi]"}], "]"}], 
        RowBox[{"\[Phi]", "(", 
         SubscriptBox["x", "1"], ")"}]}]}]}], "..."}], 
    RowBox[{"\[Phi]", "(", 
     SubscriptBox["x", "n"], ")"}], " ", 
    RowBox[{"exp", "(", 
     RowBox[{"-", 
      RowBox[{"S", "[", "\[Phi]", "]"}]}], ")"}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.784882989187642*^9, 3.784883077132448*^9}, {
  3.7848831396366262`*^9, 3.784883142478765*^9}, {3.7848835847679853`*^9, 
  3.78488359388803*^9}, {3.78813762787077*^9, 
  3.788137627873369*^9}},ExpressionUUID->"4f8ce539-0425-4df6-8ad8-\
e78bf7c60fd4"],

Cell[TextData[{
 "Here ",
 Cell[BoxData[
  FormBox["Z", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "3bbec191-f968-494a-901e-3f16036a6c08"],
 " is the \[OpenCurlyDoubleQuote]vacuum\[CloseCurlyDoubleQuote] functional"
}], "Text",
 CellChangeTimes->{{3.7848830795292377`*^9, 3.7848830838299828`*^9}, {
  3.7848831204150763`*^9, 
  3.784883126175921*^9}},ExpressionUUID->"2e1ce962-3ec4-451c-bcc0-\
6ecdf5acdf23"],

Cell[BoxData[
 FormBox[
  RowBox[{"Z", "=", 
   RowBox[{"\[Integral]", 
    RowBox[{
     RowBox[{"[", 
      RowBox[{"\[DifferentialD]", "\[Phi]"}], "]"}], 
     RowBox[{"exp", "(", 
      RowBox[{"-", 
       RowBox[{"S", "[", "\[Phi]", "]"}]}], ")"}]}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.7848830795292377`*^9, 3.7848830838299828`*^9}, {
  3.7848831204150763`*^9, 3.7848831537628403`*^9}, {3.784883596562817*^9, 
  3.78488360137661*^9}, {3.788137627874798*^9, 
  3.7881376278752813`*^9}},ExpressionUUID->"f76abdb0-f97a-4cac-b43e-\
5d3b53e9c3ea"],

Cell[TextData[{
 "If the system is symmetric under some transformation, ",
 StyleBox["the correlation function at the same place should be invariant",
  FontSlant->"Italic"],
 ":"
}], "Text",
 CellChangeTimes->{{3.7848832674463463`*^9, 3.784883301397984*^9}, {
  3.78488333783922*^9, 3.784883343414419*^9}, {3.784886775578362*^9, 
  3.7848868067992*^9}},ExpressionUUID->"c98ebe62-1e78-4861-b3af-4864b5e5c81b"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[LeftAngleBracket]", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Phi]", "(", 
       SubsuperscriptBox["x", "1", "\[Prime]"], ")"}], "..."}], 
     RowBox[{"\[Phi]", "(", 
      SubsuperscriptBox["x", "n", "\[Prime]"], ")"}]}], 
    "\[RightAngleBracket]"}], "=", 
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      RowBox[{
