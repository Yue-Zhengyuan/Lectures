Cell[TextData[{
 "i.e. ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Omega]", 
    RowBox[{"\[Mu]", " ", "\[Nu]"}]], TraditionalForm]],ExpressionUUID->
  "2e4eef6f-02c6-40fb-896d-506b0c3c2b19"],
 " ",
 StyleBox["is",
  FontSlant->"Italic"],
 " ",
 StyleBox["antisymmetric",
  FontSlant->"Italic"],
 ". Therefore, when we calculate the derivative ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[PartialD]", 
     SuperscriptBox["x", "\[Prime]\[Mu]"]}], "/", 
    RowBox[{"\[PartialD]", 
     SubscriptBox["\[Omega]", 
      RowBox[{"\[Rho]", " ", "\[Sigma]"}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "bff2e84d-88db-4ba9-badc-0a4ce71e8567"],
 ", we will get two terms and a factor of 1/2:"
}], "Text",
 CellChangeTimes->{{3.784635679004387*^9, 3.7846356995680227`*^9}, {
  3.784635801601595*^9, 3.784635801928212*^9}, {3.78657251431577*^9, 
  3.786572550128518*^9}, {3.7865729922075853`*^9, 
  3.7865730057778473`*^9}},ExpressionUUID->"a74c4448-cdeb-486a-a9be-\
aaf89fdb9476"],

Cell[BoxData[{
 FormBox[
  RowBox[{
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", 
      SuperscriptBox["x", "\[Prime]\[Mu]"]}], 
     RowBox[{"\[PartialD]", 
      SubscriptBox["\[Omega]", 
       RowBox[{"\[Rho]", " ", "\[Sigma]"}]]}]], 
    RowBox[{"(", 
     RowBox[{"x", ",", "0"}], ")"}]}], "=", 
   RowBox[{
    FractionBox["\[PartialD]", 
     RowBox[{"\[PartialD]", 
      SubscriptBox["\[Omega]", 
       RowBox[{"\[Rho]", " ", "\[Sigma]"}]]}]], 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["x", "\[Mu]"], "+", 
      RowBox[{
       FractionBox["1", "2"], 
       SuperscriptBox["\[Eta]", 
        RowBox[{"\[Mu]", " ", "\[Rho]"}]], 
       SubscriptBox["\[Omega]", 
        RowBox[{"\[Rho]", " ", "\[Sigma]"}]], 
       SuperscriptBox["x", "\[Sigma]"]}], "+", 
      UnderscriptBox[
       UnderscriptBox[
        RowBox[{
         FractionBox["1", "2"], 
         SuperscriptBox["\[Eta]", 
          RowBox[{"\[Mu]", " ", "\[Sigma]"}]], 
         SubscriptBox["\[Omega]", 
          RowBox[{"\[Sigma]", " ", "\[Rho]"}]], 
         SuperscriptBox["x", "\[Rho]"]}], "\[UnderBrace]"], 
       RowBox[{"rename", " ", "\[Sigma]", " ", "and", " ", "\[Rho]"}]]}], 
     ")"}]}]}], TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{
     FractionBox["\[PartialD]", 
      RowBox[{"\[PartialD]", 
       SubscriptBox["\[Omega]", 
        RowBox[{"\[Rho]", " ", "\[Sigma]"}]]}]], 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["x", "\[Mu]"], "+", 
       RowBox[{
        FractionBox["1", "2"], 
        SuperscriptBox["\[Eta]", 
         RowBox[{"\[Mu]", " ", "\[Rho]"}]], 
        SubscriptBox["\[Omega]", 
         RowBox[{"\[Rho]", " ", "\[Sigma]"}]], 
        SuperscriptBox["x", "\[Sigma]"]}], "-", 
       RowBox[{
        FractionBox["1", "2"], 
        SuperscriptBox["\[Eta]", 
         RowBox[{"\[Mu]", " ", "\[Sigma]"}]], 
        SubscriptBox["\[Omega]", 
         RowBox[{"\[Rho]", " ", "\[Sigma]"}]], 
        SuperscriptBox["x", "\[Rho]"]}]}], ")"}]}], "=", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SuperscriptBox["\[Eta]", 
         RowBox[{"\[Mu]", " ", "\[Rho]"}]], 
        SuperscriptBox["x", "\[Sigma]"]}], "-", 
       RowBox[{
        SuperscriptBox["\[Eta]", 
         RowBox[{"\[Mu]", " ", "\[Sigma]"}]], 
        SuperscriptBox["x", "\[Rho]"]}]}], ")"}]}]}]}], 
  TraditionalForm]}], "Equation",
 CellChangeTimes->{{3.784635679004387*^9, 3.7846356995680227`*^9}, {
   3.784635801601595*^9, 3.784635801928212*^9}, {3.78657251431577*^9, 
