4bcf4cf7eec3"],

Cell[BoxData[
 FormBox[
  RowBox[{"LHS", "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[Eta]", 
      RowBox[{"\[Mu]", " ", "\[Nu]"}]], " ", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox[
        SuperscriptBox["\[Delta]", "\[Mu]"], "\[Rho]"], "+", 
       SubscriptBox[
        SuperscriptBox["\[Omega]", "\[Mu]"], "\[Rho]"]}], ")"}], 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox[
        SuperscriptBox["\[Delta]", "\[Nu]"], "\[Sigma]"], "+", 
       SubscriptBox[
        SuperscriptBox["\[Omega]", "\[Nu]"], "\[Sigma]"]}], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["\[Eta]", 
         RowBox[{"\[Nu]", " ", "\[Rho]"}]], "+", 
        SubscriptBox["\[Omega]", 
         RowBox[{"\[Nu]", " ", "\[Rho]"}]]}], ")"}], 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox[
         SuperscriptBox["\[Delta]", "\[Nu]"], "\[Sigma]"], "+", 
        SubscriptBox[
         SuperscriptBox["\[Omega]", "\[Nu]"], "\[Sigma]"]}], ")"}]}], "=", 
     RowBox[{
      SubscriptBox["\[Eta]", 
       RowBox[{"\[Rho]", " ", "\[Sigma]"}]], "+", 
      SubscriptBox["\[Omega]", 
       RowBox[{"\[Rho]", " ", "\[Sigma]"}]], "+", 
      SubscriptBox["\[Omega]", 
       RowBox[{"\[Sigma]", " ", "\[Rho]"}]], "+", 
      RowBox[{"O", "(", 
       SuperscriptBox["\[Omega]", "2"], ")"}]}]}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.784635233902163*^9, 3.7846353451910143`*^9}, {
  3.784635417088005*^9, 3.784635425411821*^9}, {3.784635514724876*^9, 
  3.78463566597654*^9}},ExpressionUUID->"99da342e-303b-49b1-93aa-\
eeb8383aeb48"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[DoubleLongRightArrow]", "      ", 
    SubscriptBox["\[Omega]", 
     RowBox[{"\[Rho]", " ", "\[Sigma]"}]]}], "=", 
   RowBox[{"-", " ", 
    SubscriptBox["\[Omega]", 
     RowBox[{"\[Sigma]", " ", "\[Rho]"}]]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.784635233902163*^9, 3.7846353451910143`*^9}, {
  3.784635417088005*^9, 3.784635425411821*^9}, {3.784635514724876*^9, 
