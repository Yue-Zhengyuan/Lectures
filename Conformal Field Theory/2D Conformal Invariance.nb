      SubscriptBox["\[Epsilon]", "\[Nu]"], 
      SuperscriptBox["T", 
       RowBox[{"\[Mu]", " ", "\[Nu]"}]]}], ")"}]}], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[Epsilon]", "\[Nu]"], 
     RowBox[{
      SubscriptBox["\[PartialD]", "\[Mu]"], 
      SuperscriptBox["T", 
       RowBox[{"\[Mu]", " ", "\[Nu]"}]]}]}], "+", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SubscriptBox["\[PartialD]", "\[Mu]"], 
        SubscriptBox["\[Epsilon]", "\[Nu]"]}], "+", 
       RowBox[{
        SubscriptBox["\[PartialD]", "\[Nu]"], 
        SubscriptBox["\[Epsilon]", "\[Mu]"]}]}], ")"}], 
     SuperscriptBox["T", 
