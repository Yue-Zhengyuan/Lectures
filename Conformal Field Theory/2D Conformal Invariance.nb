 CellChangeTimes->{{3.78812588451336*^9, 3.788125904900448*^9}, 
   3.788163436894167*^9},ExpressionUUID->"84fc0689-9fca-4b4f-9c9b-\
19b1c667cb0d"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[Delta]", 
     RowBox[{"\[Epsilon]", ",", 
      OverscriptBox["\[Epsilon]", "_"]}]], 
    RowBox[{"\[LeftAngleBracket]", "X", "\[RightAngleBracket]"}]}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"-", 
      FractionBox["1", 
       RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]"}]]}], 
     RowBox[{
      SubscriptBox["\[ContourIntegral]", "C"], 
      RowBox[{
       RowBox[{"\[DifferentialD]", "z"}], " ", 
       RowBox[{"\[Epsilon]", "(", "z", ")"}], 
       RowBox[{"\[LeftAngleBracket]", 
        RowBox[{
         RowBox[{"T", "(", "z", ")"}], "X"}], "\[RightAngleBracket]"}]}]}]}], 
    "+", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]"}]], 
     RowBox[{
      SubscriptBox["\[ContourIntegral]", "C"], 
      RowBox[{
       RowBox[{"\[DifferentialD]", 
