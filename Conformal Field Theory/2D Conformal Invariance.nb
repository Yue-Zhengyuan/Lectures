Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{
     SubsuperscriptBox["e", "\[Mu]", "a"], 
     SubsuperscriptBox["e", "\[Nu]", "b"], 
     SuperscriptBox["g", 
      RowBox[{"\[Mu]", " ", "\[Nu]"}]]}], "=", 
    SuperscriptBox["\[Eta]", 
     RowBox[{"a", " ", "b"}]]}], ",", " ", 
   RowBox[{
    SubscriptBox["g", 
     RowBox[{"\[Mu]", " ", "\[Nu]"}]], "=", 
