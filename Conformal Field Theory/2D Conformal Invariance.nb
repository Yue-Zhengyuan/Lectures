     RowBox[{"x", "+", 
      RowBox[{"\[ImaginaryI]", " ", "y"}]}]}], ",", " ", 
    RowBox[{
     OverscriptBox["z", "_"], "\[Congruent]", 
     RowBox[{"x", "-", 
      RowBox[{"\[ImaginaryI]", " ", "y"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "4893e51d-5104-433a-be87-080399fdd2cd"]
