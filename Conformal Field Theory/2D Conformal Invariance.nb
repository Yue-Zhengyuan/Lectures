Cell[TextData[{
 "It is useful to construct the ",
 StyleBox["holomorphic coordinates",
  FontWeight->"Bold"],
 " from the Euclidean coordinates ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["x", "0"], ",", 
    SuperscriptBox["x", "1"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "1d51d162-df8b-4d79-9fe2-f8636c654cee"],
 ":"
}], "Text",
 CellChangeTimes->{{3.788135910028288*^9, 3.788135937064755*^9}, {
  3.788136017261277*^9, 3.7881360233057127`*^9}, {3.788136625784049*^9, 
  3.788136627669922*^9}},ExpressionUUID->"7784d0d7-7e0b-4c38-b483-\
4c9c9344e9bc"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"z", "=", 
    RowBox[{
     SuperscriptBox["x", "0"], "+", 
     RowBox[{"\[ImaginaryI]", " ", 
      SuperscriptBox["x", "1"]}]}]}], ",", " ", 
   RowBox[{
    OverscriptBox["z", "_"], "=", 
    RowBox[{
     SuperscriptBox["x", "0"], "-", 
     RowBox[{"\[ImaginaryI]", " ", 
      SuperscriptBox["x", "1"]}]}]}]}], TraditionalForm]], "EquationNumbered",\

 CellChangeTimes->{{3.788135910028288*^9, 3.788135937064755*^9}, {
  3.788136017261277*^9, 
  3.78813604773566*^9}},ExpressionUUID->"1acbe49b-e16c-4dcf-bd65-\
b4b50f0a2e62"],

