Cell[TextData[{
 "The correlation function ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[LeftAngleBracket]", 
    RowBox[{
     RowBox[{"A", "(", "z", ")"}], 
     RowBox[{"B", "(", "w", ")"}]}], "\[RightAngleBracket]"}], 
