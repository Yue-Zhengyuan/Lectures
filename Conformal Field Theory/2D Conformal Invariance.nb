Cell["The inverse relations are", "Text",
 CellChangeTimes->{{3.78813646133057*^9, 3.78813646482306*^9}, {
  3.788137439876871*^9, 
  3.788137476989951*^9}},ExpressionUUID->"bdafb7f4-5ab1-4f93-a4e6-\
e00f9cafca28"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SuperscriptBox["x", "0"], "=", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{"z", "+", 
       OverscriptBox["z", "_"]}], ")"}]}]}], ",", " ", 
   RowBox[{
    SuperscriptBox["x", "1"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"-", "\[ImaginaryI]"}], "2"], 
     RowBox[{"(", 
      RowBox[{"z", "-", 
       OverscriptBox["z", "_"]}], ")"}]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.78813646133057*^9, 3.788136491573852*^9}, {
  3.788136543739168*^9, 
  3.7881365458373213`*^9}},ExpressionUUID->"d6e18f29-5de8-456d-92ed-\
3d5543e86217"],

Cell["The transformation Jacobian is", "Text",
 CellChangeTimes->{{3.788136141657522*^9, 
  3.788136147236405*^9}},ExpressionUUID->"fc8c5001-6a78-4469-83e4-\
3f8161db6baf"],

Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    RowBox[{"D", "(", 
     RowBox[{"z", ",", 
      OverscriptBox["z", "_"]}], ")"}], 
    RowBox[{"D", "(", 
     RowBox[{
      SuperscriptBox["x", "0"], ",", 
      SuperscriptBox["x", "1"]}], ")"}]], "=", 
   RowBox[{
    RowBox[{"det", "(", GridBox[{
       {
        RowBox[{
         RowBox[{"\[PartialD]", "z"}], "/", 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "0"]}]}], 
        RowBox[{
         RowBox[{"\[PartialD]", "z"}], "/", 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "1"]}]}]},
       {
        RowBox[{
         RowBox[{"\[PartialD]", 
          OverscriptBox["z", "_"]}], "/", 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "0"]}]}], 
        RowBox[{
         RowBox[{"\[PartialD]", 
          OverscriptBox["z", "_"]}], "/", 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "1"]}]}]}
      }], ")"}], "=", 
    RowBox[{
     RowBox[{"det", "(", GridBox[{
        {"1", "\[ImaginaryI]"},
        {"1", 
         RowBox[{"-", "\[ImaginaryI]"}]}
       }], ")"}], "=", 
     RowBox[{
      RowBox[{"-", "2"}], "\[ImaginaryI]"}]}]}]}], 
