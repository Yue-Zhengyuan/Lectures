Cell[BoxData[{
 FormBox[
  RowBox[{
   RowBox[{"\[PartialD]", 
    RowBox[{
     OverscriptBox["\[PartialD]", "_"], "\[Phi]"}]}], "=", 
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "\[Rho]"]}], 
      RowBox[{"\[PartialD]", "z"}]], 
     RowBox[{
      FractionBox["\[PartialD]", 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "\[Rho]"]}]], 
      RowBox[{"(", 
       RowBox[{
        FractionBox[
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "\[Sigma]"]}], 
         RowBox[{"\[PartialD]", 
          OverscriptBox["z", "_"]}]], 
        FractionBox[
         RowBox[{"\[PartialD]", "\[Phi]"}], 
