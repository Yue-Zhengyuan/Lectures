d1431f4b0815"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"T", "(", "z", ")"}], 
    RowBox[{"\[Phi]", "(", 
     RowBox[{"w", ",", 
      OverscriptBox["w", "_"]}], ")"}]}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      FractionBox["h", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"z", "-", "w"}], ")"}], "2"]], 
      RowBox[{"\[Phi]", "(", 
       RowBox[{"w", ",", 
        OverscriptBox["w", "_"]}], ")"}]}], "+", 
     RowBox[{
      FractionBox["1", 
       RowBox[{"z", "-", "w"}]], 
