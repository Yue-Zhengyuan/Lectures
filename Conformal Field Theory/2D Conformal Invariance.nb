      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "0"]}], 
      RowBox[{"\[PartialD]", "z"}]], 
     SubscriptBox["T", "00"]}], "+", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "0"]}], 
      RowBox[{"\[PartialD]", "z"}]], 
     FractionBox[
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "1"]}], 
      RowBox[{"\[PartialD]", "z"}]], 
     SubscriptBox["T", "01"]}], "+", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "1"]}], 
      RowBox[{"\[PartialD]", "z"}]], 
     FractionBox[
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "0"]}], 
      RowBox[{"\[PartialD]", "z"}]], 
     SubscriptBox["T", "10"]}], "+", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "1"]}], 
      RowBox[{"\[PartialD]", "z"}]], 
     FractionBox[
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "1"]}], 
      RowBox[{"\[PartialD]", "z"}]], 
     SubscriptBox["T", "11"]}]}]}], TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{
     RowBox[{
      FractionBox["1", "2"], "\[Cross]", 
      FractionBox["1", "2"]}], 
     SubscriptBox["T", "00"]}], "+", 
    RowBox[{
     FractionBox["1", "2"], 
     FractionBox[
      RowBox[{"-", "\[ImaginaryI]"}], "2"], 
     SubscriptBox["T", "01"]}], "+", 
    RowBox[{
     FractionBox[
      RowBox[{"-", "\[ImaginaryI]"}], "2"], 
     FractionBox["1", "2"], 
     SubscriptBox["T", "10"]}], "+", 
    RowBox[{
     FractionBox[
      RowBox[{"-", "\[ImaginaryI]"}], "2"], 
     FractionBox[
      RowBox[{"-", "\[ImaginaryI]"}], "2"], 
     SubscriptBox["T", "11"]}]}]}], TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    FractionBox["1", "4"], 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["T", "00"], "-", 
      RowBox[{"2", "\[ImaginaryI]", " ", 
       SubscriptBox["T", "10"]}], "-", 
      SubscriptBox["T", "11"]}], ")"}]}]}], TraditionalForm]}], "Equation",
