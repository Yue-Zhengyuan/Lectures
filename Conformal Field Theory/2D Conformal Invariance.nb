Cell[CellGroupData[{

Cell["Holomorphic Components of the Metric", "Subsection",
 CellChangeTimes->{{3.788165817873597*^9, 
  3.7881658270210257`*^9}},ExpressionUUID->"e213d002-af84-4791-bfad-\
a790e4532cb5"],

Cell["The flat metric in the holomorphic coordinates becomes", "Text",
 CellChangeTimes->{{3.7881366108187113`*^9, 
  3.788136642282259*^9}},ExpressionUUID->"7c227d61-45f4-4c1f-8c2b-\
c2a59e1339b4"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["g", 
    RowBox[{"i", " ", "j"}]], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"(", GridBox[{
        {"1", "0"},
        {"0", "1"}
       }], ")"}], "\[Rule]", 
     SubscriptBox["g", 
      RowBox[{"a", " ", "b"}]]}], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "i"]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["z", "a"]}]], 
      FractionBox[
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "j"]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["z", "b"]}]], 
      SubscriptBox["g", 
       RowBox[{"i", " ", "j"}]]}], "=", 
     RowBox[{"(", GridBox[{
        {"0", 
         RowBox[{"1", "/", "2"}]},
        {
         RowBox[{"1", "/", "2"}], "0"}
       }], ")"}]}]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.7881366108187113`*^9, 3.788136662353517*^9}, {
  3.7881366927489557`*^9, 3.788136736255713*^9}, {3.788136937576014*^9, 
  3.78813694055658*^9}},ExpressionUUID->"7f4ad3ad-d8e8-4b5c-b95d-\
5fa601bace6a"],

Cell[TextData[{
 "Here ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["z", "0"], "=", "z"}], ",", " ", 
    RowBox[{
     SuperscriptBox["z", "1"], "=", 
     OverscriptBox["z", "_"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "872567ec-a508-4620-8c31-57c78581865a"],
 ". For example:"
}], "Text",
 CellChangeTimes->{{3.788136737818103*^9, 3.7881367525469723`*^9}, {
  3.788137047512936*^9, 
  3.7881370495306273`*^9}},ExpressionUUID->"1aaebbfa-78b0-44ab-b513-\
787d001f002c"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["g", 
    RowBox[{"z", " ", 
     OverscriptBox["z", "_"]}]], "=", 
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "i"]}], 
      RowBox[{"\[PartialD]", "z"}]], 
     FractionBox[
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "j"]}], 
      RowBox[{"\[PartialD]", 
       OverscriptBox["z", "_"]}]], 
     SubscriptBox["g", 
      RowBox[{"i", " ", "j"}]]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{"\[PartialD]", 
         SuperscriptBox["x", "0"]}], 
        RowBox[{"\[PartialD]", "z"}]], 
       FractionBox[
        RowBox[{"\[PartialD]", 
         SuperscriptBox["x", "0"]}], 
        RowBox[{"\[PartialD]", 
         OverscriptBox["z", "_"]}]]}], "+", 
      RowBox[{
       FractionBox[
        RowBox[{"\[PartialD]", 
         SuperscriptBox["x", "1"]}], 
        RowBox[{"\[PartialD]", "z"}]], 
       FractionBox[
        RowBox[{"\[PartialD]", 
         SuperscriptBox["x", "1"]}], 
        RowBox[{"\[PartialD]", 
         OverscriptBox["z", "_"]}]]}]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        FractionBox["1", "2"], "\[Cross]", 
        FractionBox["1", "2"]}], "+", 
       RowBox[{
        FractionBox[
         RowBox[{"-", "\[ImaginaryI]"}], "2"], "\[Cross]", 
        FractionBox["\[ImaginaryI]", "2"]}]}], "=", 
      FractionBox["1", "2"]}]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.788136737818103*^9, 3.7881367525469723`*^9}, {
  3.788137047512936*^9, 3.788137139970862*^9}, {3.788166076555016*^9, 
  3.788166078079875*^9}},ExpressionUUID->"8a4cb465-840a-4528-8e24-\
92d671fc8ca2"],

Cell["The inverse of the metric is then", "Text",
 CellChangeTimes->{{3.788137176981675*^9, 
  3.788137183107607*^9}},ExpressionUUID->"a2363795-87ee-4ab3-a177-\
95f958d98cfe"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox["g", 
    RowBox[{"a", " ", "b"}]], "=", 
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      SubscriptBox["g", 
       RowBox[{"a", " ", "b"}]], ")"}], 
     RowBox[{"-", "1"}]], "=", 
    RowBox[{"(", GridBox[{
       {"0", "2"},
       {"2", "0"}
      }], ")"}]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.788137176981675*^9, 
  3.7881372089154367`*^9}},ExpressionUUID->"ddfdb6e9-3798-4e88-8380-\
b933bd2c760d"]
}, Open  ]],

