Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[LeftAngleBracket]", 
    RowBox[{
     RowBox[{"T", "(", "z", ")"}], "X"}], "\[RightAngleBracket]"}], "=", 
   RowBox[{
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", "1"}], "n"], 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        FractionBox["1", 
         RowBox[{"z", "-", 
          SubscriptBox["w", "i"]}]], 
        RowBox[{
         SubscriptBox["\[PartialD]", 
          SubscriptBox["w", "i"]], 
         RowBox[{"\[LeftAngleBracket]", "X", "\[RightAngleBracket]"}]}]}], 
       "+", 
       RowBox[{
        FractionBox[
         SubscriptBox["h", "i"], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"z", "-", 
            SubscriptBox["w", "i"]}], ")"}], "2"]], 
        RowBox[{"\[LeftAngleBracket]", "X", "\[RightAngleBracket]"}]}]}], 
      "}"}]}], "+", 
    RowBox[{"reg", "."}]}]}], TraditionalForm]], "EquationNumbered",
