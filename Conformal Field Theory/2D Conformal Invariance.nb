       RowBox[{
        SubscriptBox["\[PartialD]", "\[Nu]"], 
        SubscriptBox["\[Epsilon]", "\[Mu]"]}]}], ")"}], 
     SuperscriptBox["T", 
      RowBox[{"\[Mu]", " ", "\[Nu]"}]]}]}]}], 
  TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[Epsilon]", "\[Nu]"], 
     RowBox[{
      SubscriptBox["\[PartialD]", "\[Mu]"], 
      SuperscriptBox["T", 
       RowBox[{"\[Mu]", " ", "\[Nu]"}]]}]}], "+", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["\[PartialD]", "\[Rho]"], 
       SuperscriptBox["\[Epsilon]", "\[Rho]"]}], ")"}], 
     SubscriptBox["\[Eta]", 
      RowBox[{"\[Mu]", " ", "\[Nu]"}]], 
     SuperscriptBox["T", 
      RowBox[{"\[Mu]", " ", "\[Nu]"}]]}], "+", 
    RowBox[{
     FractionBox["1", "2"], 
     SuperscriptBox["\[Epsilon]", 
