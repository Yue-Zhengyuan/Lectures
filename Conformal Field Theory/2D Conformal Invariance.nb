      RowBox[{"reg", ".", "\[LineSeparator]", 
       RowBox[{
        OverscriptBox["T", "_"], "(", 
        OverscriptBox["z", "_"], ")"}]}], 
      RowBox[{"\[Phi]", "(", 
       RowBox[{"w", ",", 
        OverscriptBox["w", "_"]}], ")"}]}]}], "=", 
