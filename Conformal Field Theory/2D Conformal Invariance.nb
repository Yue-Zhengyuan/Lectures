Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox["e", "a"], "=", 
   RowBox[{
    RowBox[{
     SubsuperscriptBox["e", "\[Mu]", "a"], "(", "x", ")"}], 
    RowBox[{"\[DifferentialD]", 
     SuperscriptBox["x", "\[Mu]"]}], "            ", 
