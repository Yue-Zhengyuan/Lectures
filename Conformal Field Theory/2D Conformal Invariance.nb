      RowBox[{"(", 
       RowBox[{
        FractionBox["\[PartialD]", 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "\[Rho]"]}]], 
        FractionBox[
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "\[Sigma]"]}], 
         RowBox[{"\[PartialD]", 
          OverscriptBox["z", "_"]}]]}], ")"}], 
      FractionBox[
       RowBox[{"\[PartialD]", "\[Phi]"}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "\[Sigma]"]}]]}], "+", 
     RowBox[{
      FractionBox[
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "\[Rho]"]}], 
       RowBox[{"\[PartialD]", "z"}]], 
      FractionBox[
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "\[Sigma]"]}], 
       RowBox[{"\[PartialD]", 
        OverscriptBox["z", "_"]}]], 
      RowBox[{"(", 
       RowBox[{
        FractionBox["\[PartialD]", 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "\[Rho]"]}]], 
        FractionBox[
         RowBox[{"\[PartialD]", "\[Phi]"}], 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "\[Sigma]"]}]]}], ")"}]}]}]}]}], 
  TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "\[Rho]"]}], 
      RowBox[{"\[PartialD]", "z"}]], 
     UnderscriptBox[
      UnderscriptBox[
       FractionBox[
        RowBox[{"\[PartialD]", 
         SubsuperscriptBox["\[Delta]", "\[Rho]", "\[Sigma]"]}], 
        RowBox[{"\[PartialD]", 
         OverscriptBox["z", "_"]}]], "\[UnderBrace]"], "0"], 
     FractionBox[
      RowBox[{"\[PartialD]", "\[Phi]"}], 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "\[Sigma]"]}]]}], "+"}]}], 
  TraditionalForm]}], "Equation",
