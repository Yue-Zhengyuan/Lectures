Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SuperscriptBox["x", "\[Mu]"], "\[Rule]", 
    SuperscriptBox["x", "\[Prime]\[Mu]"]}], "=", 
   RowBox[{
    SuperscriptBox["x", "\[Mu]"], "+", 
    RowBox[{
     SuperscriptBox["\[Epsilon]", "\[Mu]"], "(", "x", ")"}]}]}], 
  TraditionalForm]], "EquationNumbered",
