Cell[CellGroupData[{

Cell["\<\
Ward Identities for Translation, Rotation and Scale Invariance\
\>", "Subsubsection",
 CellChangeTimes->{{3.787892127208077*^9, 3.787892144173441*^9}, {
  3.7884323095704947`*^9, 
  3.788432311709117*^9}},ExpressionUUID->"c35ec350-55f5-49db-8645-\
be63ee6ca1fe"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[LeftAngleBracket]", 
    RowBox[{
     RowBox[{"T", "(", "z", ")"}], "X"}], "\[RightAngleBracket]"}], "=", 
   RowBox[{
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", "1"}], "n"], 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        FractionBox[
         SubscriptBox["h", "i"], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"z", "-", 
            SubscriptBox["w", "i"]}], ")"}], "2"]], 
        RowBox[{"\[LeftAngleBracket]", "X", "\[RightAngleBracket]"}]}], "+", 
       RowBox[{
        FractionBox["1", 
         RowBox[{"z", "-", 
          SubscriptBox["w", "i"]}]], 
        RowBox[{
         SubscriptBox["\[PartialD]", 
          SubscriptBox["w", "i"]], 
         RowBox[{"\[LeftAngleBracket]", "X", "\[RightAngleBracket]"}]}]}]}], 
      "}"}]}], "+", 
    RowBox[{"reg", "."}]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.787891051225946*^9, 3.787891155940534*^9}, {
  3.788395878646121*^9, 
  3.788395883087884*^9}},ExpressionUUID->"97d598a2-b525-476b-b503-\
1855e9363638"],

Cell["A similar result hold for the anti-holomorphic part.", "Text",
 CellChangeTimes->{{3.787892315444813*^9, 
  3.787892322654293*^9}},ExpressionUUID->"34f73d21-373f-4b12-bb54-\
6f06e882af9a"]
}, Open  ]],

Cell[CellGroupData[{

Cell["Unified Ward Identity", "Subsubsection",
 CellChangeTimes->{{3.78789176901689*^9, 3.7878917801807632`*^9}, {
  3.787892983786755*^9, 3.787892987385859*^9}, {3.788432290977531*^9, 
  3.788432292222806*^9}},ExpressionUUID->"c1c3c3ed-b3bb-45ba-a100-\
0fc565079fee"],

Cell[TextData[{
 "The three Ward Identity above can be combined into a single equation. Given \
an arbitrary conformal coordinates change ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["x", "\[Nu]"], "\[Rule]", 
    RowBox[{
     SuperscriptBox["x", "\[Nu]"], "+", 
     RowBox[{
      SuperscriptBox["\[Epsilon]", "\[Nu]"], "(", "x", ")"}]}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "abcea6ce-5b99-40be-b098-9f3e38898aa4"],
 ", we get"
}], "Text",
 CellChangeTimes->{{3.78789305996649*^9, 3.787893081013522*^9}, {
  3.7880810862764606`*^9, 
  3.788081131864493*^9}},ExpressionUUID->"f6b2eb3f-d7c6-41ec-a986-\
cf95cfaa51b6"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[PartialD]", "\[Mu]"], 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["\[Epsilon]", "\[Nu]"], 
      SuperscriptBox["T", 
       RowBox[{"\[Mu]", " ", "\[Nu]"}]]}], ")"}]}], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[Epsilon]", "\[Nu]"], 
     RowBox[{
      SubscriptBox["\[PartialD]", "\[Mu]"], 
      SuperscriptBox["T", 
       RowBox[{"\[Mu]", " ", "\[Nu]"}]]}]}], "+", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["\[PartialD]", "\[Mu]"], 
       SubscriptBox["\[Epsilon]", "\[Nu]"]}], ")"}], 
     SuperscriptBox["T", 
      RowBox[{"\[Mu]", " ", "\[Nu]"}]]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.78789305996649*^9, 3.787893081013522*^9}, {
  3.7880810862764606`*^9, 3.788081148743373*^9}, {3.788125769580511*^9, 
  3.788125805768447*^9}},ExpressionUUID->"252da8f0-33bb-4b9a-acbb-\
315eee63f424"],

Cell["To exploit the properties of conformal transformations", "Text",
 CellChangeTimes->{{3.7881258127136927`*^9, 3.788125846197709*^9}, {
  3.7881630551967*^9, 3.7881630630510693`*^9}, {3.7881631220060997`*^9, 
  3.7881631401485157`*^9}},ExpressionUUID->"20b2f7ad-9d45-41ed-9e41-\
d02111a3b7bb"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    FractionBox["1", "2"], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[PartialD]", "\[Mu]"], 
       SubscriptBox["\[Epsilon]", "\[Nu]"]}], "+", 
      RowBox[{
       SubscriptBox["\[PartialD]", "\[Nu]"], 
       SubscriptBox["\[Epsilon]", "\[Mu]"]}]}], ")"}]}], "=", 
   RowBox[{
    FractionBox["1", "2"], 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["\[PartialD]", "\[Rho]"], 
      SuperscriptBox["\[Epsilon]", "\[Rho]"]}], ")"}], 
    SubscriptBox["\[Eta]", 
     RowBox[{"\[Mu]", " ", "\[Nu]"}]]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.7881631484214163`*^9, 
  3.788163199510049*^9}},ExpressionUUID->"d655dcb1-11b0-4bba-8551-\
c27e2b401043"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    FractionBox["1", "2"], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[PartialD]", "\[Mu]"], 
       SubscriptBox["\[Epsilon]", "\[Nu]"]}], "-", 
      RowBox[{
       SubscriptBox["\[PartialD]", "\[Nu]"], 
       SubscriptBox["\[Epsilon]", "\[Mu]"]}]}], ")"}]}], "=", 
   RowBox[{
    FractionBox["1", "2"], 
    SuperscriptBox["\[Epsilon]", 
     RowBox[{"\[Alpha]", " ", "\[Beta]"}]], 
    RowBox[{
     SubscriptBox["\[PartialD]", "\[Alpha]"], 
     SubscriptBox["\[Epsilon]", "\[Beta]"]}], 
    SubscriptBox["\[Epsilon]", 
     RowBox[{"\[Mu]", " ", "\[Nu]"}]]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.7881631484214163`*^9, 
  3.7881632338878098`*^9}},ExpressionUUID->"5c4ea699-d8c0-49b9-a593-\
1c4c3db9d616"],

Cell[TextData[{
 "(here ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["1", "2"], 
    RowBox[{
     SubscriptBox["\[PartialD]", "\[Rho]"], 
     SuperscriptBox["\[Epsilon]", "\[Rho]"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "09a06a0b-3112-49aa-8971-9501d84c5167"],
 " is the local scale factor ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "(", "x", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "b09ec38d-3862-4289-89b8-8372de0c7ce9"],
 ", and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["1", "2"], 
    SuperscriptBox["\[Epsilon]", 
     RowBox[{"\[Alpha]", " ", "\[Beta]"}]], 
    RowBox[{
     SubscriptBox["\[PartialD]", "\[Alpha]"], 
     SubscriptBox["\[Epsilon]", "\[Beta]"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "58df3ce2-9629-4e51-9f51-0d1de17a6e23"],
 " is the local rotation angle) we rewrite the second term "
}], "Text",
 CellChangeTimes->{{3.7881258127136927`*^9, 3.788125846197709*^9}, {
  3.7881630551967*^9, 3.7881630630510693`*^9}, {3.7881631220060997`*^9, 
  3.788163143420411*^9}, {3.788163273574856*^9, 
  3.78816333509312*^9}},ExpressionUUID->"8a6b426f-1941-41d3-8add-\
053af057dd49"],

Cell[BoxData[{
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[PartialD]", "\[Mu]"], 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["\[Epsilon]", "\[Nu]"], 
      SuperscriptBox["T", 
       RowBox[{"\[Mu]", " ", "\[Nu]"}]]}], ")"}]}], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[Epsilon]", "\[Nu]"], 
     RowBox[{
