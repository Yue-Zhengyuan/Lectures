  3.788404463652372*^9, 
  3.7884044704512243`*^9}},ExpressionUUID->"0809fa6d-b2a5-40bd-9bfd-\
a76f30b7e7e2"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{
     SubscriptBox["\[PartialD]", "0"], 
     SubscriptBox["\[Epsilon]", "0"]}], "=", 
    RowBox[{
     SubscriptBox["\[PartialD]", "1"], 
     SubscriptBox["\[Epsilon]", "1"]}]}], ",", " ", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[PartialD]", "0"], 
     SubscriptBox["\[Epsilon]", "1"]}], "=", 
