Cell[CellGroupData[{

Cell["Math Appendix: The Tetrad", "Subsubsection",
 CellChangeTimes->{{3.7878938397017593`*^9, 3.787893858520331*^9}, 
   3.7879573740293903`*^9},ExpressionUUID->"30692653-2483-492e-a6ad-\
ebc4d8194018"],

Cell[TextData[{
 "On a Riemannian manifold, since it is \[OpenCurlyDoubleQuote]locally flat\
\[CloseCurlyDoubleQuote] at every point, we can introduce at each point a ",
 StyleBox["local orthogonal frame of basis vectors",
  FontSlant->"Italic"],
 " of the cotangent space:"
}], "Text",
 CellChangeTimes->{{3.787957408311833*^9, 
  3.7879574717281513`*^9}},ExpressionUUID->"6782b8f5-1f24-469a-bc6d-\
42cfcad67eea"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox["e", "a"], "=", 
   RowBox[{
    RowBox[{
     SubsuperscriptBox["e", "\[Mu]", "a"], "(", "x", ")"}], 
    RowBox[{"\[DifferentialD]", 
     SuperscriptBox["x", "\[Mu]"]}], "            ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"a", "=", "1"}], ",", "...", ",", "d"}], ")"}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.787957408311833*^9, 3.787957493933558*^9}, {
  3.787959516324501*^9, 
  3.787959516685717*^9}},ExpressionUUID->"7e4fde0e-def9-4fbe-9a66-\
e3d1f252e10e"],

Cell[TextData[{
 "Here ",
 Cell[BoxData[
  FormBox["d", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "720e3252-c18b-4534-a71e-3fe8a99702b2"],
 " is the dimension of the manifold, and the basis vectors ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["e", "a"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "e53a1eaa-6455-41f9-b805-259fe8934a92"],
 " form a ",
 StyleBox["tetrad",
  FontWeight->"Bold"],
 ". A natural choice is"
}], "Text",
 CellChangeTimes->{{3.787957498222228*^9, 
  3.787957549926627*^9}},ExpressionUUID->"93a47a96-0a03-448f-b7c6-\
308f0d73f05d"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{
     SubsuperscriptBox["e", "\[Mu]", "a"], 
     SubsuperscriptBox["e", "\[Nu]", "b"], 
     SuperscriptBox["g", 
      RowBox[{"\[Mu]", " ", "\[Nu]"}]]}], "=", 
    SuperscriptBox["\[Eta]", 
     RowBox[{"a", " ", "b"}]]}], ",", " ", 
   RowBox[{
    SubscriptBox["g", 
     RowBox[{"\[Mu]", " ", "\[Nu]"}]], "=", 
    RowBox[{
     SubscriptBox["\[Eta]", 
      RowBox[{"a", " ", "b"}]], 
     SubsuperscriptBox["e", "\[Mu]", "a"], 
     SubsuperscriptBox["e", "\[Nu]", "b"]}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.787957498222228*^9, 
  3.787957598500272*^9}},ExpressionUUID->"474c924e-e5d2-465f-9add-\
cdc1f53de836"],

Cell[TextData[{
 "The lower (Greek) index of ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["e", "\[Mu]", "a"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "bc46fea0-4f30-4621-b0d5-e5f5a475d91a"],
 " is called an ",
 StyleBox["Einstein index",
  FontWeight->"Bold"],
 ", and the upper (Latin) index is called a ",
 StyleBox["Lorentz index",
  FontWeight->"Bold"],
 StyleBox[". ",
  FontSlant->"Italic"],
 "They are raised or lowered by the metric tensor ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["g", 
    RowBox[{"\[Mu]", " ", "\[Nu]"}]], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "79e0efa8-3402-46b0-884a-e4e4ab8f2ac9"],
 " and the Minkowski tensor ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Eta]", 
    RowBox[{"a", " ", "b"}]], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "9ae42c05-6445-4eca-b6e4-d051f0089582"],
 ", respectively. "
}], "Text",
 CellChangeTimes->{{3.787957628792741*^9, 3.7879576715352583`*^9}, {
  3.787957706019994*^9, 
  3.787957758714929*^9}},ExpressionUUID->"62e86995-a2a8-4cac-8194-\
442fcf9bbdd1"],

Cell[TextData[{
 "Recall that under a change of coordinates ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "\[Rule]", 
    RowBox[{
     SuperscriptBox["x", "\[Prime]"], "(", "x", ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "aaa7b944-5f00-4f48-b771-c6fe44cea3ea"],
 ", the metric transforms according to"
}], "Text",
 CellChangeTimes->{{3.787957358459961*^9, 3.78795738136971*^9}, {
  3.7879577869492826`*^9, 3.787957794770503*^9}, {3.787957862643564*^9, 
  3.787957889553303*^9}},ExpressionUUID->"61b20cb5-9de5-4c71-88a4-\
ba68ac158a0f"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{
     SubscriptBox["g", 
      RowBox[{"\[Mu]", " ", "\[Nu]"}]], "(", "x", ")"}], "\[Rule]", 
    RowBox[{
     SubsuperscriptBox["g", 
      RowBox[{"\[Mu]", " ", "\[Nu]"}], "\[Prime]"], "(", 
     SuperscriptBox["x", "\[Prime]"], ")"}]}], "=", 
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", 
      SuperscriptBox["x", "\[Rho]"]}], 
     RowBox[{"\[PartialD]", 
      SuperscriptBox["x", "\[Prime]\[Mu]"]}]], 
    FractionBox[
     RowBox[{"\[PartialD]", 
      SuperscriptBox["x", "\[Sigma]"]}], 
     RowBox[{"\[PartialD]", 
      SuperscriptBox["x", "\[Prime]\[Nu]"]}]], 
    RowBox[{
     SubscriptBox["g", 
      RowBox[{"\[Rho]", " ", "\[Sigma]"}]], "(", "x", ")"}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.787957358459961*^9, 3.78795738136971*^9}, {
  3.7879577869492826`*^9, 3.787957794770503*^9}, {3.787957862643564*^9, 
  3.787957903190372*^9}, {3.787958086610251*^9, 3.787958154998953*^9}, {
  3.787959522154594*^9, 
  3.787959529178803*^9}},ExpressionUUID->"a84b42b1-7668-41f6-b5ac-\
90d41a17828e"],

Cell[TextData[{
 "but ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Eta]", 
    RowBox[{"a", " ", "b"}]], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "6fb9155c-0bdd-4cf4-b2e1-ad11b7579955"],
 " does ",
 StyleBox["not",
  FontSlant->"Italic"],
 " change. Thus, using ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["g", 
     RowBox[{"\[Mu]", " ", "\[Nu]"}]], "=", 
    RowBox[{
     SubscriptBox["\[Eta]", 
      RowBox[{"a", " ", "b"}]], 
     SubsuperscriptBox["e", "\[Mu]", "a"], 
     SubsuperscriptBox["e", "\[Nu]", "b"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "e0f32f80-2ddf-41db-bd56-2524454b1fbd"],
 ", we find that"
}], "Text",
 CellChangeTimes->{{3.787957358459961*^9, 3.78795738136971*^9}, {
  3.7879577869492826`*^9, 3.787957794770503*^9}, {3.787957862643564*^9, 
  3.7879579399639807`*^9}, {3.787958169734125*^9, 
  3.787958192823023*^9}},ExpressionUUID->"aeef78dd-85df-4ac9-8865-\
2322a52f0287"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{
     SubsuperscriptBox["e", "\[Mu]", "a"], "(", "x", ")"}], "\[Rule]", 
    RowBox[{
     SubsuperscriptBox["e", "\[Mu]", 
      RowBox[{"\[Prime]", " ", "a"}]], "(", 
     SuperscriptBox["x", "\[Prime]"], ")"}]}], "=", 
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", 
      SuperscriptBox["x", "\[Nu]"]}], 
     RowBox[{"\[PartialD]", 
      SuperscriptBox["x", "\[Prime]\[Mu]"]}]], 
    RowBox[{
     SubsuperscriptBox["e", "\[Nu]", "a"], "(", "x", ")"}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.787957799030764*^9, 3.787957840043676*^9}, {
  3.7879595311977377`*^9, 
  3.787959538643714*^9}},ExpressionUUID->"cdc55e65-d056-4f80-b6b7-\
6a89944d4fa0"]
}, Open  ]],

