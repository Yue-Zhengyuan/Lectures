fca95156131b"],

Cell[BoxData[{
 FormBox[
  RowBox[{
   RowBox[{
    SubsuperscriptBox["g", 
     RowBox[{"\[Mu]", " ", "\[Nu]"}], "\[Prime]"], "(", 
    SuperscriptBox["x", "\[Prime]"], ")"}], "\[TildeEqual]", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      SubsuperscriptBox["\[Delta]", "\[Mu]", "\[Rho]"], "-", 
      FractionBox[
       RowBox[{"\[PartialD]", 
        RowBox[{
         SuperscriptBox["\[Epsilon]", "\[Rho]"], "(", "x", ")"}]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "\[Mu]"]}]]}], ")"}], 
    RowBox[{"(", 
     RowBox[{
      SubsuperscriptBox["\[Delta]", "\[Nu]", "\[Sigma]"], "-", 
      FractionBox[
       RowBox[{"\[PartialD]", 
        RowBox[{
         SuperscriptBox["\[Epsilon]", "\[Sigma]"], "(", "x", ")"}]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "\[Nu]"]}]]}], ")"}], 
    RowBox[{
     SubscriptBox["g", 
      RowBox[{"\[Rho]", " ", "\[Sigma]"}]], "(", "x", ")"}]}]}], 
  TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{
   RowBox[{"\[TildeEqual]", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SubsuperscriptBox["\[Delta]", "\[Mu]", "\[Rho]"], 
        SubsuperscriptBox["\[Delta]", "\[Nu]", "\[Sigma]"]}], "-", 
       RowBox[{
        SubsuperscriptBox["\[Delta]", "\[Mu]", "\[Rho]"], 
        RowBox[{
         SubscriptBox["\[PartialD]", "\[Nu]"], 
         RowBox[{
          SuperscriptBox["\[Epsilon]", "\[Sigma]"], "(", "x", ")"}]}]}], "-", 
       
       RowBox[{
        SubsuperscriptBox["\[Delta]", "\[Nu]", "\[Sigma]"], 
        RowBox[{
         SubscriptBox["\[PartialD]", "\[Mu]"], 
