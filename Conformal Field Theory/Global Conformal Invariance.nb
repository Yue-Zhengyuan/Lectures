  FormatType->"TraditionalForm",ExpressionUUID->
  "e131c268-5c76-47e7-9ea6-504b870728ce"],
 ", we have"
}], "Text",
 CellChangeTimes->{{3.788406495285844*^9, 
  3.788406525921111*^9}},ExpressionUUID->"1b77f63d-3aa1-497d-a9b6-\
cb357dbb43aa"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"(", 
     RowBox[{"m", "+", "n"}], ")"}], 
    RowBox[{"p", "(", 
     RowBox[{"n", ",", "m"}], ")"}]}], "=", 
   RowBox[{
    RowBox[{"0", " ", "\[DoubleLongRightArrow]", 
     RowBox[{"p", "(", 
      RowBox[{"n", ",", "m"}], ")"}]}], "=", 
    RowBox[{
     RowBox[{"p", "(", 
      RowBox[{"n", ",", 
       RowBox[{"-", "n"}]}], ")"}], 
