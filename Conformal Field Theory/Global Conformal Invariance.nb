  3.7881374884834843`*^9}},ExpressionUUID->"bb222d4a-8f7b-41cf-b961-\
f125f670c4dc"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"\[DifferentialD]", "z"}], 
    RowBox[{"\[DifferentialD]", 
     OverscriptBox["z", "_"]}]}], "=", 
   RowBox[{
    RowBox[{"-", "2"}], "\[ImaginaryI]", 
    RowBox[{"\[DifferentialD]", 
     SuperscriptBox["x", "0"]}], 
    RowBox[{"\[DifferentialD]", 
     SuperscriptBox["x", "1"]}]}]}], TraditionalForm]], "Equation",
