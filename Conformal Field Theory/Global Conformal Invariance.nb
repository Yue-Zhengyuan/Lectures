Cell["Summarize all the above calculations, we finally find", "Text",
 CellChangeTimes->{{3.788407075679644*^9, 
  3.788407094383637*^9}},ExpressionUUID->"45d02341-944b-4cbc-9926-\
a4f8d99ea01a"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"p", "(", 
    RowBox[{"n", ",", "m"}], ")"}], "=", 
   RowBox[{
    FractionBox["1", "12"], 
    RowBox[{"n", "(", 
     RowBox[{
      SuperscriptBox["n", "2"], "-", "1"}], ")"}], 
    SubscriptBox["\[Delta]", 
     RowBox[{
      RowBox[{"m", "+", "n"}], ",", " ", "0"}]]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.788407097041561*^9, 
  3.788407119584996*^9}},ExpressionUUID->"397d9324-a573-4ade-bcfe-\
05cb58a13d37"],

Cell["and the Virasoro algebra is", "Text",
 CellChangeTimes->{{3.788407123685515*^9, 
  3.78840712985608*^9}},ExpressionUUID->"ee559eb3-8066-40de-83fc-\
e446fb5ffb20"],

Cell[BoxData[
