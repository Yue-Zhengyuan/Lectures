  3.788137439876871*^9, 
  3.788137476989951*^9}},ExpressionUUID->"bdafb7f4-5ab1-4f93-a4e6-\
e00f9cafca28"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SuperscriptBox["x", "0"], "=", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{"z", "+", 
       OverscriptBox["z", "_"]}], ")"}]}]}], ",", " ", 
   RowBox[{
    SuperscriptBox["x", "1"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"-", "\[ImaginaryI]"}], "2"], 
     RowBox[{"(", 
      RowBox[{"z", "-", 
       OverscriptBox["z", "_"]}], ")"}]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.78813646133057*^9, 3.788136491573852*^9}, {
  3.788136543739168*^9, 
  3.7881365458373213`*^9}},ExpressionUUID->"d6e18f29-5de8-456d-92ed-\
3d5543e86217"],

Cell["We also define the derivatives", "Text",
 CellChangeTimes->{{3.788136114391961*^9, 
  3.788136125851603*^9}},ExpressionUUID->"e5cd27e8-db72-45dc-9dc4-\
0f5101d18172"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"\[PartialD]", 
     RowBox[{"\[Congruent]", 
      SubscriptBox["\[PartialD]", "z"]}]}], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "\[Rho]"]}], 
       RowBox[{"\[PartialD]", "z"}]], 
      FractionBox["\[PartialD]", 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "\[Rho]"]}]]}], "=", 
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         SubscriptBox["\[PartialD]", "0"], 
         RowBox[{"-", "\[ImaginaryI]"}]}], 
        SubscriptBox["\[PartialD]", "1"]}], ")"}]}]}]}], ",", " "}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.788136114391961*^9, 3.788136136790897*^9}, {
  3.788136404362179*^9, 3.788136455540786*^9}, {3.788136558164048*^9, 
  3.788136569871952*^9}, {3.788137546000228*^9, 3.7881375469369583`*^9}, {
  3.788167869849666*^9, 3.788167874784718*^9}, {3.788168304278884*^9, 
  3.7881683092233057`*^9}},ExpressionUUID->"48a89b2d-6fad-44c2-a78a-\
5f68904e14f2"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    OverscriptBox["\[PartialD]", "_"], 
    RowBox[{"\[Congruent]", 
     SubscriptBox["\[PartialD]", 
      OverscriptBox["z", "_"]]}]}], "=", 
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "\[Rho]"]}], 
      RowBox[{"\[PartialD]", 
       OverscriptBox["z", "_"]}]], 
     FractionBox["\[PartialD]", 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "\[Rho]"]}]]}], "=", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SubscriptBox["\[PartialD]", "0"], 
        RowBox[{"+", "\[ImaginaryI]"}]}], 
       SubscriptBox["\[PartialD]", "1"]}], ")"}]}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.788136114391961*^9, 3.788136136790897*^9}, {
  3.788136404362179*^9, 3.788136455540786*^9}, {3.788136558164048*^9, 
  3.788136569871952*^9}, {3.788137546000228*^9, 3.7881375469369583`*^9}, {
  3.788167869849666*^9, 3.788167880385087*^9}, {3.78816831084976*^9, 
  3.7881683155163403`*^9}},ExpressionUUID->"49e8d88c-f355-4bc5-b278-\
a2da947659ba"],

Cell[CellGroupData[{

Cell["Jacobian", "Subsubsection",
 CellChangeTimes->{{3.788402769326413*^9, 
  3.78840277052885*^9}},ExpressionUUID->"8bf6cbfe-f87e-4b6a-a255-\
bb4d5b8c4f23"],

Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    RowBox[{"D", "(", 
     RowBox[{"z", ",", 
      OverscriptBox["z", "_"]}], ")"}], 
    RowBox[{"D", "(", 
     RowBox[{
      SuperscriptBox["x", "0"], ",", 
      SuperscriptBox["x", "1"]}], ")"}]], "=", 
   RowBox[{
    RowBox[{"det", "(", GridBox[{
       {
        RowBox[{
         RowBox[{"\[PartialD]", "z"}], "/", 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "0"]}]}], 
        RowBox[{
         RowBox[{"\[PartialD]", "z"}], "/", 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "1"]}]}]},
       {
        RowBox[{
         RowBox[{"\[PartialD]", 
          OverscriptBox["z", "_"]}], "/", 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "0"]}]}], 
        RowBox[{
         RowBox[{"\[PartialD]", 
          OverscriptBox["z", "_"]}], "/", 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "1"]}]}]}
      }], ")"}], "=", 
    RowBox[{
     RowBox[{"det", "(", GridBox[{
        {"1", "\[ImaginaryI]"},
        {"1", 
         RowBox[{"-", "\[ImaginaryI]"}]}
       }], ")"}], "=", 
     RowBox[{
      RowBox[{"-", "2"}], "\[ImaginaryI]"}]}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.788136141657522*^9, 3.788136146807063*^9}, {
  3.7881362085946712`*^9, 
  3.788136335675499*^9}},ExpressionUUID->"61ac94f7-994d-45f5-b96d-\
661ef3e185b0"],

Cell["Therefore", "Text",
