  3.788406155838512*^9, 
  3.7884061662035007`*^9}},ExpressionUUID->"0ff4c1d2-217c-4295-94b1-\
d49603a95447"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubsuperscriptBox["L", "n", "\[Prime]"], "=", 
    RowBox[{
     SubscriptBox["L", "n"], "+", 
     RowBox[{
      FractionBox[
       RowBox[{"c", " ", 
        RowBox[{"p", "(", 
         RowBox[{"n", ",", "0"}], ")"}]}], "n"], " ", 
      RowBox[{"(", 
       RowBox[{"n", "\[NotEqual]", "0"}], ")"}]}]}]}], ",", " ", 
   RowBox[{
    SubsuperscriptBox["L", "0", "\[Prime]"], "=", 
    RowBox[{
     SubscriptBox["L", "0"], "+", 
     FractionBox[
      RowBox[{"c", " ", 
       RowBox[{"p", "(", 
        RowBox[{"1", ",", 
         RowBox[{"-", "1"}]}], ")"}]}], "2"]}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.788405856474*^9, 
  3.788405911486844*^9}},ExpressionUUID->"49ec4849-aa55-450d-96a4-\
b503ee357e0b"],

Cell["Then", "Text",
 CellChangeTimes->{{3.788405913731495*^9, 
  3.78840591540987*^9}},ExpressionUUID->"c55691e2-5747-4fa5-ac75-\
156b8fb1ac3e"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"[", 
    RowBox[{
     SubsuperscriptBox["L", "n", "\[Prime]"], ",", 
     SubsuperscriptBox["L", "0", "\[Prime]"]}], "]"}], "=", 
   RowBox[{
    RowBox[{"[", 
     RowBox[{
      SubscriptBox["L", "n"], ",", 
      SubscriptBox["L", "0"]}], "]"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"n", " ", 
       SubscriptBox["L", "n"]}], "+", 
      RowBox[{"c", " ", 
       RowBox[{"p", "(", 
        RowBox[{"n", ",", "0"}], ")"}]}]}], "=", 
     RowBox[{"n", " ", 
      SubsuperscriptBox["L", "n", "\[Prime]"]}]}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.788405913731495*^9, 3.7884059845705967`*^9}, {
  3.788406040002349*^9, 
  3.788406114249093*^9}},ExpressionUUID->"ffdab3ae-0946-4e7c-8b6c-\
01062dbcba8a"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"[", 
    RowBox[{
     SubsuperscriptBox["L", "1", "\[Prime]"], ",", 
     SubsuperscriptBox["L", 
      RowBox[{"-", "1"}], "\[Prime]"]}], "]"}], "=", 
   RowBox[{
    RowBox[{"[", 
     RowBox[{
      SubscriptBox["L", "1"], ",", 
