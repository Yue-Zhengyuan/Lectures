          SuperscriptBox["\[Epsilon]", "\[Rho]"], "(", "x", ")"}]}]}]}], 
      ")"}], 
     RowBox[{
      SubscriptBox["g", 
       RowBox[{"\[Rho]", " ", "\[Sigma]"}]], "(", "x", ")"}]}]}], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["g", 
      RowBox[{"\[Mu]", " ", "\[Nu]"}]], "(", "x", ")"}], "-", 
    RowBox[{
     RowBox[{
      SubscriptBox["g", 
       RowBox[{"\[Mu]", " ", "\[Sigma]"}]], "(", "x", ")"}], 
     RowBox[{
      SubscriptBox["\[PartialD]", "\[Nu]"], 
      RowBox[{
       SuperscriptBox["\[Epsilon]", "\[Sigma]"], "(", "x", ")"}]}]}], "-", 
    RowBox[{
     RowBox[{
      SubscriptBox["g", 
       RowBox[{"\[Rho]", " ", "\[Nu]"}]], "(", "x", ")"}], 
     RowBox[{
      SubscriptBox["\[PartialD]", "\[Mu]"], 
      RowBox[{
       SuperscriptBox["\[Epsilon]", "\[Rho]"], "(", "x", ")"}]}]}]}]}], 
  TraditionalForm]}], "Equation",
 CellChangeTimes->{{3.788255013031352*^9, 3.788255071741016*^9}, {
