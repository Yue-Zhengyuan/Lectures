Cell[BoxData[{
 FormBox[
  RowBox[{
   RowBox[{"\[PartialD]", 
    RowBox[{
     OverscriptBox["\[PartialD]", "_"], "\[Phi]"}]}], "=", 
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "\[Rho]"]}], 
      RowBox[{"\[PartialD]", "z"}]], 
     RowBox[{
      FractionBox["\[PartialD]", 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "\[Rho]"]}]], 
      RowBox[{"(", 
       RowBox[{
        FractionBox[
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "\[Sigma]"]}], 
         RowBox[{"\[PartialD]", 
          OverscriptBox["z", "_"]}]], 
        FractionBox[
         RowBox[{"\[PartialD]", "\[Phi]"}], 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "\[Sigma]"]}]]}], ")"}]}]}], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "\[Rho]"]}], 
       RowBox[{"\[PartialD]", "z"}]], 
      RowBox[{"(", 
       RowBox[{
        FractionBox["\[PartialD]", 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "\[Rho]"]}]], 
        FractionBox[
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "\[Sigma]"]}], 
         RowBox[{"\[PartialD]", 
          OverscriptBox["z", "_"]}]]}], ")"}], 
      FractionBox[
       RowBox[{"\[PartialD]", "\[Phi]"}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "\[Sigma]"]}]]}], "+", 
     RowBox[{
      FractionBox[
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "\[Rho]"]}], 
       RowBox[{"\[PartialD]", "z"}]], 
      FractionBox[
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "\[Sigma]"]}], 
       RowBox[{"\[PartialD]", 
        OverscriptBox["z", "_"]}]], 
      RowBox[{"(", 
       RowBox[{
        FractionBox["\[PartialD]", 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "\[Rho]"]}]], 
        FractionBox[
         RowBox[{"\[PartialD]", "\[Phi]"}], 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "\[Sigma]"]}]]}], ")"}]}]}]}]}], 
  TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "\[Rho]"]}], 
      RowBox[{"\[PartialD]", "z"}]], 
     UnderscriptBox[
      UnderscriptBox[
       FractionBox[
        RowBox[{"\[PartialD]", 
         SubsuperscriptBox["\[Delta]", "\[Rho]", "\[Sigma]"]}], 
        RowBox[{"\[PartialD]", 
         OverscriptBox["z", "_"]}]], "\[UnderBrace]"], "0"], 
     FractionBox[
      RowBox[{"\[PartialD]", "\[Phi]"}], 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "\[Sigma]"]}]]}], "+"}]}], 
  TraditionalForm]}], "Equation",
 CellChangeTimes->{{3.788168163695612*^9, 3.788168273911133*^9}, {
  3.788168322072504*^9, 
  3.788168438113513*^9}},ExpressionUUID->"c3f38518-068f-4bd4-abbe-\
e3df9e3ef645"]
}, Open  ]],

Cell[CellGroupData[{

Cell["The Metric Tensor", "Subsubsection",
 CellChangeTimes->{{3.788165817873597*^9, 3.7881658270210257`*^9}, {
  3.7884027814365683`*^9, 
  3.788402785996195*^9}},ExpressionUUID->"e213d002-af84-4791-bfad-\
a790e4532cb5"],

Cell["The flat metric in the holomorphic coordinates becomes", "Text",
 CellChangeTimes->{{3.7881366108187113`*^9, 
  3.788136642282259*^9}},ExpressionUUID->"7c227d61-45f4-4c1f-8c2b-\
c2a59e1339b4"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["g", 
    RowBox[{"i", " ", "j"}]], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"(", GridBox[{
        {"1", "0"},
        {"0", "1"}
       }], ")"}], "\[Rule]", 
     SubscriptBox["g", 
      RowBox[{"a", " ", "b"}]]}], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "i"]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["z", "a"]}]], 
      FractionBox[
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "j"]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["z", "b"]}]], 
      SubscriptBox["g", 
       RowBox[{"i", " ", "j"}]]}], "=", 
     RowBox[{"(", GridBox[{
        {"0", 
         RowBox[{"1", "/", "2"}]},
        {
         RowBox[{"1", "/", "2"}], "0"}
       }], ")"}]}]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.7881366108187113`*^9, 3.788136662353517*^9}, {
  3.7881366927489557`*^9, 3.788136736255713*^9}, {3.788136937576014*^9, 
  3.78813694055658*^9}},ExpressionUUID->"7f4ad3ad-d8e8-4b5c-b95d-\
5fa601bace6a"],

Cell[TextData[{
 "Here ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["z", "0"], "=", "z"}], ",", " ", 
    RowBox[{
     SuperscriptBox["z", "1"], "=", 
     OverscriptBox["z", "_"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "872567ec-a508-4620-8c31-57c78581865a"],
 ". For example:"
}], "Text",
 CellChangeTimes->{{3.788136737818103*^9, 3.7881367525469723`*^9}, {
  3.788137047512936*^9, 
  3.7881370495306273`*^9}},ExpressionUUID->"1aaebbfa-78b0-44ab-b513-\
787d001f002c"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["g", 
    RowBox[{"z", " ", 
     OverscriptBox["z", "_"]}]], "=", 
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "i"]}], 
      RowBox[{"\[PartialD]", "z"}]], 
     FractionBox[
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "j"]}], 
      RowBox[{"\[PartialD]", 
       OverscriptBox["z", "_"]}]], 
     SubscriptBox["g", 
      RowBox[{"i", " ", "j"}]]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{"\[PartialD]", 
         SuperscriptBox["x", "0"]}], 
        RowBox[{"\[PartialD]", "z"}]], 
       FractionBox[
        RowBox[{"\[PartialD]", 
         SuperscriptBox["x", "0"]}], 
        RowBox[{"\[PartialD]", 
         OverscriptBox["z", "_"]}]]}], "+", 
      RowBox[{
       FractionBox[
        RowBox[{"\[PartialD]", 
         SuperscriptBox["x", "1"]}], 
        RowBox[{"\[PartialD]", "z"}]], 
       FractionBox[
        RowBox[{"\[PartialD]", 
         SuperscriptBox["x", "1"]}], 
        RowBox[{"\[PartialD]", 
         OverscriptBox["z", "_"]}]]}]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        FractionBox["1", "2"], "\[Cross]", 
        FractionBox["1", "2"]}], "+", 
       RowBox[{
        FractionBox[
         RowBox[{"-", "\[ImaginaryI]"}], "2"], "\[Cross]", 
        FractionBox["\[ImaginaryI]", "2"]}]}], "=", 
      FractionBox["1", "2"]}]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.788136737818103*^9, 3.7881367525469723`*^9}, {
  3.788137047512936*^9, 3.788137139970862*^9}, {3.788166076555016*^9, 
  3.788166078079875*^9}},ExpressionUUID->"8a4cb465-840a-4528-8e24-\
92d671fc8ca2"],

Cell["The inverse of the metric is then", "Text",
 CellChangeTimes->{{3.788137176981675*^9, 
  3.788137183107607*^9}},ExpressionUUID->"a2363795-87ee-4ab3-a177-\
95f958d98cfe"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox["g", 
    RowBox[{"a", " ", "b"}]], "=", 
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      SubscriptBox["g", 
       RowBox[{"a", " ", "b"}]], ")"}], 
     RowBox[{"-", "1"}]], "=", 
    RowBox[{"(", GridBox[{
       {"0", "2"},
       {"2", "0"}
      }], ")"}]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.788137176981675*^9, 
  3.7881372089154367`*^9}},ExpressionUUID->"ddfdb6e9-3798-4e88-8380-\
b933bd2c760d"]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[{
 
 CounterBox["Subsection"],
 ". The Witt Algebra"
}], "Subsection",
 CellChangeTimes->{
  3.7884027959399357`*^9, {3.788402908078457*^9, 
   3.7884029111492357`*^9}},ExpressionUUID->"5568b67c-b8f6-4a7f-8620-\
72cd725e4f41"],

Cell[TextData[{
 "The generators of 2D conformal transformation can be found from the Laurent \
expansion (about the origin) of the mapping ",
 Cell[BoxData[
  FormBox[
   RowBox[{"z", "\[Rule]", 
    RowBox[{"z", "+", 
     RowBox[{"\[Epsilon]", "(", "z", ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "c04b32cc-6d61-46d2-92cf-cfa7c47d6997"],
 ": mathematically, we write"
}], "Text",
 CellChangeTimes->{{3.788403118680385*^9, 3.788403182785385*^9}, {
  3.7884035203873873`*^9, 3.788403526590371*^9}, {3.788403656227294*^9, 
  3.788403664181686*^9}},ExpressionUUID->"755e494c-98ac-4603-9c4b-\
7c560d576225"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox["z", "\[Prime]"], "=", 
   RowBox[{"z", "+", 
    RowBox[{
     UnderscriptBox["\[Sum]", "n"], " ", 
     RowBox[{
      SubscriptBox["\[Epsilon]", "n"], 
      SuperscriptBox["z", "n"]}]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.788403118680385*^9, 3.7884031961405087`*^9}, {
   3.788403227556346*^9, 3.7884032287032537`*^9}, 3.788403530793581*^9, {
   3.7884036109412193`*^9, 
   3.7884036144936037`*^9}},ExpressionUUID->"3c57c898-4b95-4091-bf3a-\
d3c7e3c4da2e"],

Cell["However, in CFT, people adopt the convention", "Text",
 CellChangeTimes->{{3.788403536808609*^9, 3.788403539208671*^9}, {
  3.7884035741666613`*^9, 3.788403585301498*^9}, {3.788404225502241*^9, 
  3.7884042257625027`*^9}},ExpressionUUID->"1eebad4c-cb6b-412d-8edd-\
4d3d5e4e8a90"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox["z", "\[Prime]"], "=", 
   RowBox[{"z", "+", 
    RowBox[{
     UnderscriptBox["\[Sum]", "n"], " ", 
     RowBox[{
      SuperscriptBox["z", 
       RowBox[{"n", "+", "1"}]], " ", 
      SubscriptBox["\[Epsilon]", "n"]}]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.7884035934962378`*^9, 3.7884036200190477`*^9}, {
   3.788403720727159*^9, 3.7884037664034653`*^9}, {3.788403828480413*^9, 
   3.788403883692577*^9}, {3.788404216113434*^9, 3.7884042184239187`*^9}, 
   3.7884042710598583`*^9},ExpressionUUID->"75984981-9595-476c-9f8d-\
93dde032f8bf"],

Cell[TextData[{
 "Then, for the transformation of a particular ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "7e9a4586-7fcf-4df6-b0bf-4d1a31e695e0"],
 ", we have"
}], "Text",
 CellChangeTimes->{{3.788403886597478*^9, 
  3.788403905159038*^9}},ExpressionUUID->"d7219f93-e075-4216-b3f8-\
241c5328db87"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox["z", "\[Prime]"], "=", 
   RowBox[{
    RowBox[{"z", "+", 
     RowBox[{
      SuperscriptBox["z", 
       RowBox[{"n", "+", "1"}]], 
      SubscriptBox["\[Epsilon]", "n"]}]}], "\[Congruent]", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "-", 
       SubscriptBox["l", "n"]}], ")"}], "z"}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.7884039090012217`*^9, 3.7884039289506474`*^9}, {
  3.7884042289113073`*^9, 
  3.788404234029622*^9}},ExpressionUUID->"8403f6cd-5d91-485c-bbc2-\
73255c2351ae"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["l", "n"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "405cffda-73a6-4876-8597-f1b1b2af458b"],
 " is defined as the differential operator"
}], "Text",
 CellChangeTimes->{{3.788403942446672*^9, 
  3.7884039653877983`*^9}},ExpressionUUID->"69dddd1a-fcb2-47c8-87a5-\
2bbede9e918d"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["l", "n"], "=", 
   RowBox[{
    RowBox[{"-", 
     SuperscriptBox["z", 
      RowBox[{"n", "+", "1"}]]}], 
    SubscriptBox["\[PartialD]", "z"]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.788403942446672*^9, 
  3.788403987710144*^9}},ExpressionUUID->"8357720d-e94c-454d-ac4f-\
c9d74d017017"],

Cell[TextData[{
 "so that a finite transformation is the exponentiation ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{"-", 
     SubscriptBox["l", "n"]}]], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "7215ddf3-9b45-4d0e-99ad-717ae769ac0c"],
 ". Thus ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["l", "n"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "139f9445-59b5-479b-80ec-b3b12dc39385"],
 " is interpreted as a transformation generator."
}], "Text",
 CellChangeTimes->{{3.7884040429624853`*^9, 3.788404178071257*^9}, {
  3.7884042389332523`*^9, 
  3.788404238933321*^9}},ExpressionUUID->"919d39a9-a4dd-4ab6-9f10-\
feee10d0909f"],

Cell["Similarly, for the anti-holomorphic part, people write", "Text",
 CellChangeTimes->{{3.788403622192644*^9, 
  3.788403634062696*^9}},ExpressionUUID->"4290fe8a-3c80-4d90-8c97-\
cb56ef235593"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox[
    OverscriptBox["z", "_"], "\[Prime]"], "=", 
   RowBox[{
    RowBox[{
     OverscriptBox["z", "_"], "+", 
     RowBox[{
      UnderscriptBox["\[Sum]", "n"], " ", 
      RowBox[{
       SuperscriptBox[
        OverscriptBox["z", "_"], 
        RowBox[{"n", "+", "1"}]], " ", 
       SubscriptBox[
        OverscriptBox["\[Epsilon]", "_"], "n"]}]}]}], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{
        UnderscriptBox["\[Sum]", "n"], 
        SubscriptBox[
         OverscriptBox["l", "_"], "n"]}]}], ")"}], 
     OverscriptBox["z", "_"]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.788403622192644*^9, 3.7884036530338783`*^9}, {
  3.7884037264482203`*^9, 3.788403727378195*^9}, {3.7884039974308023`*^9, 
  3.788404011509404*^9}, {3.7884042635354633`*^9, 
  3.7884042747751427`*^9}},ExpressionUUID->"35a74eab-3be8-4293-8787-\
6a7f6ef03c3b"],

Cell["with", "Text",
 CellChangeTimes->{{3.788404013070445*^9, 
  3.788404014167451*^9}},ExpressionUUID->"e3907a57-808d-4ad1-8e87-\
1242c0623353"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox[
    OverscriptBox["l", "_"], "n"], "=", 
   RowBox[{
    RowBox[{"-", 
     SuperscriptBox[
      OverscriptBox["z", "_"], 
      RowBox[{"n", "+", "1"}]]}], 
    SubscriptBox["\[PartialD]", 
     OverscriptBox["z", "_"]]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.788404013070445*^9, 
  3.7884040361932373`*^9}},ExpressionUUID->"d9158ae8-a6e2-4bbe-b4da-\
3d3e83c1bb1b"],

Cell[TextData[{
 "The commutation rules of the generators ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["l", "n"], ",", 
    SubscriptBox[
     OverscriptBox["l", "_"], "n"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "2cabb5ab-6d51-4e56-a3c7-1ede71237314"],
 " are "
}], "Text",
 CellChangeTimes->{{3.788404280192699*^9, 
  3.788404299178574*^9}},ExpressionUUID->"ceb8a506-cb54-421f-93f7-\
f52b73425787"],

Cell[BoxData[{
 FormBox[
  RowBox[{
   RowBox[{"[", 
    RowBox[{
     SubscriptBox["l", "n"], ",", 
     SubscriptBox["l", "m"]}], "]"}], "=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"n", "-", "m"}], ")"}], 
    SubscriptBox["l", 
     RowBox[{"m", "+", "n"}]]}]}], TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{
   RowBox[{"[", 
    RowBox[{
     SubscriptBox[
      OverscriptBox["l", "_"], "n"], ",", 
     SubscriptBox[
      OverscriptBox["l", "_"], "m"]}], "]"}], "=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"n", "-", "m"}], ")"}], 
    SubscriptBox[
     OverscriptBox["l", "_"], 
     RowBox[{"m", "+", "n"}]]}]}], TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{
   RowBox[{"[", 
    RowBox[{
     SubscriptBox["l", "n"], ",", 
     SubscriptBox[
      OverscriptBox["l", "_"], "m"]}], "]"}], "=", "0"}], 
  TraditionalForm]}], "EquationNumbered",
 CellFrame->0.5,
 CellChangeTimes->{{3.788404280192699*^9, 
  3.788404351063415*^9}},ExpressionUUID->"9b186b5c-6d2e-43e7-a832-\
d5da61275e12"],

Cell[TextData[{
 "called the ",
 StyleBox["Witt algebra",
  FontWeight->"Bold"],
 ". "
}], "Text",
 CellChangeTimes->{{3.7884043537697372`*^9, 
  3.788404365203671*^9}},ExpressionUUID->"33452043-05b1-43fa-9b37-\
72ba47070996"]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[{
 
 CounterBox["Subsection"],
 ". Global Conformal Transformations in 2D"
}], "Subsection",
 CellChangeTimes->{
  3.7884027959399357`*^9, {3.788402908078457*^9, 3.7884029111492357`*^9}, {
   3.7884086910657043`*^9, 
   3.788408700299719*^9}},ExpressionUUID->"427cd0b8-9117-47dd-ad43-\
c9fd49863330"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"f", "(", "z", ")"}], "=", 
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{"a", " ", "z"}], "+", "b"}], 
      RowBox[{
       RowBox[{"c", " ", "z"}], "+", "d"}]], "   ", "with", "  ", 
     RowBox[{"det", "(", GridBox[{
        {"a", "b"},
        {"c", "d"}
       }], ")"}]}], "=", "1"}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.788408705160297*^9, 
  3.7884087378409348`*^9}},ExpressionUUID->"ca92c404-5420-4890-b8d8-\
55c964f50a70"]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[{
 
 CounterBox["Subsection"],
 ". Central Extension and Virasoro Algebra"
}], "Subsection",
 CellChangeTimes->{
  3.7884027959399357`*^9, {3.788402908078457*^9, 3.7884029111492357`*^9}, {
   3.788404560867743*^9, 
   3.788404569693246*^9}},ExpressionUUID->"3cdb742b-5233-4550-b918-\
27135891f6f7"],

Cell[TextData[{
 "There is a ",
 StyleBox["unique",
  FontSlant->"Italic"],
 " ",
 StyleBox["central extension",
  FontWeight->"Bold"],
 " of the Witt algebra, called the ",
 StyleBox["Virasoro algebra",
  FontWeight->"Bold"],
 ". "
}], "Text",
 CellChangeTimes->{{3.788404597503386*^9, 3.788404754502235*^9}, {
  3.78840497491087*^9, 3.788404982184226*^9}, {3.7884054775485497`*^9, 
  3.788405480504908*^9}},ExpressionUUID->"f6387b18-275d-4a7f-9cad-\
e0e6cc067312"],

Cell[TextData[{
 "We first consider the holomorphic part. According to the mathematical \
definition of central extensions, the generators ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["l", "n"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "b55afcd2-3af5-44a4-80c2-18d84a372433"],
 " are promoted to ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["L", "n"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d2bb5ff1-e19e-4b6d-98dd-8b5c7956da38"],
 ", and a new element ",
 Cell[BoxData[
  FormBox[
   RowBox[{"c", "\[Element]", "\[DoubleStruckCapitalC]"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "50485aeb-48df-4233-adfd-37bc9e4e1aed"],
 " (the ",
 StyleBox["central charge",
  FontWeight->"Bold"],
 ") is introduced so that"
}], "Text",
 CellChangeTimes->{{3.788404597503386*^9, 3.788404754502235*^9}, {
  3.78840497491087*^9, 3.788404982184226*^9}, {3.7884054775485497`*^9, 
  3.788405496960647*^9}, {3.788406230789193*^9, 
  3.788406242327224*^9}},ExpressionUUID->"64e51ef5-3647-4d8c-899e-\
7b0b99954466"],

Cell[BoxData[{
 FormBox[
  RowBox[{
   RowBox[{"[", 
    RowBox[{
     SubscriptBox["L", "n"], ",", 
     SubscriptBox["L", "m"]}], "]"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"n", "-", "m"}], ")"}], 
     SubscriptBox["L", 
      RowBox[{"m", "+", "n"}]]}], "+", 
    RowBox[{"c", " ", 
     RowBox[{"p", "(", 
      RowBox[{"n", ",", "m"}], ")"}]}]}]}], 
  TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{
   RowBox[{"[", 
    RowBox[{
     SubscriptBox["L", "n"], ",", "c"}], "]"}], "=", "0"}], 
  TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{
   RowBox[{"[", 
    RowBox[{"c", ",", "c"}], "]"}], "=", "0"}], 
  TraditionalForm]}], "EquationNumbered",
 CellFrame->0.5,
 CellChangeTimes->{{3.788404597503386*^9, 3.788404961016938*^9}, 
   3.788405317037272*^9, {3.788405347853862*^9, 3.7884054577141237`*^9}, {
   3.78840557530586*^9, 
   3.7884055803152533`*^9}},ExpressionUUID->"951f8271-7040-4640-8dbb-\
354ebcfb888a"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["p", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "126eab07-5cfb-4a72-ab86-066ce518bfba"],
 " is a ",
 StyleBox["bilinear",
  FontSlant->"Italic"],
 " mapping onto ",
 Cell[BoxData[
  FormBox["\[DoubleStruckCapitalC]", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "6776b4b0-d767-47d6-96c7-1ad8878e5225"],
 " (this is required by the bi-linearity of the Lie bracket). The last two \
equations mean that ",
 Cell[BoxData[
  FormBox["c", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "0215f63c-4b0d-43c6-918a-00b4c8c1de3c"],
 " commutes with all group elements in the Virasoro algebra. Similar \
relations hold for the anti-holomorphic part (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     OverscriptBox["l", "_"], "n"], "\[Rule]", 
    SubscriptBox[
     OverscriptBox["L", "_"], "n"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "44d859c4-d183-4628-81c7-af7b5aa321d2"],
 " etc). "
}], "Text",
 CellChangeTimes->{{3.788404971832056*^9, 3.788405076029276*^9}, {
  3.788405113437025*^9, 3.7884051726540003`*^9}, {3.788405440052537*^9, 
  3.7884054683170443`*^9}, {3.788405502268688*^9, 3.788405551744335*^9}, {
  3.78840626246216*^9, 
  3.788406305962612*^9}},ExpressionUUID->"108c6d23-3b73-4f8c-90b7-\
673f21e597e9"],

Cell[TextData[{
 StyleBox["Remark",
  FontSlant->"Italic"],
 ": In Virasoro algebra, the central charge is simply a (complex) number. But \
for central extensions to an arbitrary Lie algebra, ",
 Cell[BoxData[
  FormBox["c", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "526ec21b-59bf-4cad-9a39-07c4c5ff6582"],
 " can be any object in some set ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "96ca4ad6-a873-4250-b27f-5e919ceeede2"],
 ". Then ",
 Cell[BoxData[
  FormBox["p", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "7d530e45-cb62-4eb0-a583-8bb065ca8227"],
 " should map onto ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "ef47e21f-8698-4da4-919f-a3cdc87aae10"],
 ". "
}], "Text",
 CellChangeTimes->{{3.788404971832056*^9, 3.788405076029276*^9}, {
  3.788405113437025*^9, 
  3.788405250621294*^9}},ExpressionUUID->"c457d481-e7a9-46e6-af0f-\
18c648376a22"],

Cell[CellGroupData[{

Cell[TextData[{
 "Determine the Function ",
 Cell[BoxData[
  FormBox[
   RowBox[{"p", "(", 
    RowBox[{"m", ",", "n"}], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "f08be513-842c-4d84-a556-9689bfbb04a5"]
}], "Subsubsection",
 CellChangeTimes->{{3.7884056635582123`*^9, 
  3.788405676268806*^9}},ExpressionUUID->"c8499be0-d7e1-46bc-8e52-\
f7da72e2e257"],

Cell[TextData[{
 "1. Since the Lie bracket is ",
 StyleBox["anti-symmetric",
  FontSlant->"Italic"],
 ", we obtain"
}], "Text",
 CellChangeTimes->{{3.7884056923437967`*^9, 3.78840570467006*^9}, {
  3.788406097508024*^9, 
  3.7884060976948338`*^9}},ExpressionUUID->"1b93346e-544e-4a93-80c5-\
8c4a795e4d1c"],

Cell[BoxData[{
 FormBox[
  RowBox[{
   RowBox[{"[", 
    RowBox[{
     SubscriptBox["L", "n"], ",", 
     SubscriptBox["L", "m"]}], "]"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"n", "-", "m"}], ")"}], 
     SubscriptBox["L", 
      RowBox[{"m", "+", "n"}]]}], "+", 
    RowBox[{"c", " ", "p", 
     RowBox[{"(", 
      RowBox[{"n", ",", "m"}], ")"}]}]}]}], 
  TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{"-", 
     RowBox[{"[", 
      RowBox[{
       SubscriptBox["L", "m"], ",", 
       SubscriptBox["L", "n"]}], "]"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       RowBox[{"(", 
        RowBox[{"m", "-", "n"}], ")"}]}], 
      SubscriptBox["L", 
       RowBox[{"n", "+", "m"}]]}], "-", 
     RowBox[{"c", " ", 
      RowBox[{"p", "(", 
       RowBox[{"m", ",", "n"}], ")"}], " "}]}]}]}], 
  TraditionalForm]}], "Equation",
 CellChangeTimes->{{3.788405710561961*^9, 
  3.7884058324433727`*^9}},ExpressionUUID->"2ed43671-b41d-4b78-ae7e-\
665358a87d91"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[DoubleLongRightArrow]", " ", 
    RowBox[{"p", "(", 
     RowBox[{"n", ",", "m"}], ")"}]}], "=", 
   RowBox[{"-", 
    RowBox[{"p", "(", 
     RowBox[{"m", ",", "n"}], ")"}]}]}], TraditionalForm]], "EquationNumbered",\

 CellChangeTimes->{{3.788405710561961*^9, 
  3.7884058324416437`*^9}},ExpressionUUID->"0e7f2b0f-5244-4d8b-99e6-\
228788bbc4ff"],

Cell["2. We can always make", "Text",
 CellChangeTimes->{{3.788405710561961*^9, 
  3.788405838697566*^9}},ExpressionUUID->"da3578bd-6c09-4bdd-83de-\
cf3735aeefcf"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"p", "(", 
     RowBox[{"1", ",", 
      RowBox[{"-", "1"}]}], ")"}], "=", "0"}], ",", " ", 
   RowBox[{
    RowBox[{"p", "(", 
     RowBox[{"n", ",", "0"}], ")"}], "=", "0"}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.788405710561961*^9, 
  3.788405847980358*^9}},ExpressionUUID->"4ae23ef5-7b00-4a07-9447-\
4bdd241c161c"],

Cell[TextData[{
 "If they are not zero, we redefine the ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["L", "n"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "845aa397-67a2-4c5f-9433-429d654203d5"],
 " operators as"
}], "Text",
