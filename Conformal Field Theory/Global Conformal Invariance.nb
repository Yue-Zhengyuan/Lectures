617968f9973c"],

Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    RowBox[{"\[PartialD]", 
     SuperscriptBox["x", "\[Rho]"]}], 
    RowBox[{"\[PartialD]", 
     SuperscriptBox["x", "\[Prime]\[Mu]"]}]], "=", 
   RowBox[{
    SubsuperscriptBox["\[Delta]", "\[Mu]", "\[Rho]"], "-", 
    FractionBox[
     RowBox[{"\[PartialD]", 
      RowBox[{
       SuperscriptBox["\[Epsilon]", "\[Rho]"], "(", "x", ")"}]}], 
     RowBox[{"\[PartialD]", 
      SuperscriptBox["x", "\[Mu]"]}]], "+", 
    RowBox[{"O", "(", 
     SuperscriptBox["\[Epsilon]", "2"], ")"}]}]}], 
  TraditionalForm]], "Equation",
