Cell[CellGroupData[{

Cell["Two-Point Functions", "Subsubsection",
 CellChangeTimes->{{3.788430921771703*^9, 
  3.788430926592782*^9}},ExpressionUUID->"3ec895ae-e1c9-4987-80fb-\
7fa9d85e6d57"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[LeftAngleBracket]", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[Phi]", "i"], "(", 
      RowBox[{"z", ",", 
       OverscriptBox["z", "_"]}], ")"}], 
     RowBox[{
      SubscriptBox["\[Phi]", "j"], "(", 
      RowBox[{"w", ",", 
       OverscriptBox["w", "_"]}], ")"}]}], "\[RightAngleBracket]"}], "=", 
   FractionBox[
    RowBox[{
     SubscriptBox["C", 
      RowBox[{"i", " ", "j"}]], 
     SubscriptBox["\[Delta]", 
      RowBox[{
       SubscriptBox["h", "i"], 
       SubscriptBox["h", "j"]}]], 
     SubscriptBox["\[Delta]", 
      RowBox[{
       SubscriptBox[
        OverscriptBox["h", "_"], "i"], 
       SubscriptBox[
        OverscriptBox["h", "_"], "j"]}]]}], 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"z", "-", "w"}], ")"}], 
      RowBox[{"2", 
       SubscriptBox["h", "i"]}]], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        OverscriptBox["z", "_"], "-", 
        OverscriptBox["w", "_"]}], ")"}], 
      RowBox[{"2", 
       SubscriptBox[
        OverscriptBox["h", "_"], "i"]}]]}]]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.7884310057029743`*^9, 3.788431015060396*^9}, {
  3.788431078699644*^9, 3.788431182082033*^9}, {3.788431245277809*^9, 
  3.788431285860701*^9}},ExpressionUUID->"781a0d6c-ac11-4f82-891a-\
09c221165662"],

Cell["\<\
The Kronecker-deltas mean that the two-point function vanishes if the \
conformal dimensions of the two fields are different. \
\>", "Text",
