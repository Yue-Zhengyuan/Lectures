     SuperscriptBox["x", "0"], "+", 
     RowBox[{"\[ImaginaryI]", " ", 
      SuperscriptBox["x", "1"]}]}]}], ",", " ", 
   RowBox[{
    OverscriptBox["z", "_"], "=", 
    RowBox[{
     SuperscriptBox["x", "0"], "-", 
     RowBox[{"\[ImaginaryI]", " ", 
      SuperscriptBox["x", "1"]}]}]}]}], TraditionalForm]], "EquationNumbered",\

 CellChangeTimes->{{3.788135910028288*^9, 3.788135937064755*^9}, {
  3.788136017261277*^9, 3.78813604773566*^9}, {3.788402599230241*^9, 
  3.788402599232094*^9}},ExpressionUUID->"1acbe49b-e16c-4dcf-bd65-\
b4b50f0a2e62"],

Cell[BoxData[
 FormBox[
