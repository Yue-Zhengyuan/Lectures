 CounterBox["Subsection"],
 ". Central Charge"
}], "Subsection",
 CellChangeTimes->{{3.784671443506769*^9, 3.7846714497264423`*^9}, {
  3.788127948978467*^9, 3.788127950328603*^9}, {3.788128022743924*^9, 
  3.7881280291402493`*^9}, {3.78813339159142*^9, 
  3.788133393854507*^9}},ExpressionUUID->"780b4925-de1d-4da8-9fb7-\
099a2f36a2cf"],

Cell["\<\
The central charge is determined from the OPE of the EM tensor with itself:\
\>", "Text",
 CellChangeTimes->{{3.788167049998005*^9, 
  3.7881670691508617`*^9}},ExpressionUUID->"b9f60b6f-1491-42c9-94d2-\
d6396f882db6"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"T", "(", "z", ")"}], 
    RowBox[{"T", "(", "w", ")"}]}], "=", 
   RowBox[{"4", 
    SuperscriptBox["\[Pi]", "2"], 
    RowBox[{
     SuperscriptBox["h", "2"], ":", 
     RowBox[{
      RowBox[{"\[PartialD]", 
       RowBox[{"\[Phi]", "(", "z", ")"}]}], 
      RowBox[{"\[PartialD]", 
       RowBox[{"\[Phi]", "(", "z", ")"}]}]}], ":", " ", ":", 
     RowBox[{
      RowBox[{"\[PartialD]", 
       RowBox[{"\[Phi]", "(", "w", ")"}]}], 
      RowBox[{"\[PartialD]", 
       RowBox[{"\[Phi]", "(", "w", ")"}]}]}], ":"}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.788167049998005*^9, 3.788167125123558*^9}, {
  3.788231054833075*^9, 
  3.788231071011429*^9}},ExpressionUUID->"e2113616-6b48-45a9-ab15-\
3ee03e56c8c3"],

Cell["Using Wick\[CloseCurlyQuote]s Theorem", "Text",
 CellChangeTimes->{{3.7881671281147413`*^9, 
  3.788167136679431*^9}},ExpressionUUID->"72690c8c-fcf3-4a7d-b1b8-\
3a3ebb721f86"],

Cell[BoxData[{
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"T", "(", "z", ")"}], 
    RowBox[{"T", "(", "w", ")"}]}], "=", 
   RowBox[{"4", 
    SuperscriptBox["\[Pi]", "2"], 
    RowBox[{
     SuperscriptBox["h", "2"], "(", 
     RowBox[{":", 
      RowBox[{
       RowBox[{"\[PartialD]", 
        OverscriptBox[
         RowBox[{"\[Phi]", 
          RowBox[{"(", "z", ")"}], 
          RowBox[{"\[PartialD]", "\[Phi]"}]}], "\[OverBracket]"]}], 
       RowBox[{"(", "z", ")"}]}], ":", " ", ":", 
      RowBox[{
       RowBox[{"\[PartialD]", 
        OverscriptBox[
         RowBox[{"\[Phi]", 
          RowBox[{"(", "w", ")"}], 
          RowBox[{"\[PartialD]", "\[Phi]"}]}], "\[OverBracket]"]}], 
       RowBox[{"(", "w", ")"}]}], ":", "\[LineSeparator]", "\t\t", 
      RowBox[{"+", 
       RowBox[{":", 
        RowBox[{
         RowBox[{"\[PartialD]", 
          OverscriptBox[
           RowBox[{"\[Phi]", 
            RowBox[{"(", "z", ")"}], 
            RowBox[{"\[PartialD]", 
             OverscriptBox[
              RowBox[{
               RowBox[{"\[Phi]", 
                RowBox[{"(", "z", ")"}]}], ":", " ", ":", 
               RowBox[{"\[PartialD]", "\[Phi]"}]}], "\[OverBracket]"]}], 
            RowBox[{"(", "w", ")"}], 
            RowBox[{"\[PartialD]", "\[Phi]"}]}], "\[OverBracket]"]}], 
         RowBox[{"(", "w", ")"}]}], ":"}]}]}], ")"}]}]}], 
  TraditionalForm], "\[LineSeparator]", 
 FormBox["=", TraditionalForm]}], "Equation",
 CellChangeTimes->{{3.7882309006449623`*^9, 3.788230951843851*^9}, {
  3.788231006943555*^9, 
  3.788231122675047*^9}},ExpressionUUID->"a29e8842-559e-4d12-a4f4-\
70aeab32c60c"]
}, Open  ]],

