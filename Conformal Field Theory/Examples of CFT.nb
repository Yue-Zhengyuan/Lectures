Cell[CellGroupData[{

Cell["Free Fermion", "Section",
 CellChangeTimes->{{3.7846712041476507`*^9, 3.7846712112211733`*^9}, {
  3.788126872368821*^9, 
  3.78812687459291*^9}},ExpressionUUID->"716febaf-1f9f-4a02-a53a-\
0d087e6b537d"],

Cell[CellGroupData[{

Cell[TextData[{
 
 CounterBox["Subsection"],
 ". Action and Equation of Motion"
}], "Subsection",
 CellChangeTimes->{{3.784671443506769*^9, 3.7846714497264423`*^9}, {
  3.788127948978467*^9, 
  3.788127950328603*^9}},ExpressionUUID->"46cc54f8-5930-46b1-aa18-\
827246ed493b"],

Cell[BoxData[
 FormBox[
  RowBox[{"S", "=", 
   RowBox[{
    FractionBox["1", "2"], "h", 
    RowBox[{"\[Integral]", 
     RowBox[{
      RowBox[{
       SuperscriptBox["\[DifferentialD]", "2"], "x"}], " ", 
      SuperscriptBox["\[CapitalPsi]", "\[Dagger]"], 
      SuperscriptBox["\[Gamma]", "0"], 
      SuperscriptBox["\[Gamma]", "\[Mu]"], 
      RowBox[{
       SubscriptBox["\[PartialD]", "\[Mu]"], "\[CapitalPsi]"}]}]}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.788232845525*^9, 3.7882328751332693`*^9}, {
  3.788233080825741*^9, 
  3.7882330863605433`*^9}},ExpressionUUID->"3f7f3ef6-7de8-43ad-a9ce-\
c1956c5a482d"],

Cell[TextData[{
 "Here the ",
 StyleBox["Dirac matrices",
  FontWeight->"Bold"],
 " ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[Gamma]", "\[Mu]"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "3cd8ab3f-d529-4455-82b3-3d4a11dd1e78"],
 " satisfy the ",
 StyleBox["Dirac algebra",
  FontWeight->"Bold"],
 ":"
}], "Text",
 CellChangeTimes->{{3.7882328854491987`*^9, 
  3.78823291888591*^9}},ExpressionUUID->"944d379e-04a3-4068-9c8a-\
9f16108c14ac"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{
     SuperscriptBox["\[Gamma]", "\[Mu]"], 
     SuperscriptBox["\[Gamma]", "\[Nu]"]}], "+", 
    RowBox[{
     SuperscriptBox["\[Gamma]", "\[Nu]"], 
     SuperscriptBox["\[Gamma]", "\[Mu]"]}]}], "=", 
   RowBox[{"2", 
    SuperscriptBox["\[Eta]", 
     RowBox[{"\[Mu]", " ", "\[Nu]"}]]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.7882328854491987`*^9, 3.7882329660144167`*^9}, {
  3.788232996277224*^9, 
  3.788232996737232*^9}},ExpressionUUID->"9b1bf265-54b4-4491-bfa6-\
c729ad6d6179"],

Cell[TextData[{
 "We can ",
 StyleBox["choose the representation",
  FontSlant->"Italic"],
 " in which ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Eta]", 
     RowBox[{"\[Mu]", " ", "\[Nu]"}]], "=", 
    RowBox[{"diag", "(", 
     RowBox[{"1", ",", "1"}], ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "94d5f6e1-9f2d-46e4-a8a3-e1e3022c01e0"],
 " (in Euclidean space). Then"
}], "Text",
 CellChangeTimes->{{3.788232971397917*^9, 
  3.788233018157102*^9}},ExpressionUUID->"41159aa2-878a-4cbe-a13f-\
667d7a2c22f6"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SuperscriptBox["\[Gamma]", "0"], "=", 
    RowBox[{
     SubscriptBox["\[Sigma]", "x"], "=", 
     RowBox[{"(", GridBox[{
        {"0", "1"},
        {"1", "0"}
       }], ")"}]}]}], ",", " ", 
   RowBox[{
    SuperscriptBox["\[Gamma]", "1"], "=", 
    RowBox[{
     SubscriptBox["\[Sigma]", "y"], "=", 
     RowBox[{"(", GridBox[{
        {"0", 
         RowBox[{"-", "\[ImaginaryI]"}]},
        {"\[ImaginaryI]", "0"}
       }], ")"}]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.788232971397917*^9, 
  3.788233056300045*^9}},ExpressionUUID->"7096bd5e-ef3c-4996-b192-\
fa1539e3633b"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SuperscriptBox["\[Gamma]", "0"], 
    SuperscriptBox["\[Gamma]", "\[Mu]"], 
    SubscriptBox["\[PartialD]", "\[Mu]"]}], "=", 
   RowBox[{"2", 
    RowBox[{"(", GridBox[{
       {
        OverscriptBox["\[PartialD]", "_"], "0"},
       {"0", "\[PartialD]"}
