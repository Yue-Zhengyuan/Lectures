}], "Subsection",
 CellChangeTimes->{{3.794199385789407*^9, 3.794199402168536*^9}, {
  3.794202983587244*^9, 3.794203000265932*^9}, {3.79428620264554*^9, 
  3.794286209573552*^9}},ExpressionUUID->"1ec6ba53-dfb2-472f-a222-\
2a18fe0e57ce"],

Cell["The specific heat per volume is", "Text",
 CellChangeTimes->{{3.7942869719454603`*^9, 3.794286991382579*^9}, {
  3.794290062669631*^9, 3.794290062909649*^9}, {3.794290169549016*^9, 
  3.7942901697698402`*^9}, {3.794291618525127*^9, 
  3.794291621544365*^9}},ExpressionUUID->"291516b7-9048-4aa8-b033-\
0a7e4737b8ba"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["c", "V"], "=", 
   RowBox[{
    RowBox[{
     FractionBox["1", "V"], 
     FractionBox[
      RowBox[{"\[PartialD]", "E"}], 
      RowBox[{"\[PartialD]", "T"}]]}], "=", 
    RowBox[{
     FractionBox["1", "V"], 
     RowBox[{
      UnderscriptBox["\[Sum]", 
       RowBox[{"k", ",", "s"}]], 
      RowBox[{
       FractionBox["\[PartialD]", 
        RowBox[{"\[PartialD]", "T"}]], 
       RowBox[{"(", 
        FractionBox[
         RowBox[{"\[HBar]", " ", 
          RowBox[{
           SubscriptBox["\[Omega]", "s"], "(", "k", ")"}]}], 
         RowBox[{
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"\[Beta]", " ", "\[HBar]", " ", 
            RowBox[{
             SubscriptBox["\[Omega]", "s"], "(", "k", ")"}]}]], "-", "1"}]], 
        ")"}]}]}]}]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.7942869719454603`*^9, 3.794287041288629*^9}, {
   3.794290057492897*^9, 3.794290110734479*^9}, {3.794291625222933*^9, 
   3.794291628529477*^9}, 3.794292203890706*^9, {3.794356569832366*^9, 
   3.794356570862365*^9}},ExpressionUUID->"a72dde1c-7e27-4418-a658-\
e035048b6305"],

Cell["Or, in the continuous limit (large crystal)", "Text",
 CellChangeTimes->{{3.794290649351582*^9, 
  3.794290664351843*^9}},ExpressionUUID->"d17bebe6-a70a-4e16-9570-\
5f0ef0591bb2"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["c", "V"], "=", 
   RowBox[{
    FractionBox["\[PartialD]", 
     RowBox[{"\[PartialD]", "T"}]], 
    RowBox[{"(", 
     RowBox[{
      UnderscriptBox["\[Sum]", "s"], 
      RowBox[{
       SubscriptBox["\[Integral]", "BZ"], 
       RowBox[{
        FractionBox[
         RowBox[{
          SuperscriptBox["\[DifferentialD]", "3"], "k"}], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"2", "\[Pi]"}], ")"}], "3"]], 
        FractionBox[
         RowBox[{"\[HBar]", " ", 
          RowBox[{
           SubscriptBox["\[Omega]", "s"], "(", "k", ")"}]}], 
         RowBox[{
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"\[HBar]", " ", 
            RowBox[{
             RowBox[{
              SubscriptBox["\[Omega]", "s"], "(", "k", ")"}], "/", "T"}]}]], 
          "-", "1"}]]}]}]}], ")"}]}]}], TraditionalForm]], "EquationNumbered",\

 CellChangeTimes->{{3.794290649351582*^9, 3.794290720861215*^9}, {
   3.794291013712552*^9, 3.794291014854783*^9}, {3.7942916121596518`*^9, 
   3.794291613496464*^9}, 3.794292205090652*^9, {3.7943555965235567`*^9, 
   3.7943555982984037`*^9}},ExpressionUUID->"9b066257-2d69-4af2-b803-\
fdca0dd0816b"],

