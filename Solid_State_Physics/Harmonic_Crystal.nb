  3.7942060368170013`*^9}},ExpressionUUID->"17be2289-6134-45f1-b522-\
7ea090fbf937"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[Omega]", "(", "k", ")"}], "=", 
   RowBox[{
    RowBox[{"2", 
     SqrtBox[
      RowBox[{
       UnderscriptBox["\[Sum]", 
        RowBox[{"l", ">", "0"}]], 
       RowBox[{
        FractionBox[
         SubscriptBox["K", "l"], "m"], 
        SuperscriptBox["sin", "2"], 
        FractionBox[
         RowBox[{"k", " ", "l", " ", "a"}], "2"]}]}]]}], "=", 
    RowBox[{"a", 
