      RowBox[{
       UnderscriptBox["\[Sum]", 
        RowBox[{"l", ">", "0"}]], 
       FractionBox[
        RowBox[{
         SubscriptBox["K", "l"], 
         SuperscriptBox["l", "2"]}], "m"]}]], 
     RowBox[{"\[LeftBracketingBar]", "k", "\[RightBracketingBar]"}]}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.794206006783866*^9, 3.794206048777892*^9}, {
