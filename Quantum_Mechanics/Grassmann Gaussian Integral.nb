Cell[BoxData[{
 FormBox[
  RowBox[{
   RowBox[{"Z", "(", "\[Xi]", ")"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"\[Integral]", 
      RowBox[{"\[DifferentialD]", 
       SubscriptBox["\[Eta]", "n"]}]}], "..."}], 
    RowBox[{"\[DifferentialD]", 
     SubscriptBox["\[Eta]", "1"]}], 
    RowBox[{
     UnderscriptBox["\[Product]", 
      RowBox[{"i", "<", "j"}]], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", 
        RowBox[{
         SubscriptBox["\[Eta]", "i"], 
         SubscriptBox["A", 
          RowBox[{"i", " ", "j"}]], 
         SubscriptBox["\[Eta]", "j"]}]}], ")"}], 
      RowBox[{
       UnderoverscriptBox["\[Product]", 
        RowBox[{"i", "=", "1"}], "n"], " ", 
       RowBox[{"(", 
        RowBox[{"1", "+", 
         RowBox[{
          SubscriptBox["\[Xi]", "i"], 
          SubscriptBox["\[Eta]", "i"]}]}], ")"}]}]}]}]}]}], 
  TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{
     RowBox[{"\[Integral]", 
      RowBox[{"\[DifferentialD]", 
       SubscriptBox["\[Eta]", "n"]}]}], "..."}], 
    RowBox[{"\[DifferentialD]", 
     SubscriptBox["\[Eta]", "1"]}]}]}], TraditionalForm]}], "Equation",
