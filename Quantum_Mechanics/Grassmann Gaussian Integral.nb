Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"Z", "(", "0", ")"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"\[Integral]", 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[Eta]", "1"]}]}], "..."}], 
     RowBox[{"\[DifferentialD]", 
      SubscriptBox["\[Eta]", "n"]}], 
     RowBox[{"exp", "(", 
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", "2"]}], 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"i", ",", 
          RowBox[{"j", "=", "1"}]}], "n"], 
        RowBox[{
         SubscriptBox["\[Eta]", "i"], 
         SubscriptBox["A", 
          RowBox[{"i", " ", "j"}]], 
         SubscriptBox["\[Eta]", "j"]}]}]}], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Integral]", 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[Eta]", "n"]}]}], "..."}], 
     RowBox[{"\[DifferentialD]", 
      SubscriptBox["\[Eta]", "1"]}], 
     RowBox[{"exp", "(", 
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", "2"]}], 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"i", ",", 
          RowBox[{"j", "=", "1"}]}], "n"], 
        RowBox[{
         SubscriptBox["\[Eta]", "i"], 
         SubscriptBox["A", 
          RowBox[{"i", " ", "j"}]], 
         SubscriptBox["\[Eta]", "j"]}]}]}], ")"}]}]}]}], 
  TraditionalForm]], "Equation",
