Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[Eta]", 
        RowBox[{"a", "(", "1", ")"}]], "..."}], 
      SubscriptBox["\[Eta]", 
       RowBox[{"a", "(", "m", ")"}]]}], "\[RightAngleBracket]"}], 
    "\[Congruent]", 
    FractionBox[
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"\[Integral]", 
          RowBox[{"\[DifferentialD]", 
           SubscriptBox["\[Eta]", "n"]}]}], "..."}], 
        RowBox[{"\[DifferentialD]", 
         SubscriptBox["\[Eta]", "1"]}], " ", 
        SubscriptBox["\[Eta]", 
         RowBox[{"a", "(", "1", ")"}]]}], "..."}], 
      SubscriptBox["\[Eta]", 
       RowBox[{"a", "(", "m", ")"}]], 
      RowBox[{"exp", "(", 
       RowBox[{
        RowBox[{"-", 
         FractionBox["1", "2"]}], 
        SubscriptBox["\[Eta]", "i"], 
        SubscriptBox["A", 
         RowBox[{"i", " ", "j"}]], 
        SubscriptBox["\[Eta]", "j"]}], ")"}]}], 
     RowBox[{
      RowBox[{
       RowBox[{"\[Integral]", 
        RowBox[{"\[DifferentialD]", 
         SubscriptBox["\[Eta]", "n"]}]}], "..."}], 
      RowBox[{"\[DifferentialD]", 
       SubscriptBox["\[Eta]", "1"]}], " ", 
      RowBox[{"exp", "(", 
       RowBox[{
        RowBox[{"-", 
         FractionBox["1", "2"]}], 
        SubscriptBox["\[Eta]", "i"], 
        SubscriptBox["A", 
         RowBox[{"i", " ", "j"}]], 
        SubscriptBox["\[Eta]", "j"]}], ")"}]}]]}], "=", 
   SubscriptBox[
    RowBox[{"[", 
     RowBox[{
      RowBox[{
       FractionBox["\[PartialD]", 
        RowBox[{"\[PartialD]", 
         SubscriptBox["\[Xi]", 
          RowBox[{"a", "(", "m", ")"}]]}]], "..."}], 
      RowBox[{
       FractionBox["\[PartialD]", 
        RowBox[{"\[PartialD]", 
         SubscriptBox["\[Xi]", 
          RowBox[{"a", "(", "1", ")"}]]}]], 
       RowBox[{"\[ScriptCapitalZ]", "(", "\[Xi]", ")"}]}]}], "]"}], 
    RowBox[{"\[Xi]", "=", "0"}]]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.786499567220491*^9, 3.786499576391224*^9}, {
