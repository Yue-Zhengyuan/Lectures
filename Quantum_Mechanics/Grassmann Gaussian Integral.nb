      RowBox[{
       SubscriptBox["\[Eta]", "j"], "  ", "\[DoubleLongRightArrow]", "  ", 
       RowBox[{"Z", "(", "0", ")"}]}]}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Integral]", 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[Eta]", "n"]}]}], "..."}], 
     RowBox[{"\[DifferentialD]", 
      SubscriptBox["\[Eta]", "1"]}], 
     RowBox[{"exp", "(", 
      RowBox[{"-", 
       RowBox[{
        UnderscriptBox["\[Sum]", 
         RowBox[{"i", "<", "j"}]], " ", 
        RowBox[{
         SubscriptBox["\[Eta]", "i"], 
         SubscriptBox["A", 
          RowBox[{"i", " ", "j"}]], 
         SubscriptBox["\[Eta]", "j"]}]}]}], ")"}]}]}]}], 
  TraditionalForm]], "Equation",
