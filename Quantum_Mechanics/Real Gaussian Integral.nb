Cell[BoxData[{
 FormBox[
  RowBox[{
   RowBox[{"Z", "(", 
    RowBox[{
     OverscriptBox["J", "_"], ",", "J"}], ")"}], "=", 
   RowBox[{
    FractionBox["1", "\[Pi]"], 
    RowBox[{"\[Integral]", 
     RowBox[{
      RowBox[{"\[DifferentialD]", "x"}], 
      RowBox[{"\[DifferentialD]", "y"}], " ", 
      RowBox[{"exp", "(", 
       RowBox[{
        RowBox[{
         RowBox[{"-", "a"}], " ", 
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox["x", "2"], "+", 
           SuperscriptBox["y", "2"]}], ")"}]}], "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"x", "-", 
           RowBox[{"\[ImaginaryI]", " ", "y"}]}], ")"}], "J"}], "+", 
        RowBox[{
         OverscriptBox["J", "_"], " ", 
         RowBox[{"(", 
          RowBox[{"x", "+", 
           RowBox[{"\[ImaginaryI]", " ", "y"}]}], ")"}]}]}], ")"}]}]}]}]}], 
  TraditionalForm], "\[LineSeparator]", 
 FormBox[
