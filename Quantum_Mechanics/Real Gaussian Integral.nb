      RowBox[{"\[DifferentialD]", "x"}], " ", 
      RowBox[{"exp", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"-", "a"}], " ", 
         SuperscriptBox["x", "2"]}], "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"J", "+", 
           OverscriptBox["J", "_"]}], ")"}], "x"}]}], "]"}], " ", 
      RowBox[{"\[Integral]", 
       RowBox[{
        RowBox[{"\[DifferentialD]", "y"}], " ", 
        RowBox[{"exp", "[", 
         RowBox[{
          RowBox[{
