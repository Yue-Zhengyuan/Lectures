 CellChangeTimes->{{3.7863574483173103`*^9, 3.7863574672644978`*^9}, {
  3.7863575464683037`*^9, 3.7863575904421787`*^9}, {3.786359324290818*^9, 
  3.786359329916162*^9}, {3.786361101573393*^9, 3.786361101579097*^9}, {
  3.7871818289883833`*^9, 3.787181845291544*^9}, {3.801512866678783*^9, 
  3.801512914983914*^9}},ExpressionUUID->"14778555-e2d2-49fa-bc43-\
040df372c1e5"],

Cell[TextData[{
 "They are called ",
 StyleBox["Grassmann numbers",
  FontWeight->"Bold"],
 ". "
}], "Text",
 CellChangeTimes->{{3.7863574483173103`*^9, 3.7863574672644978`*^9}, {
  3.7863575464683037`*^9, 3.7863575904421787`*^9}, {3.786359324290818*^9, 
  3.786359329916162*^9}, {3.786361101573393*^9, 3.786361101579097*^9}, {
  3.7871818289883833`*^9, 3.787181845291544*^9}, {3.801512866678783*^9, 
  3.801512868243246*^9}},ExpressionUUID->"9409fbd3-d055-4eeb-87bd-\
fc0edf25d5c3"],

Cell[TextData[{
 "For obvious reasons, we also require ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["c", "i"], ",", 
      SubscriptBox["\[Eta]", "j"]}], "}"}], "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "8c76b5b9-65ca-41b7-96a8-5c3a18f47687"],
 " for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "\[NotEqual]", "j"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "9fcb2001-61b4-437b-bbed-fa9ed57966bc"],
 ". "
}], "Text",
 CellChangeTimes->{{3.7863574483173103`*^9, 3.7863574672644978`*^9}, {
   3.7863575464683037`*^9, 3.7863575904421787`*^9}, {3.786359324290818*^9, 
