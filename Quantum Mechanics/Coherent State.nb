Cell[CellGroupData[{

Cell["Building Coherent State from Vacuum", "Subsection",
 CellChangeTimes->{{3.7863579779018*^9, 
  3.786357986914486*^9}},ExpressionUUID->"34423066-f7d1-46ae-abab-\
ec37a96c2694"],

Cell[CellGroupData[{

Cell["One Kind of Fermions", "Subsubsection",
 CellChangeTimes->{{3.7864150892825317`*^9, 
  3.786415094485347*^9}},ExpressionUUID->"e82a6f71-3f15-4293-9029-\
76eeaa444d90"],

Cell["\<\
First consider the case of only one kind of fermions. To build the coherent \
state from vacuum, we again examine the expansion\
\>", "Text",
 CellChangeTimes->{{3.786358245963118*^9, 3.786358265110273*^9}, {
  3.786415050399817*^9, 
  3.786415063331984*^9}},ExpressionUUID->"b2d550f9-3d5c-42ad-9446-\
9b6e647111ad"],

Cell[BoxData[
 FormBox[
  RowBox[{
   TemplateBox[{RowBox[{" ", "\[Eta]"}]},
    "Ket"], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["c", "0"], 
     TemplateBox[{RowBox[{" ", "0"}]},
      "Ket"]}], "+", 
    RowBox[{
     SubscriptBox["c", "1"], 
     TemplateBox[{RowBox[{" ", "1"}]},
      "Ket"]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.786358245963118*^9, 
  3.786358309277561*^9}},ExpressionUUID->"5ef77aa6-1ee5-4bdf-9a37-\
21e2b2687a08"],

Cell[TextData[{
 "We point out that the coefficients here are ",
 StyleBox["Grassmann numbers. ",
  FontSlant->"Italic"],
 "Then"
}], "Text",
 CellChangeTimes->{{3.786358383460615*^9, 3.7863583978673887`*^9}, {
   3.786358562880221*^9, 3.786358604053318*^9}, {3.786358680705702*^9, 
   3.786358687074238*^9}, {3.786359339632444*^9, 3.786359343204237*^9}, {
   3.786360229275881*^9, 3.7863602341487837`*^9}, 3.786360267199093*^9, {
   3.786360417246472*^9, 3.786360448501156*^9}, {3.7863615490303097`*^9, 
   3.786361549167715*^9}},ExpressionUUID->"02a559f1-ba22-4b20-a35f-\
beda21cf666a"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"c", 
    TemplateBox[{RowBox[{" ", "\[Eta]"}]},
     "Ket"]}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"c", " ", 
      SubscriptBox["c", "0"], 
      TemplateBox[{RowBox[{" ", "0"}]},
       "Ket"]}], "+", 
     RowBox[{"c", " ", 
      SubscriptBox["c", "1"], 
      TemplateBox[{RowBox[{" ", "1"}]},
       "Ket"]}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        SubscriptBox["c", "0"]}], "c", 
       TemplateBox[{RowBox[{" ", "0"}]},
        "Ket"]}], "-", 
      RowBox[{
       SubscriptBox["c", "1"], "c", 
       TemplateBox[{RowBox[{" ", "1"}]},
        "Ket"]}]}], " ", "=", 
     RowBox[{
      RowBox[{"-", 
       SubscriptBox["c", "1"]}], 
      TemplateBox[{RowBox[{" ", "0"}]},
       "Ket"]}]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.786358245963118*^9, 3.786358358534614*^9}, {
  3.786358399989771*^9, 3.7863584126266623`*^9}, {3.78635850025797*^9, 
  3.786358529220757*^9}, {3.786358609639015*^9, 3.7863586230975323`*^9}, {
  3.786358693951726*^9, 3.786358699811376*^9}, {3.786360278734652*^9, 
  3.786360332387453*^9}, {3.786360451601261*^9, 
  3.78636045585776*^9}},ExpressionUUID->"0c2e8922-bc9c-4753-a09d-\
705070ec8848"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[Eta]", 
    TemplateBox[{RowBox[{" ", "\[Eta]"}]},
     "Ket"]}], "=", 
   RowBox[{
    RowBox[{"\[Eta]", 
     TemplateBox[{RowBox[{" ", "0"}]},
      "Ket"]}], "+", 
    RowBox[{"\[Eta]", " ", 
     SubscriptBox["c", "1"], 
     TemplateBox[{RowBox[{" ", "1"}]},
      "Ket"]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.786358245963118*^9, 3.786358358534614*^9}, {
   3.786358399989771*^9, 3.7863584126266623`*^9}, {3.78635850025797*^9, 
   3.786358529220757*^9}, {3.786358609639015*^9, 3.7863586447676086`*^9}, 
   3.786360345557329*^9},ExpressionUUID->"430b6a4c-1eff-4e4c-a84d-\
2f8c2570ec7e"],

Cell["Now we conclude that", "Text",
 CellChangeTimes->{{3.7863585321638002`*^9, 3.78635855314459*^9}, {
  3.7863603500805063`*^9, 
  3.786360354474937*^9}},ExpressionUUID->"5694a160-1c13-4f29-b77c-\
bc2c21ca68ad"],

Cell[BoxData[
 FormBox[
  RowBox[{" ", 
   RowBox[{
    SubscriptBox["c", "1"], "=", 
    RowBox[{"-", "\[Eta]"}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.786358245963118*^9, 3.786358358534614*^9}, {
   3.786358399989771*^9, 3.7863584126266623`*^9}, {3.78635850025797*^9, 
   3.7863585292190247`*^9}, 
   3.786360458662971*^9},ExpressionUUID->"b15c0cad-86e1-45da-9bb3-\
281076fcc2fd"],

Cell[TextData[{
 "Again, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", "0"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "6a53aa3b-9f1c-406d-a789-b4faa401d014"],
 " is just a normalization and we can set it to be 1. "
}], "Text",
 CellChangeTimes->{{3.786357988200087*^9, 3.786358053375601*^9}, {
  3.786360367246614*^9, 3.786360391290381*^9}, {3.786360545181435*^9, 
  3.786360571082819*^9}},ExpressionUUID->"edffd5f3-6e2c-48c1-8ef3-\
6665a13f8436"],

Cell[TextData[{
 "What about the ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Eta]", " ", 
    SubscriptBox["c", "1"], 
    TemplateBox[{RowBox[{" ", "1"}]},
     "Ket"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a153f27b-21d4-4988-81cc-f4782d9188b8"],
 " term? Luckily, because of the anti-commutativity, any Grassmann number \
raised to power greater than 2 gives 0:"
}], "Text",
 CellChangeTimes->{{3.786357988200087*^9, 3.786358053375601*^9}, {
  3.786360367246614*^9, 3.786360391290381*^9}, {3.7863605731851797`*^9, 
  3.786360577925942*^9}},ExpressionUUID->"7981d1d4-445d-4d05-a308-\
6db0a2880b66"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox["\[Eta]", "n"], "=", 
   RowBox[{
    RowBox[{"0", "        ", "for", " ", "all", " ", "n"}], "\[GreaterEqual]",
     "2"}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.786357988200087*^9, 
  3.78635810329921*^9}},ExpressionUUID->"24b68d16-9c7d-4f82-af1c-\
32be7216d6f3"],

Cell[TextData[{
 "Then ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Eta]", " ", 
     SubscriptBox["c", "1"], 
     TemplateBox[{
       RowBox[{" ", "1"}]},
      "Ket"]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       SuperscriptBox["\[Eta]", "2"]}], 
      TemplateBox[{
        RowBox[{" ", "1"}]},
       "Ket"]}], "=", "0"}]}], TraditionalForm]],ExpressionUUID->
  "5a242c73-b8dd-4c47-ab52-73bd5d6a2cf4"],
 ", which automatically vanishes. Therefore"
}], "Text",
 CellChangeTimes->{{3.786360476281247*^9, 3.786360515347509*^9}, {
  3.786360582228834*^9, 3.786360595390871*^9}, {3.8031649705362062`*^9, 
  3.80316497561381*^9}},ExpressionUUID->"4b11f78c-6c9a-44d6-adb6-\
3111a461d5bb"],

Cell[BoxData[
 FormBox[
  RowBox[{
   TemplateBox[{RowBox[{" ", "\[Eta]"}]},
    "Ket"], "=", 
   RowBox[{
    RowBox[{
     TemplateBox[{RowBox[{" ", "0"}]},
      "Ket"], "-", 
     RowBox[{"\[Eta]", 
      TemplateBox[{RowBox[{" ", "1"}]},
       "Ket"]}]}], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{"\[Eta]", " ", 
        SuperscriptBox["c", "\[Dagger]"]}]}], ")"}], 
     TemplateBox[{RowBox[{" ", "0"}]},
      "Ket"]}]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.786360476281247*^9, 3.786360515347509*^9}, {
  3.786360582228834*^9, 
  3.7863606648514357`*^9}},ExpressionUUID->"751f6c5b-6dab-4977-884f-\
b661f99eb04f"],

Cell["The \[OpenCurlyDoubleQuote]Hermitian conjugate\[CloseCurlyDoubleQuote] \
of this state is", "Text",
 CellChangeTimes->{{3.786415122960938*^9, 3.786415132022355*^9}, {
  3.786415411462125*^9, 
  3.7864154114623823`*^9}},ExpressionUUID->"3f2c3f8b-a8f0-4dec-b15e-\
0c95cb1b1f2a"],

Cell[BoxData[
 FormBox[
  RowBox[{
   TemplateBox[{"\[Eta]"},
    "Bra"], "=", 
   RowBox[{
    RowBox[{
     TemplateBox[{"0"},
      "Bra"], " ", 
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{"c", " ", 
        OverscriptBox["\[Eta]", "_"]}]}], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      TemplateBox[{"0"},
       "Bra"], " ", 
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{
         OverscriptBox["\[Eta]", "_"], " ", "c"}]}], ")"}]}], "=", 
     RowBox[{
      RowBox[{
       TemplateBox[{"0"},
        "Bra"], "+", 
       RowBox[{
        OverscriptBox["\[Eta]", "_"], 
        TemplateBox[{"0"},
         "Bra"], "c"}]}], "=", 
      RowBox[{
       TemplateBox[{"0"},
        "Bra"], "+", 
       RowBox[{
        OverscriptBox["\[Eta]", "_"], 
        TemplateBox[{"1"},
         "Bra"]}]}]}]}]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.786415122960938*^9, 3.786415132022355*^9}, {
  3.7864154128689127`*^9, 3.7864155099385023`*^9}, {3.7864155514021873`*^9, 
  3.786415553966075*^9}},ExpressionUUID->"095ac3ac-b350-4431-b04d-\
2346d286c6b9"],

Cell[TextData[{
 StyleBox["Remark",
  FontSlant->"Italic"],
 ": Because the exponential function simplifies greatly in the fermion case, \
