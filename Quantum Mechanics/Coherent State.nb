Cell[BoxData[{
 FormBox[
  RowBox[{
   RowBox[{"\[LeftAngleBracket]", 
    RowBox[{"f", "|", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "\[ImaginaryI]"}], " ", "H", " ", 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["c", "\[Dagger]"], ",", "c"}], ")"}], "T"}]], "|", 
     "i"}], "\[RightAngleBracket]"}], "=", 
   RowBox[{"\[Integral]", " ", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       UnderoverscriptBox["\[Product]", 
        RowBox[{"k", "=", "0"}], "N"], " ", 
       RowBox[{
        RowBox[{"\[DifferentialD]", 
         SubscriptBox[
          OverscriptBox["\[Eta]", "_"], "k"]}], 
        RowBox[{"\[DifferentialD]", 
         SubscriptBox["\[Eta]", "k"]}]}]}], ")"}], " ", 
     RowBox[{"exp", "(", 
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", "2"]}], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox[
           OverscriptBox["\[Eta]", "_"], "0"], 
          SubscriptBox["\[Eta]", "0"]}], "+", 
         RowBox[{
          SubscriptBox[
           OverscriptBox["\[Eta]", "_"], "N"], 
          SubscriptBox["\[Eta]", "N"]}]}], ")"}]}], ")"}], 
     RowBox[{"(", 
      RowBox[{
       UnderoverscriptBox["\[Product]", 
        RowBox[{"k", "=", "0"}], 
        RowBox[{"N", "-", "1"}]], 
       RowBox[{"exp", "[", 
        RowBox[{
         RowBox[{
          FractionBox["1", "2"], " ", 
          RowBox[{"(", 
           RowBox[{
            SubscriptBox["\[PartialD]", "t"], 
            SubscriptBox[
             OverscriptBox["\[Eta]", "_"], "k"]}], ")"}], 
          SubscriptBox["\[Eta]", "k"], "\[Epsilon]"}], "-", 
         RowBox[{
          FractionBox["1", "2"], 
          SubscriptBox[
           OverscriptBox["\[Eta]", "_"], 
           RowBox[{"k", "+", "1"}]], " ", 
          RowBox[{"(", 
           RowBox[{
            SubscriptBox["\[PartialD]", "t"], 
            SubscriptBox["\[Eta]", 
             RowBox[{"k", "+", "1"}]]}], ")"}], "\[Epsilon]"}], "-", 
         RowBox[{"\[ImaginaryI]", " ", 
          RowBox[{"H", "(", 
           RowBox[{
            SubscriptBox[
             OverscriptBox["\[Eta]", "_"], 
             RowBox[{"k", "+", "1"}]], ",", 
            SubscriptBox["\[Eta]", "k"]}], ")"}], "\[Epsilon]"}]}], "]"}]}], 
      ")"}], 
     TemplateBox[{"f",RowBox[{" ", 
         SubscriptBox["\[Eta]", "N"]}]},
      "BraKet"], 
     TemplateBox[{SubscriptBox["\[Eta]", "0"],RowBox[{" ", "i"}]},
      "BraKet"]}]}]}], TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{"\[Integral]", 
    RowBox[{
     RowBox[{
      RowBox[{"[", 
       RowBox[{"\[DifferentialD]", 
        OverscriptBox["\[Eta]", "_"]}], "]"}], "[", 
      RowBox[{"\[DifferentialD]", "\[Eta]"}], "]"}], 
     RowBox[{"exp", "(", 
      RowBox[{"\[ImaginaryI]", 
       RowBox[{
        SubsuperscriptBox["\[Integral]", "0", "T"], 
        RowBox[{"\[DifferentialD]", 
         RowBox[{"t", " ", "[", 
          RowBox[{
           RowBox[{
            FractionBox["1", 
             RowBox[{"2", "\[ImaginaryI]"}]], 
            RowBox[{"(", 
             RowBox[{
              RowBox[{
               RowBox[{"(", 
                RowBox[{
                 SubscriptBox["\[PartialD]", "t"], 
                 OverscriptBox["\[Eta]", "_"]}], ")"}], "\[Eta]"}], "-", 
              RowBox[{
               OverscriptBox["\[Eta]", "_"], 
               RowBox[{
                SubscriptBox["\[PartialD]", "t"], "\[Eta]"}]}]}], ")"}]}], 
           "-", 
           RowBox[{"H", "(", 
            RowBox[{
             OverscriptBox["\[Eta]", "_"], ",", "\[Eta]"}], ")"}]}], 
          "]"}]}]}]}], ")"}], 
     UnderscriptBox[
      UnderscriptBox[
       RowBox[{"exp", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", 
           FractionBox["1", "2"]}], 
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             SubscriptBox[
              OverscriptBox["\[Eta]", "_"], "i"], 
             SubscriptBox["\[Eta]", "i"]}], "+", 
            RowBox[{
             SubscriptBox[
              OverscriptBox["\[Eta]", "_"], "f"], 
             SubscriptBox["\[Eta]", "f"]}]}], ")"}]}], ")"}], 
        TemplateBox[{"f",RowBox[{" ", 
            SubscriptBox["\[Eta]", "f"]}]},
         "BraKet"], 
        TemplateBox[{SubscriptBox["\[Eta]", "i"],RowBox[{" ", "i"}]},
         "BraKet"]}], "\[UnderBrace]"], 
      RowBox[{"the", " ", "\"\<projection operator\>\""}]]}]}]}], 
  TraditionalForm]}], "Equation",
 CellChangeTimes->{{3.7864067212275667`*^9, 3.7864067861825323`*^9}, {
   3.78640682141462*^9, 3.7864069291054773`*^9}, 3.7864069623042088`*^9, {
