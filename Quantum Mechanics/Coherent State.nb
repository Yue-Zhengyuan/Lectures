         RowBox[{
          RowBox[{"-", "\[ImaginaryI]"}], " ", "H", " ", "\[Epsilon]"}]], "|", 
        SubscriptBox["q", 
         RowBox[{"N", "-", "1"}]]}], "\[RightAngleBracket]"}]}], "..."}], 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      SubscriptBox["q", "1"], "|", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], " ", "H", " ", "\[Epsilon]"}]], "|", 
      SubscriptBox["q", "0"]}], "\[RightAngleBracket]"}]}]}], 
  TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{"\[Integral]", " ", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       UnderoverscriptBox["\[Product]", 
        RowBox[{"k", "=", "1"}], 
        RowBox[{"N", "-", "1"}]], " ", 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["q", "k"]}]}], ")"}], 
     RowBox[{"(", 
      RowBox[{
       UnderoverscriptBox["\[Product]", 
        RowBox[{"j", "=", "0"}], 
