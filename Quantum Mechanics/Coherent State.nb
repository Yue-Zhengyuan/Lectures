         RowBox[{"-", "\[ImaginaryI]"}], " ", "H", " ", 
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox["c", "\[Dagger]"], ",", "c"}], ")"}], 
         "\[Epsilon]"}]], "|", 
       SubscriptBox["\[Eta]", 
        RowBox[{"N", "-", "1"}]]}], "\[RightAngleBracket]"}], 
     RowBox[{"(", 
      RowBox[{
       UnderoverscriptBox["\[Product]", 
        RowBox[{"k", "=", "1"}], 
        RowBox[{"N", "-", "2"}]], 
       RowBox[{"\[LeftAngleBracket]", 
        RowBox[{
         SubscriptBox["\[Eta]", 
          RowBox[{"k", "+", "1"}]], "|", 
         SuperscriptBox["\[ExponentialE]", 
          RowBox[{
           RowBox[{"-", "\[ImaginaryI]"}], " ", "H", " ", 
           RowBox[{"(", 
            RowBox[{
             SuperscriptBox["c", "\[Dagger]"], ",", "c"}], ")"}], 
           "\[Epsilon]"}]], "|", 
         SubscriptBox["c", "k"]}], "\[RightAngleBracket]"}]}], ")"}], 
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{
       SubscriptBox["\[Eta]", "1"], "|", 
       SuperscriptBox["\[ExponentialE]", 
