Cell[BoxData[
 FormBox[
  RowBox[{"LHS", "=", 
   RowBox[{
    RowBox[{"\[Integral]", " ", 
     RowBox[{
      UnderscriptBox["\[Product]", 
       RowBox[{"k", ",", " ", "i", ",", " ", "j"}]], " ", 
