Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[LeftAngleBracket]", 
    RowBox[{"f", "|", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "\[ImaginaryI]"}], " ", "H", " ", 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["a", "\[Dagger]"], ",", "a"}], ")"}], "T"}]], "|", 
     "i"}], "\[RightAngleBracket]"}], "=", 
   RowBox[{"\[Integral]", " ", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       UnderoverscriptBox["\[Product]", 
        RowBox[{"k", "=", "0"}], "N"], " ", 
       FractionBox[
        RowBox[{
         RowBox[{"\[DifferentialD]", 
          SubscriptBox[
           OverscriptBox["\[Alpha]", "_"], "k"]}], 
         RowBox[{"\[DifferentialD]", 
          SubscriptBox["\[Alpha]", "k"]}]}], 
        RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]"}]]}], ")"}], " ", 
     RowBox[{"exp", "(", 
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", "2"]}], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox[
           OverscriptBox["\[Alpha]", "_"], "0"], 
          SubscriptBox["\[Alpha]", "0"]}], "+", 
         RowBox[{
          SubscriptBox[
           OverscriptBox["\[Alpha]", "_"], "N"], 
          SubscriptBox["\[Alpha]", "N"]}]}], ")"}]}], ")"}], 
     RowBox[{"(", 
      RowBox[{
       UnderoverscriptBox["\[Product]", 
        RowBox[{"k", "=", "0"}], 
        RowBox[{"N", "-", "1"}]], 
       RowBox[{"exp", "[", 
        RowBox[{
         RowBox[{
          FractionBox["1", "2"], 
          RowBox[{"(", 
           RowBox[{
            SubscriptBox["\[PartialD]", "t"], 
            SubscriptBox[
             OverscriptBox["\[Alpha]", "_"], "k"]}], ")"}], 
          SubscriptBox["\[Alpha]", "k"], "\[Epsilon]"}], "-", 
         RowBox[{
          FractionBox["1", "2"], 
          SubscriptBox[
           OverscriptBox["\[Alpha]", "_"], 
           RowBox[{"k", "+", "1"}]], " ", 
          RowBox[{"(", 
           RowBox[{
            SubscriptBox["\[PartialD]", "t"], 
            SubscriptBox["\[Alpha]", 
             RowBox[{"k", "+", "1"}]]}], ")"}], "\[Epsilon]"}], "-", 
         RowBox[{"\[ImaginaryI]", " ", 
          RowBox[{"H", "(", 
           RowBox[{
            SubscriptBox[
             OverscriptBox["\[Alpha]", "_"], 
             RowBox[{"k", "+", "1"}]], ",", 
            SubscriptBox["\[Alpha]", "k"]}], ")"}], "\[Epsilon]"}]}], "]"}]}],
       ")"}], 
     TemplateBox[{"f",RowBox[{" ", 
         SubscriptBox["\[Alpha]", "N"]}]},
