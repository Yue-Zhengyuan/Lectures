2dbdbcee5ba7"],

Cell[BoxData[{
 FormBox[
  RowBox[{
   RowBox[{"\[DoubleLongRightArrow]", " ", "LHS"}], "=", 
   RowBox[{
    RowBox[{"\[Integral]", " ", 
     RowBox[{
      UnderscriptBox["\[Product]", "k"], " ", 
      RowBox[{
       RowBox[{"\[DifferentialD]", 
        SubscriptBox[
         OverscriptBox["\[Eta]", "_"], "k"]}], 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[Eta]", "k"]}], " ", 
       RowBox[{"(", 
        RowBox[{"1", "-", 
         RowBox[{
          SubscriptBox[
           OverscriptBox["\[Eta]", "_"], "k"], 
          SubscriptBox["\[Eta]", "k"]}]}], ")"}], " ", 
       TemplateBox[{RowBox[{" ", "\[Eta]"}]},
        "Ket"], 
       TemplateBox[{"\[Eta]"},
        "Bra"]}]}]}], "=", 
    RowBox[{"\[Integral]", " ", 
     RowBox[{
      UnderscriptBox["\[Product]", "k"], " ", 
      RowBox[{
       RowBox[{"\[DifferentialD]", 
        SubscriptBox[
         OverscriptBox["\[Eta]", "_"], "k"]}], 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[Eta]", "k"]}], " ", 
       RowBox[{"(", 
        RowBox[{"1", "-", 
         RowBox[{
          SubscriptBox[
           OverscriptBox["\[Eta]", "_"], "k"], 
          SubscriptBox["\[Eta]", "k"]}]}], ")"}], " ", 
       RowBox[{"(", 
        RowBox[{
         TemplateBox[{RowBox[{" ", "0"}]},
          "Ket"], "-", 
         RowBox[{
          SubscriptBox["\[Eta]", "k"], 
          TemplateBox[{RowBox[{" ", "1"}]},
           "Ket"]}]}], ")"}], 
       RowBox[{"(", 
        RowBox[{
         TemplateBox[{"0"},
          "Bra"], "-", 
         RowBox[{
          TemplateBox[{"1"},
           "Bra"], 
          SubscriptBox[
           OverscriptBox["\[Eta]", "_"], "k"]}]}], ")"}]}]}]}]}]}], 
  TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"\t", 
   RowBox[{"=", 
    RowBox[{"\[Integral]", " ", 
     RowBox[{
      UnderscriptBox["\[Product]", 
       RowBox[{"k", ",", " ", "i", ",", " ", "j"}]], " ", 
      RowBox[{
       RowBox[{"\[DifferentialD]", 
        SubscriptBox[
         OverscriptBox["\[Eta]", "_"], "k"]}], 
       RowBox[{"\[DifferentialD]", 
        RowBox[{
         SubscriptBox["\[Eta]", "k"], " ", "[", " ", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"1", "+", 
             RowBox[{
              SubscriptBox["\[Eta]", "k"], 
              SubscriptBox[
               OverscriptBox["\[Eta]", "_"], "k"]}]}], ")"}], " ", 
           TemplateBox[{RowBox[{" ", "0"}]},
            "Ket"], 
           TemplateBox[{"0"},
            "Bra"]}], "-", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"1", "+", 
             RowBox[{
              SubscriptBox["\[Eta]", "k"], 
              SubscriptBox[
               OverscriptBox["\[Eta]", "_"], "k"]}]}], ")"}], 
           SubscriptBox["\[Eta]", "i"], 
           TemplateBox[{RowBox[{" ", "1"}]},
            "Ket"], 
           TemplateBox[{"0"},
            "Bra"]}], "-", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"1", "+", 
             RowBox[{
              SubscriptBox["\[Eta]", "k"], 
              SubscriptBox[
               OverscriptBox["\[Eta]", "_"], "k"]}]}], ")"}], 
           SubscriptBox[
            OverscriptBox["\[Eta]", "_"], "k"], 
           TemplateBox[{RowBox[{" ", "0"}]},
            "Ket"], 
           TemplateBox[{"1"},
            "Bra"]}], "+", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"1", "+", 
             RowBox[{
              SubscriptBox["\[Eta]", "k"], 
              SubscriptBox[
               OverscriptBox["\[Eta]", "_"], "k"]}]}], ")"}], 
           SubscriptBox["\[Eta]", "k"], 
           SubscriptBox[
            OverscriptBox["\[Eta]", "_"], "k"], 
           TemplateBox[{RowBox[{" ", "1"}]},
            "Ket"], 
           TemplateBox[{"1"},
            "Bra"]}]}], " ", "]"}]}]}]}]}]}]}], TraditionalForm]}], "Equation",\

 CellChangeTimes->{{3.7863635835049553`*^9, 3.786363584963876*^9}, {
   3.78636362717819*^9, 3.786363656446331*^9}, {3.7863637101500177`*^9, 
