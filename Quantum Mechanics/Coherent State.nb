a7b087cc53bd"],

Cell[BoxData[
 FormBox[
  RowBox[{
   TemplateBox[{RowBox[{" ", "\[Eta]"}]},
    "Ket"], "=", 
   RowBox[{
    RowBox[{
     UnderscriptBox["\[Product]", "k"], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", 
        RowBox[{
         SubscriptBox["\[Eta]", "k"], 
