Cell[BoxData[{
 FormBox[
  RowBox[{
   RowBox[{"\[LeftAngleBracket]", 
    RowBox[{
     SubscriptBox["q", "f"], "|", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "\[ImaginaryI]"}], " ", 
       RowBox[{"H", "(", 
        RowBox[{"p", ",", "q"}], ")"}], " ", "T"}]], "|", 
     SubscriptBox["q", "i"]}], "\[RightAngleBracket]"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"\[Integral]", 
        RowBox[{"\[DifferentialD]", 
         SubscriptBox["q", 
          RowBox[{"N", "-", "1"}]]}]}], "..."}], 
      RowBox[{"\[DifferentialD]", 
       SubscriptBox["q", "1"]}], 
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{
        SubscriptBox["q", "N"], "|", 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{
          RowBox[{"-", "\[ImaginaryI]"}], " ", "H", " ", "\[Epsilon]"}]], "|", 
        SubscriptBox["q", 
         RowBox[{"N", "-", "1"}]]}], "\[RightAngleBracket]"}], 
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{
