Cell[BoxData[
 FormBox[
  RowBox[{
   TemplateBox[{RowBox[{" ", "\[Alpha]"}]},
    "Ket"], "=", 
   RowBox[{
    RowBox[{
     UnderscriptBox["\[Sum]", 
      RowBox[{"n", "\[GreaterEqual]", "0"}]], " ", 
     RowBox[{
      SubscriptBox["c", "n"], 
      RowBox[{
       TemplateBox[{RowBox[{" ", "n"}]},
        "Ket"], " ", "\[DoubleLongRightArrow]", "a"}], 
      TemplateBox[{RowBox[{" ", "\[Alpha]"}]},
       "Ket"]}]}], "=", 
    RowBox[{
     RowBox[{
      UnderscriptBox["\[Sum]", 
       RowBox[{"n", "\[GreaterEqual]", "0"}]], " ", 
      RowBox[{
       SubscriptBox["c", "n"], "(", 
       RowBox[{"a", 
        TemplateBox[{RowBox[{" ", "n"}]},
         "Ket"]}], ")"}]}], "=", 
     RowBox[{
      RowBox[{
       UnderscriptBox["\[Sum]", 
        RowBox[{"n", "\[GreaterEqual]", "1"}]], " ", 
       RowBox[{
        SubscriptBox["c", "n"], 
        SqrtBox["n"], 
        TemplateBox[{RowBox[{" ", 
            RowBox[{"n", "-", "1"}]}]},
         "Ket"]}]}], "=", 
      RowBox[{
       UnderscriptBox["\[Sum]", 
        RowBox[{"n", "\[GreaterEqual]", "0"}]], " ", 
       RowBox[{
        SubscriptBox["c", 
         RowBox[{"n", "+", "1"}]], 
        SqrtBox[
         RowBox[{"n", "+", "1"}]], 
        TemplateBox[{RowBox[{" ", "n"}]},
         "Ket"]}]}]}]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.786353141443932*^9, 3.7863532431750937`*^9}, {
