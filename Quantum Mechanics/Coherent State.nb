Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["\[Eta]", "k"], 
      StyleBox[
       SubsuperscriptBox["c", "k", "\[Dagger]"],
       FontColor->RGBColor[1, 0, 0]]}], ")"}], 
    RowBox[{"(", 
     RowBox[{
      StyleBox[
       SubscriptBox["\[Eta]", "l"],
       FontColor->RGBColor[1, 0, 0]], 
      SubsuperscriptBox["c", "l", "\[Dagger]"]}], ")"}]}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"-", 
      SubscriptBox["\[Eta]", "k"]}], 
     SubscriptBox["\[Eta]", "l"], 
     StyleBox[
      SubsuperscriptBox["c", "k", "\[Dagger]"],
      FontColor->RGBColor[1, 0, 0]], 
     StyleBox[
      SubsuperscriptBox["c", "l", "\[Dagger]"],
      FontColor->RGBColor[1, 0, 0]]}], "=", 
    RowBox[{
     RowBox[{
      StyleBox[
       SubscriptBox["\[Eta]", "l"],
       FontColor->RGBColor[1, 0, 0]], 
      StyleBox[
       SubscriptBox["\[Eta]", "k"],
       FontColor->RGBColor[1, 0, 0]], 
      SubsuperscriptBox["c", "k", "\[Dagger]"], 
      SubsuperscriptBox["c", "l", "\[Dagger]"]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        SubscriptBox["\[Eta]", "l"]}], 
       StyleBox[
        SubscriptBox["\[Eta]", "k"],
        FontColor->RGBColor[1, 0, 0]], 
       StyleBox[
        SubsuperscriptBox["c", "l", "\[Dagger]"],
        FontColor->RGBColor[1, 0, 0]], 
       SubsuperscriptBox["c", "k", "\[Dagger]"]}], "=", 
      RowBox[{
       RowBox[{
        SubscriptBox["\[Eta]", "l"], 
        SubsuperscriptBox["c", "l", "\[Dagger]"], 
        SubscriptBox["\[Eta]", "k"], 
        SubsuperscriptBox["c", "k", "\[Dagger]"]}], "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["\[Eta]", "l"], 
          SubsuperscriptBox["c", "l", "\[Dagger]"]}], ")"}], 
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["\[Eta]", "k"], 
          SubsuperscriptBox["c", "k", "\[Dagger]"]}], ")"}]}]}]}]}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.786362044536686*^9, 3.7863621073276653`*^9}, {
