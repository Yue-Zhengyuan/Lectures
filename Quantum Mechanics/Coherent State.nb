      RowBox[{"(", 
       RowBox[{"1", "-", 
        RowBox[{
         SubscriptBox["\[Eta]", "k"], 
         SubsuperscriptBox["c", "k", "\[Dagger]"]}]}], ")"}], 
      TemplateBox[{RowBox[{" ", "0"}]},
       "Ket"]}]}], "=", 
    RowBox[{
     RowBox[{
      UnderscriptBox["\[Product]", "k"], 
      RowBox[{"(", 
       RowBox[{
        TemplateBox[{RowBox[{" ", 
            SubscriptBox["0", "k"]}]},
         "Ket"], "-", 
        RowBox[{
         SubscriptBox["\[Eta]", "k"], 
         TemplateBox[{RowBox[{" ", 
             SubscriptBox["1", "k"]}]},
          "Ket"]}]}], ")"}]}], "=", 
     RowBox[{
      RowBox[{"exp", "(", 
       RowBox[{"-", 
        RowBox[{
         UnderscriptBox["\[Sum]", "k"], " ", 
         RowBox[{
          SubscriptBox["\[Eta]", "k"], " ", 
          SubsuperscriptBox["c", "k", "\[Dagger]"]}]}]}], ")"}], 
      TemplateBox[{RowBox[{" ", "0"}]},
       "Ket"]}]}]}]}], TraditionalForm]], "EquationNumbered",
