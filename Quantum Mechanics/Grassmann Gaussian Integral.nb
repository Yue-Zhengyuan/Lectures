    RowBox[{"\[DifferentialD]", 
     SubscriptBox["\[Eta]", "n"]}], 
    RowBox[{"exp", "(", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["1", "2"]}], 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"i", ",", 
         RowBox[{"j", "=", "1"}]}], "n"], 
       RowBox[{
        SubscriptBox["\[Eta]", "i"], 
        SubscriptBox["A", 
         RowBox[{"i", " ", "j"}]], 
        SubscriptBox["\[Eta]", "j"]}]}]}], ")"}]}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"\[Integral]", 
      RowBox[{"\[DifferentialD]", 
       SubscriptBox["\[Eta]", "1"]}]}], "..."}], 
    RowBox[{"\[DifferentialD]", 
     SubscriptBox["\[Eta]", "n"]}], " ", 
    RowBox[{
     UnderoverscriptBox["\[Product]", 
      RowBox[{"i", ",", 
       RowBox[{"j", "=", "1"}]}], "n"], 
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{
        FractionBox["1", "2"], 
        SubscriptBox["\[Eta]", "i"], 
        SubscriptBox["A", 
         RowBox[{"i", " ", "j"}]], 
        SubscriptBox["\[Eta]", "j"]}]}], ")"}]}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.7865241470696363`*^9, 3.786524207822803*^9}, {
