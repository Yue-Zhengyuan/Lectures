79fdc51628d1"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"Z", "(", "0", ")"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", 
         FractionBox["1", "2"]}], ")"}], "n"], 
      RowBox[{"\[Integral]", " ", 
       RowBox[{
        UnderoverscriptBox["\[Product]", 
         RowBox[{"k", "=", "1"}], "n"], " ", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"\[DifferentialD]", 
           SubscriptBox["\[Eta]", "k"]}], ")"}], " ", 
         RowBox[{
          UnderscriptBox["\[Sum]", "\[Sigma]"], " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", 
             SubscriptBox["\[Eta]", "1"]}], 
            SubscriptBox["A", 
             RowBox[{"1", 
              RowBox[{"\[Sigma]", "(", "1", ")"}]}]], 
            SubscriptBox["\[Eta]", 
             RowBox[{"\[Sigma]", "(", "1", ")"}]]}], ")"}]}]}]}]}]}], "..."}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", 
       SubscriptBox["\[Eta]", "n"]}], 
      SubscriptBox["A", 
       RowBox[{"n", " ", 
        RowBox[{"\[Sigma]", "(", "n", ")"}]}]], 
      SubscriptBox["\[Eta]", 
       RowBox[{"\[Sigma]", "(", "n", ")"}]]}], ")"}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.7865783382035103`*^9, 3.786578355490011*^9}, {
  3.786580708040826*^9, 3.786580752249242*^9}, {3.786580823617079*^9, 
  3.786580845482938*^9}, {3.78658090616995*^9, 3.786580908592955*^9}, {
  3.786580993788162*^9, 
  3.7865809941512957`*^9}},ExpressionUUID->"e7396487-5fa1-412b-93d6-\
63c7e64e447e"],

Cell[TextData[{
 "Here we demand that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[Sigma]", "(", "1", ")"}], "\[NotEqual]", "1"}], ",", 
    RowBox[{
     RowBox[{"...", 
      RowBox[{"\[Sigma]", "(", 
       RowBox[{"n", "/", "2"}], ")"}]}], "\[NotEqual]", 
     RowBox[{"n", "/", "2"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "4916864c-9150-45d0-9f96-a72b94bd87c4"],
 ". Because of the anti-symmetry of ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "5337b570-20cc-4c12-90cf-515d9453df48"],
 ", each term in the summation has in fact be counted twice (that\
\[CloseCurlyQuote]s why we need the additional 1/2 factor). "
}], "Text",
 CellChangeTimes->{{3.786580996583522*^9, 3.786581054051002*^9}, {
  3.786581092315687*^9, 3.786581095842862*^9}, {3.786581259145445*^9, 
  3.786581280724456*^9}},ExpressionUUID->"f6ecf52f-1eaf-4f47-9593-\
68ede7ddb35d"],

Cell[BoxData[
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], "n"], 
