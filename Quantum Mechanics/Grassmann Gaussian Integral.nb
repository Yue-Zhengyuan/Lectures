       UnderscriptBox["\[Sum]", "\[Sigma]"], 
       SubscriptBox["A", 
        RowBox[{"1", " ", 
         RowBox[{"\[Sigma]", "(", "1", ")"}]}]]}]}], "..."}], 
    SubscriptBox["A", 
     RowBox[{"n", " ", 
      RowBox[{"\[Sigma]", "(", "n", ")"}]}]], 
    RowBox[{"\[Integral]", " ", 
     RowBox[{
      UnderoverscriptBox["\[Product]", 
       RowBox[{"k", "=", "1"}], "n"], " ", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"\[DifferentialD]", 
         SubscriptBox["\[Eta]", "k"]}], ")"}], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{
           SubscriptBox["\[Eta]", "1"], 
