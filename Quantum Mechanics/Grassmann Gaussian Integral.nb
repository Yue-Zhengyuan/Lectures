      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], 
       RowBox[{"i", "+", "j"}]], 
      SubscriptBox[
       OverscriptBox["\[Xi]", "_"], "i"], 
      SubscriptBox["\[Xi]", "j"], 
      SuperscriptBox[
       RowBox[{"det", " ", "[", "A", "]"}], 
       RowBox[{"(", 
        RowBox[{"i", " ", "j"}], ")"}]]}]}], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"det", " ", "A"}], ")"}], 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"i", ",", 
        RowBox[{"j", "=", "1"}]}], "n"], 
      RowBox[{
       SubscriptBox[
        RowBox[{
         SubscriptBox["\[Xi]", "i"], " ", "[", 
         SuperscriptBox["A", 
          RowBox[{"-", "1"}]], "]"}], 
        RowBox[{"i", " ", "j"}]], 
       SubscriptBox[
        OverscriptBox["\[Xi]", "_"], "j"]}]}]}]}]}], 
  TraditionalForm]}], "Equation",
