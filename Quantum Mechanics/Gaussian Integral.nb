2338259f6b01"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      RowBox[{
       SubscriptBox["x", 
        RowBox[{"a", "(", "1", ")"}]], "..."}], 
      SubscriptBox["x", 
       RowBox[{"a", "(", "m", ")"}]]}], "\[RightAngleBracket]"}], 
    "\[Congruent]", 
    FractionBox[
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"\[Integral]", 
          RowBox[{"\[DifferentialD]", 
           SubscriptBox["x", "1"]}]}], "..."}], 
        RowBox[{"\[DifferentialD]", 
         SubscriptBox["x", "n"]}], " ", 
        SubscriptBox["x", 
         RowBox[{"a", "(", "1", ")"}]]}], "..."}], 
      SubscriptBox["x", 
       RowBox[{"a", "(", "m", ")"}]], 
      RowBox[{"exp", "(", 
       RowBox[{
        RowBox[{"-", 
         FractionBox["1", "2"]}], 
        SubscriptBox["x", "i"], 
        SubscriptBox["A", 
         RowBox[{"i", " ", "j"}]], 
        SubscriptBox["x", "j"]}], ")"}]}], 
     RowBox[{
      RowBox[{
       RowBox[{"\[Integral]", 
        RowBox[{"\[DifferentialD]", 
         SubscriptBox["x", "1"]}]}], "..."}], 
      RowBox[{"\[DifferentialD]", 
       SubscriptBox["x", "n"]}], " ", 
      RowBox[{"exp", "(", 
       RowBox[{
        RowBox[{"-", 
         FractionBox["1", "2"]}], 
        SubscriptBox["x", "i"], 
        SubscriptBox["A", 
         RowBox[{"i", " ", "j"}]], 
        SubscriptBox["x", "j"]}], ")"}]}]]}], "=", 
   SubscriptBox[
    RowBox[{"[", 
     RowBox[{
      RowBox[{
       FractionBox["\[PartialD]", 
        RowBox[{"\[PartialD]", 
         SubscriptBox["J", 
          RowBox[{"a", "(", "m", ")"}]]}]], "..."}], 
      RowBox[{
       FractionBox["\[PartialD]", 
        RowBox[{"\[PartialD]", 
         SubscriptBox["J", 
          RowBox[{"a", "(", "1", ")"}]]}]], 
       RowBox[{"\[ScriptCapitalZ]", "(", "J", ")"}]}]}], "]"}], 
    RowBox[{"J", "=", "0"}]]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.786502203415419*^9, 3.786502464298698*^9}, {
  3.786509463555744*^9, 3.786509466360259*^9}, {3.7865096469291*^9, 
  3.7865096491583567`*^9}},ExpressionUUID->"ccfb928d-8927-4b6c-a127-\
e68bd8ed760e"],

