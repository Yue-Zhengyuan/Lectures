            SuperscriptBox["k", "\[Prime]2"], "-", 
            RowBox[{"2", "k", " ", 
             SuperscriptBox["k", "\[Prime]"], "cos", " ", "\[Theta]"}], "+", 
            SuperscriptBox["k", "2"]}]]}]}]}]}]}], "\[LineSeparator]", "=", 
     RowBox[{
      FractionBox[
       SuperscriptBox["e", "2"], "\[Pi]"], 
      RowBox[{
       SubsuperscriptBox["\[Integral]", "0", 
        SubscriptBox["k", "F"]], 
       RowBox[{
        RowBox[{"\[DifferentialD]", 
         SuperscriptBox["k", "\[Prime]"]}], 
        RowBox[{
         SubsuperscriptBox["\[Integral]", "0", "\[Pi]"], " ", 
         RowBox[{
          RowBox[{"\[DifferentialD]", "\[Theta]"}], " ", 
          FractionBox[
           RowBox[{
            SuperscriptBox["k", "\[Prime]2"], "sin", " ", "\[Theta]"}], 
           RowBox[{
            SuperscriptBox["k", "\[Prime]2"], "-", 
            RowBox[{"2", "k", " ", 
             SuperscriptBox["k", "\[Prime]"], "cos", " ", "\[Theta]"}], "+", 
            SuperscriptBox["k", "2"]}]]}]}]}]}]}]}]}]}], 
  TraditionalForm]}], "Equation",
 CellChangeTimes->{{3.794180342785049*^9, 3.794180371938324*^9}, {
