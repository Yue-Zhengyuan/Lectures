Cell[BoxData[
 FormBox[
  RowBox[{"H", "=", 
   RowBox[{
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"a", "=", "1"}], "N"], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        FractionBox["1", 
         RowBox[{"2", "m"}]], 
        SubsuperscriptBox["p", "a", "2"]}], "+", 
       RowBox[{
        UnderscriptBox["\[Sum]", "R"], 
        FractionBox[
         RowBox[{
          RowBox[{"-", "Z"}], " ", 
          SuperscriptBox["e", "2"]}], 
         RowBox[{"\[LeftBracketingBar]", 
          RowBox[{
           SubscriptBox["x", "a"], "-", "R"}], "\[RightBracketingBar]"}]]}]}],
       ")"}]}], "+", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{
      UnderscriptBox["\[Sum]", 
       RowBox[{"a", "\[NotEqual]", "b"}]], 
      FractionBox[
       SuperscriptBox["e", "2"], 
       RowBox[{"\[LeftBracketingBar]", 
        RowBox[{
         SubscriptBox["x", "a"], "-", 
         SubscriptBox["x", "b"]}], "\[RightBracketingBar]"}]]}]}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.793003527534717*^9, 3.793003642368671*^9}, {
  3.793003942923955*^9, 3.793003943440135*^9}, {3.793142183901058*^9, 
  3.793142197334003*^9}, {3.793166541961771*^9, 3.793166543248118*^9}, {
  3.7931674706104116`*^9, 3.793167471180575*^9}, {3.793167508799451*^9, 
  3.793167527757152*^9}, {3.793167581785881*^9, 
  3.793167581945342*^9}},ExpressionUUID->"9a875834-28d6-4480-ac19-\
59a15bc3571d"],

Cell[TextData[{
 "The Hartree-Fock approximation is a variational method, which relies on the \
fact that for a time-independent Hamiltonian ",
 Cell[BoxData[
  FormBox["H", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "aff399fa-1577-475d-9df6-0d2e6cb6c5e1"],
 ", "
}], "Text",
 CellChangeTimes->{{3.793061071679582*^9, 3.7930610736759243`*^9}, {
  3.7930611291096983`*^9, 3.793061141356752*^9}, {3.79306126917054*^9, 
  3.793061272863927*^9}, {3.793146218421996*^9, 
  3.793146241402822*^9}},ExpressionUUID->"9e65d67a-f2a5-4179-80ae-\
2aab2380550b"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox[
    RowBox[{"\[LeftAngleBracket]", "H", "\[RightAngleBracket]"}], 
    "\[CapitalPsi]"], "=", 
   RowBox[{
    FractionBox[
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"\[CapitalPsi]", "|", "H", "|", "\[CapitalPsi]"}], 
      "\[RightAngleBracket]"}], 
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"\[CapitalPsi]", "|", "\[CapitalPsi]"}], 
      "\[RightAngleBracket]"}]], 
    OverscriptBox["\[LongRightArrow]", 
     RowBox[{
      RowBox[{"{", 
       SubscriptBox["\[Psi]", "n"], "}"}], " ", "orthonormal"}]], 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{"\[CapitalPsi]", "|", "H", "|", "\[CapitalPsi]"}], 
     "\[RightAngleBracket]"}]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.793061071679582*^9, 3.7930610736759243`*^9}, {
  3.7930611291096983`*^9, 
  3.793061148344095*^9}},ExpressionUUID->"96c7aa1d-bdc1-4543-ba96-\
64c64247ccd7"],

Cell[TextData[{
 "is minimized by the ground-state wave function. To get an approximate \
solution of the ground-state energy, we construct ",
 Cell[BoxData[
  FormBox["\[CapitalPsi]", TraditionalForm]],ExpressionUUID->
  "6661c3e9-1220-4c68-9d7d-dbbe77b79ff5"],
 " of a certain form, and minimize over its parameters. "
}], "Text",
