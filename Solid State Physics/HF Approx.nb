    RowBox[{"e", "[", 
     RowBox[{
      RowBox[{
       SubscriptBox["n", "0"], "(", 
       RowBox[{"\[Mu]", "+", 
        RowBox[{"e", " ", 
         RowBox[{"\[Phi]", "(", "x", ")"}]}]}], ")"}], "-", 
      RowBox[{
       SubscriptBox["n", "0"], "(", "\[Mu]", ")"}]}], "]"}]}]}], 
