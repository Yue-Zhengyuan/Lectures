 CellChangeTimes->{{3.79307735260778*^9, 
  3.793077404640416*^9}},ExpressionUUID->"01c91292-74ac-4964-b80f-\
ab425609159b"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SuperscriptBox["\[Rho]", "ind"], "(", "x", ")"}], "=", 
