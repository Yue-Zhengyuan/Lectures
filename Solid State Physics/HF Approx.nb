Cell[CellGroupData[{

Cell[TextData[{
 
 CounterBox["Subsection"],
 ". Thomas-Fermi Theory (Semiclassical Approximation)"
}], "Subsection",
 CellChangeTimes->{{3.792993382597183*^9, 3.792993410361463*^9}, {
  3.79299581459389*^9, 3.792995817935122*^9}, {3.79299585862435*^9, 
  3.792995861138424*^9}},ExpressionUUID->"8d6c682c-fd64-4da2-9a30-\
5b97a6457641"],

Cell["One-electron Schr\[ODoubleDot]dinger equation", "Text",
 CellChangeTimes->{{3.793076787144384*^9, 
  3.7930767986137867`*^9}},ExpressionUUID->"155e134b-32a4-4c30-b13e-\
d30218993c09"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"-", 
      FractionBox[
       SuperscriptBox["\[HBar]", "2"], 
       RowBox[{"2", "m"}]]}], 
     RowBox[{
      SuperscriptBox["\[Del]", "2"], 
      RowBox[{
       SubscriptBox["\[Psi]", "n"], "(", "x", ")"}]}]}], "-", 
    RowBox[{"e", " ", 
     RowBox[{"\[Phi]", "(", "x", ")"}], 
     RowBox[{
      SubscriptBox["\[Psi]", "n"], "(", "x", ")"}]}]}], "=", 
   RowBox[{
    SubscriptBox["\[Epsilon]", "n"], 
    RowBox[{
     SubscriptBox["\[Psi]", "n"], "(", "x", ")"}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.793076787144384*^9, 
  3.79307683110436*^9}},ExpressionUUID->"793bdeec-4487-4a57-8644-\
f00331343359"],

Cell["The single electron energy is approximated by", "Text",
 CellChangeTimes->{{3.793076680071793*^9, 
  3.7930766958743277`*^9}},ExpressionUUID->"3dc0d0f0-f6ad-4b5a-930f-\
55ac10450a37"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[Epsilon]", "(", "k", ")"}], "=", 
   RowBox[{
    FractionBox[
     RowBox[{
      SuperscriptBox["\[HBar]", "2"], 
      SuperscriptBox["k", "2"]}], 
     RowBox[{"2", "m"}]], "-", 
    RowBox[{"e", " ", 
     RowBox[{"\[Phi]", "(", "x", ")"}]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.793076680071793*^9, 
  3.793076714962575*^9}},ExpressionUUID->"931b440e-65cb-48af-ab74-\
c4aec5d63036"],

Cell[TextData[{
 "requiring that ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Phi]", "(", "x", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "ecb9f1ab-222b-4360-ba61-ba64d426a300"],
 " vary slowly on the scale of a Fermi wavelength. Then the equilibrium \
number density of the electrons is given by the Fermi distribution (including \
the spin degeneracy of 2)"
}], "Text",
 CellChangeTimes->{{3.793076731245184*^9, 3.793076758700753*^9}, {
  3.793076846247657*^9, 
  3.793076892139439*^9}},ExpressionUUID->"83909ca5-c534-4efd-9f7b-\
178d05dcbfd0"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"n", "(", "x", ")"}], "=", 
   RowBox[{"\[Integral]", 
    RowBox[{
     FractionBox[
      RowBox[{
       SuperscriptBox["\[DifferentialD]", "3"], "k"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"2", "\[Pi]"}], ")"}], "3"]], 
     FractionBox["2", 
      RowBox[{
       RowBox[{"exp", "[", 
        RowBox[{"\[Beta]", " ", 
         RowBox[{"(", 
          RowBox[{
           FractionBox[
            RowBox[{
             SuperscriptBox["\[HBar]", "2"], 
             SuperscriptBox["k", "2"]}], 
            RowBox[{"2", "m"}]], "-", 
           RowBox[{"e", " ", 
            RowBox[{"\[Phi]", "(", "x", ")"}]}], "-", "\[Mu]"}], ")"}]}], 
        "]"}], "+", "1"}]]}]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.793076731245184*^9, 3.793076758700753*^9}, {
  3.793076846247657*^9, 3.793076929378385*^9}, {3.793077101741411*^9, 
  3.7930771031307077`*^9}, {3.793091498790786*^9, 
  3.79309150300169*^9}},ExpressionUUID->"4af41d12-a8b7-47eb-bbea-\
b576f749f2a9"],

Cell[TextData[{
 "Compared with the density of free electron gas (",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Phi]", "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "f144d9a4-37a8-482a-a60d-40f22d5c1e37"],
