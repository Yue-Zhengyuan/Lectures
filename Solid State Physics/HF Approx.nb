   3.793146253049802*^9}},ExpressionUUID->"5c873171-8b67-41eb-bf2d-\
065b8ea31145"],

Cell[BoxData[{
 FormBox[
  RowBox[{
   RowBox[{"\[CapitalPsi]", "(", 
    RowBox[{
     SubscriptBox["q", "1"], ",", "...", ",", 
     SubscriptBox["q", "N"]}], ")"}], "=", 
   RowBox[{
    FractionBox["1", 
     SqrtBox[
      RowBox[{"N", "!"}]]], 
    RowBox[{"det", "(", GridBox[{
       {
        RowBox[{
         SubscriptBox["\[Psi]", "1"], "(", 
         SubscriptBox["q", "1"], ")"}], 
        RowBox[{
         SubscriptBox["\[Psi]", "1"], "(", 
         SubscriptBox["q", "2"], ")"}], "...", 
        RowBox[{
         SubscriptBox["\[Psi]", "1"], "(", 
         SubscriptBox["q", "N"], ")"}]},
       {
        RowBox[{
         SubscriptBox["\[Psi]", "2"], "(", 
         SubscriptBox["q", "1"], ")"}], 
        RowBox[{
         SubscriptBox["\[Psi]", "2"], "(", 
         SubscriptBox["q", "2"], ")"}], "...", 
        RowBox[{
         SubscriptBox["\[Psi]", "2"], "(", 
         SubscriptBox["q", "N"], ")"}]},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "...", 
        "\[VerticalEllipsis]"},
       {
        RowBox[{
         SubscriptBox["\[Psi]", "N"], "(", 
         SubscriptBox["q", "1"], ")"}], 
        RowBox[{
         SubscriptBox["\[Psi]", "N"], "(", 
         SubscriptBox["q", "2"], ")"}], "...", 
        RowBox[{
         SubscriptBox["\[Psi]", "N"], "(", 
         SubscriptBox["q", "N"], ")"}]}
      }], ")"}]}]}], TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{
     RowBox[{
      FractionBox["1", 
       SqrtBox[
        RowBox[{"N", "!"}]]], 
      RowBox[{
       UnderscriptBox["\[Sum]", "\[ScriptCapitalP]"], 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}], 
         RowBox[{"sgn", "(", "\[ScriptCapitalP]", ")"}]], 
        RowBox[{
         SubscriptBox["\[Psi]", "1"], "(", 
         SubscriptBox["q", 
          RowBox[{"\[ScriptCapitalP]", "(", "1", ")"}]], ")"}]}]}]}], "..."}], 
    RowBox[{
     SubscriptBox["\[Psi]", "N"], "(", 
     SubscriptBox["q", 
      RowBox[{"\[ScriptCapitalP]", "(", "N", ")"}]], ")"}]}]}], 
  TraditionalForm]}], "EquationNumbered",
 CellChangeTimes->{{3.7930050274667997`*^9, 3.7930052295074577`*^9}, {
  3.7930053285260687`*^9, 3.793005348543892*^9}, {3.793060055355982*^9, 
  3.793060176062134*^9}, {3.793142955972789*^9, 
  3.793142968251313*^9}},ExpressionUUID->"e6b3c1b5-9209-4798-b781-\
3c770c902967"],

Cell[TextData[{
 "Here ",
 Cell[BoxData[
  FormBox[
   RowBox[{"q", "\[Congruent]", 
