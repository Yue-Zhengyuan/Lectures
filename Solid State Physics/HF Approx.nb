Cell[BoxData[{
 FormBox[
  RowBox[{"I", "\[Congruent]", 
   RowBox[{
    SubscriptBox["\[Integral]", 
     RowBox[{
      SuperscriptBox["k", "\[Prime]"], "<", 
      SubscriptBox["k", "F"]}]], 
    RowBox[{
     FractionBox[
      RowBox[{
       SuperscriptBox["\[DifferentialD]", "3"], 
       SuperscriptBox["k", "\[Prime]"]}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"2", "\[Pi]"}], ")"}], "3"]], 
     FractionBox[
      RowBox[{"4", "\[Pi]", " ", 
       SuperscriptBox["e", "2"]}], 
      SuperscriptBox[
       RowBox[{"\[LeftBracketingBar]", 
        RowBox[{"k", "-", 
         SuperscriptBox["k", "\[Prime]"]}], "\[RightBracketingBar]"}], 
       "2"]]}]}]}], TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[Integral]", 
      RowBox[{
       SuperscriptBox["k", "\[Prime]"], "<", 
       SubscriptBox["k", "F"]}]], 
     RowBox[{
      FractionBox[
       RowBox[{
        SuperscriptBox["k", "\[Prime]2"], "sin", " ", "\[Theta]", " ", 
        RowBox[{"\[DifferentialD]", 
         SuperscriptBox["k", "\[Prime]"]}], 
        RowBox[{"\[DifferentialD]", "\[Theta]"}], 
        RowBox[{"\[DifferentialD]", "\[CurlyPhi]"}]}], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"2", "\[Pi]"}], ")"}], "3"]], 
      FractionBox[
       RowBox[{"4", "\[Pi]", " ", 
        SuperscriptBox["e", "2"]}], 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox["k", "\[Prime]"], "sin", " ", "\[Theta]"}], ")"}], 
         "2"], "+", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            SuperscriptBox["k", "\[Prime]"], "cos", " ", "\[Theta]"}], "-", 
           "k"}], ")"}], "2"]}]]}]}], "\[LineSeparator]", "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       SuperscriptBox["e", "2"], "\[Pi]"], 
      RowBox[{
       SubsuperscriptBox["\[Integral]", "0", 
        SubscriptBox["k", "F"]], 
       RowBox[{
        RowBox[{"\[DifferentialD]", 
         SuperscriptBox["k", "\[Prime]"]}], 
        RowBox[{
         SubsuperscriptBox["\[Integral]", "0", "\[Pi]"], " ", 
         RowBox[{
          RowBox[{"\[DifferentialD]", "\[Theta]"}], " ", 
          FractionBox[
           RowBox[{
            SuperscriptBox["k", "\[Prime]2"], "sin", " ", "\[Theta]"}], 
