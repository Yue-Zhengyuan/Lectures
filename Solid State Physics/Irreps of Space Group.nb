dc73c8a83627"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["R", "n"], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["n", "1"], 
     SubscriptBox["a", "1"]}], "+", 
    RowBox[{
     SubscriptBox["n", "2"], 
     SubscriptBox["a", "2"]}], "+", 
    RowBox[{
     SubscriptBox["n", "3"], 
     SubscriptBox["a", "3"], "    ", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["n", "1"], ",", 
       SubscriptBox["n", "2"], ",", 
       RowBox[{
        SubscriptBox["n", "3"], "\[Element]", "\[DoubleStruckCapitalZ]"}]}], 
      ")"}]}]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.791587725726713*^9, 3.791587783871312*^9}, {
  3.7915878824710913`*^9, 
  3.791587942519726*^9}},ExpressionUUID->"eb147c15-e8da-453c-a15f-\
50ff885356b7"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "1"], ",", 
    SubscriptBox["a", "2"], ",", 
    SubscriptBox["a", "3"]}], TraditionalForm]],ExpressionUUID->
  "e6aca096-b7c2-4b9e-964f-74c1bdd2e85b"],
 " are the basis vectors of the original lattice. The translation by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{"I", "|", 
     SubscriptBox["R", "n"]}], "}"}], TraditionalForm]],ExpressionUUID->
  "d9bc6953-b7af-49c8-8c7a-c9d25b211ffd"],
 " can be generated by the translations ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{"I", "|", 
      SubscriptBox["a", "i"]}], "}"}], " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"i", "=", "1"}], ",", "2", ",", "3"}], ")"}]}], 
   TraditionalForm]],ExpressionUUID->"ca8ea2e9-ee48-422d-8b74-7495d5b7c11c"],
 ":"
}], "Text",
 CellChangeTimes->{{3.791587946013153*^9, 3.791587971653253*^9}, {
  3.791588134355299*^9, 3.79158815720676*^9}, {3.791588199206985*^9, 
  3.791588244995977*^9}},ExpressionUUID->"179fb2ed-bb63-4ee8-a291-\
9fe2080704e5"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"{", 
    RowBox[{"I", "|", 
     SubscriptBox["R", "n"]}], "}"}], "=", 
   RowBox[{
    SuperscriptBox[
     RowBox[{"{", 
      RowBox[{"I", "|", 
       SubscriptBox["a", "1"]}], "}"}], 
     SubscriptBox["n", "1"]], 
    SuperscriptBox[
     RowBox[{"{", 
      RowBox[{"I", "|", 
       SubscriptBox["a", "2"]}], "}"}], 
     SubscriptBox["n", "2"]], 
    SuperscriptBox[
     RowBox[{"{", 
      RowBox[{"I", "|", 
       SubscriptBox["a", "3"]}], "}"}], 
     SubscriptBox["n", "3"]]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.791587946013153*^9, 3.791587971653253*^9}, {
  3.791588134355299*^9, 3.79158815720676*^9}, {3.791588199206985*^9, 
  3.791588277841971*^9}},ExpressionUUID->"1879f8dc-f74b-4e8b-9708-\
4591d7fad3da"],

Cell[TextData[{
 "This means that ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]],ExpressionUUID->
  "0a8b5f45-39f5-42cc-929b-5a57d4c361a9"],
