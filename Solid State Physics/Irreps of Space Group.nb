Cell[CellGroupData[{

Cell[TextData[{
 
 CounterBox["Subsection"],
 ". Basis of the Representation: Bloch Function"
}], "Subsection",
 CellChangeTimes->{{3.791587861810218*^9, 3.791587874246438*^9}, {
   3.791587930726295*^9, 3.791587931336972*^9}, {3.791610953746429*^9, 
   3.791610953865489*^9}, 
   3.7917962704255667`*^9},ExpressionUUID->"ba9e4c87-6616-4e2a-a1f0-\
a616374e022f"],

Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Phi]", 
     RowBox[{"(", "k", ")"}]], "(", "r", ")"}], TraditionalForm]],
  ExpressionUUID->"28305043-1158-4abc-86bf-b58b7682e2e4"],
 " be the basis function (field) which supports the ",
 Cell[BoxData[
  FormBox["k", TraditionalForm]],ExpressionUUID->
  "095d564a-3dd7-43d1-a508-3c7ad587a5eb"],
 "th one-dimensional irrep of ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]],ExpressionUUID->
  "6ed03f39-828e-422e-83a8-0947b89ff619"],
 ". Then under the translation"
}], "Text",
 CellChangeTimes->{{3.791589732341465*^9, 3.791589802818828*^9}, {
  3.791589923146435*^9, 
  3.7915899256513443`*^9}},ExpressionUUID->"63d75dc2-d50e-4c74-bd0e-\
43aeffb599a2"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubsuperscriptBox["\[Phi]", "k", "\[Prime]"], "(", "r", ")"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      SuperscriptBox["D", 
       RowBox[{"(", "k", ")"}]], "(", 
      RowBox[{"{", 
       RowBox[{"I", "|", 
        SubscriptBox["R", "n"]}], "}"}], ")"}], 
     RowBox[{
      SubscriptBox["\[Phi]", "k"], "(", "r", ")"}]}], "=", 
    RowBox[{
     RowBox[{"exp", "(", 
      RowBox[{
       RowBox[{"-", "\[ImaginaryI]"}], " ", 
       RowBox[{"k", "\[CenterDot]", 
        SubscriptBox["R", "n"]}]}], ")"}], 
     RowBox[{
      SubscriptBox["\[Phi]", "k"], "(", "r", ")"}]}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.791589732341465*^9, 3.791589830373748*^9}, {
  3.791590626173994*^9, 3.791590634744175*^9}, {3.7915907023668528`*^9, 
  3.791590713829237*^9}},ExpressionUUID->"100be09b-6d7b-4ea4-9f95-\
314b416a98c5"],

Cell[TextData[{
 "But the actual action on the physical system moves the value of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Phi]", "k"], "(", "r", ")"}], TraditionalForm]],
  ExpressionUUID->"9beb2c67-a4b1-48a3-b432-f1ad2d5cb52a"],
 " to position ",
 Cell[BoxData[
  FormBox[
   RowBox[{"r", "+", 
    SubscriptBox["R", "n"]}], TraditionalForm]],ExpressionUUID->
  "f51f61e7-1878-41ac-846c-fd287a374c28"],
 ". Therefore, after the action, the value of the field at ",
 Cell[BoxData[
  FormBox["r", TraditionalForm]],ExpressionUUID->
  "0b55e465-f4bb-4c37-8110-ffb14c4d45b1"],
 " is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Phi]", "k"], "(", 
    RowBox[{"r", "-", 
     SubscriptBox["R", "n"]}], ")"}], TraditionalForm]],ExpressionUUID->
  "473db797-90ed-4495-b490-76f0a58771c4"],
 ", i.e."
}], "Text",
 CellChangeTimes->{{3.7915898809024878`*^9, 3.791589894427493*^9}, {
  3.791590537789015*^9, 3.791590642788455*^9}, {3.791590716363947*^9, 
  3.791590722309754*^9}},ExpressionUUID->"832ba10d-54a7-48e7-8e52-\
85e9cb70ee0c"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubsuperscriptBox["\[Phi]", "k", "\[Prime]"], "(", "r", ")"}], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[Phi]", "k"], "(", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"{", 
        RowBox[{"I", "|", 
         SubscriptBox["R", "n"]}], "}"}], 
       RowBox[{"-", "1"}]], "r"}], ")"}], "=", 
    RowBox[{
     SubscriptBox["\[Phi]", "k"], "(", 
     RowBox[{"r", "-", 
      SubscriptBox["R", "n"]}], ")"}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.7915898809024878`*^9, 3.791589901781843*^9}, {
   3.791590307234573*^9, 3.7915903111270037`*^9}, 3.791590613846196*^9, {
   3.7915906453184767`*^9, 3.791590663740087*^9}, {3.791590724831356*^9, 
   3.791590734859455*^9}, {3.7915921941033*^9, 
   3.791592220606007*^9}},ExpressionUUID->"92886f43-6ba7-490c-b1b9-\
2ebdd534af89"],

Cell["Then", "Text",
 CellChangeTimes->{{3.791592224471382*^9, 
  3.791592224893449*^9}},ExpressionUUID->"3e00cfdb-6563-45d6-a2c5-\
1f48834eb379"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[Phi]", "k"], "(", 
    RowBox[{"r", "+", 
     SubscriptBox["R", "n"]}], ")"}], "=", 
   RowBox[{
    RowBox[{"exp", "(", 
     RowBox[{"\[ImaginaryI]", " ", 
      RowBox[{"k", "\[CenterDot]", 
       SubscriptBox["R", "n"]}]}], ")"}], 
    RowBox[{
     SubscriptBox["\[Phi]", "k"], "(", "r", ")"}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.7915898809024878`*^9, 3.791589901781843*^9}, {
   3.791590307234573*^9, 3.7915903111270037`*^9}, 3.791590613846196*^9, {
   3.7915906453184767`*^9, 3.791590663740087*^9}, {3.791590724831356*^9, 
   3.791590734859455*^9}, {3.7915921941033*^9, 3.791592228748114*^9}, {
   3.791635361224964*^9, 
   3.791635363505068*^9}},ExpressionUUID->"132f2491-f2b4-4c98-964c-\
f6edd954a12f"],

Cell[TextData[{
 "A function ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Phi]", "k"], "(", "r", ")"}], TraditionalForm]],
  ExpressionUUID->"f4e20267-b3e4-4b22-b920-a85aca1356f0"],
 " satisfying this requirement is called a ",
 StyleBox["Bloch function",
  FontWeight->"Bold"],
 ". "
}], "Text",
 CellChangeTimes->{{3.791590773501689*^9, 3.791590805321258*^9}, {
  3.791610951340218*^9, 3.791610952092657*^9}, {3.791611176068009*^9, 
  3.79161117744587*^9}},ExpressionUUID->"b94c8f02-2cbc-4837-bb1f-\
ffb250a68530"],

Cell["The general form of such a Bloch function is", "Text",
 CellChangeTimes->{{3.791610939411352*^9, 
  3.791610957702855*^9}},ExpressionUUID->"2382ec6f-ed1b-42bf-a6ae-\
fe11f4321297"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[Phi]", "k"], "(", "r", ")"}], "=", 
   RowBox[{
    RowBox[{"u", "(", "r", ")"}], 
    RowBox[{"exp", "(", 
     RowBox[{"\[ImaginaryI]", " ", 
      RowBox[{"k", "\[CenterDot]", "r"}]}], ")"}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.791610939411352*^9, 3.791610979330104*^9}, {
  3.791635542465466*^9, 
  3.791635543332818*^9}},ExpressionUUID->"1675bed1-f37e-4324-899e-\
b5ae4a09d663"],

Cell[TextData[{
 "where the function ",
 Cell[BoxData[
  FormBox[
   RowBox[{"u", "(", "r", ")"}], TraditionalForm]],ExpressionUUID->
  "e5e05cc6-6b81-4599-89fc-b04dda35f65f"],
 " is invariant under translation by lattice vectors:"
}], "Text",
 CellChangeTimes->{{3.791610984606769*^9, 3.7916110192470293`*^9}, {
  3.791635545504713*^9, 3.7916355459997168`*^9}, {3.7916357265813932`*^9, 
  3.791635733696438*^9}},ExpressionUUID->"93a7239c-756a-4378-8643-\
e876fbe22d98"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"u", "(", 
    RowBox[{"r", "+", 
     SubscriptBox["R", "n"]}], ")"}], "=", 
   RowBox[{"u", "(", "r", ")"}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.7916110221450377`*^9, 3.7916110303225203`*^9}, {
  3.791635547941051*^9, 
  3.791635550543532*^9}},ExpressionUUID->"fdda88d2-48ec-411f-856e-\
22de56edbc12"],

Cell[TextData[{
 StyleBox["Proof",
  FontSlant->"Italic"],
 ":"
}], "Text",
