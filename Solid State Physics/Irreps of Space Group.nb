  3.7915889809075127`*^9}},ExpressionUUID->"618dadd6-4faf-4f8b-92f2-\
96dc12d8e3ae"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["\[Lambda]", 
    SubscriptBox["p", "i"]], "=", 
   RowBox[{"exp", "(", 
    RowBox[{
     RowBox[{"-", "2"}], "\[Pi]", " ", "\[ImaginaryI]", 
     FractionBox[
      SubscriptBox["p", "i"], 
      SubscriptBox["N", "i"]]}], ")"}]}], TraditionalForm]], "Equation",
