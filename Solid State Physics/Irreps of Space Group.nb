   RowBox[{"2", "\[Pi]", " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       SubscriptBox["m", "1"], 
       SubscriptBox["n", "1"]}], "+", 
      RowBox[{
       SubscriptBox["m", "2"], 
       SubscriptBox["n", "2"]}], "+", 
      RowBox[{
       SubscriptBox["m", "3"], 
       SubscriptBox["n", "3"]}]}], ")"}]}]}], TraditionalForm]], "Equation",
