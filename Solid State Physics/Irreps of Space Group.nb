Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[Phi]", 
     RowBox[{"k", " ", "\[Alpha]"}]], "(", "r", ")"}], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["u", "\[Alpha]"], "(", "r", ")"}], 
    RowBox[{
     SubscriptBox["\[Psi]", "k"], "(", "r", ")"}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.791625717979785*^9, 3.7916257368972187`*^9}, {
