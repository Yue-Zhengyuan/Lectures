 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Psi]", "k"], "(", "r", ")"}], TraditionalForm]],
  ExpressionUUID->"527656fd-8a9a-43c5-b8fb-704070a93390"],
 " are linear combinations of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"exp", "(", 
    RowBox[{"\[ImaginaryI]", " ", 
     RowBox[{"k", "\[CenterDot]", "r"}]}], ")"}], TraditionalForm]],
  ExpressionUUID->"bd2f0ed7-3ed6-4022-a94e-7f0e6139e5be"],
