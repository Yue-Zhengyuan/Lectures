       RowBox[{"2", "N"}], "+", "1"}]], 
     RowBox[{
      UnderscriptBox["\[Sum]", "k"], " ", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], " ", "k", " ", 
        RowBox[{"(", 
         RowBox[{"n", "-", "m"}], ")"}]}]]}]}], "=", 
    SubscriptBox["\[Delta]", 
     RowBox[{"n", " ", "m"}]]}], ",", "   ", 
   RowBox[{
    RowBox[{
     FractionBox["1", 
