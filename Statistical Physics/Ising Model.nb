  3.784951663594925*^9}},ExpressionUUID->"cd8dbec7-8bbf-4bfd-ae68-\
6312f0ab235d"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox[
    RowBox[{"[", 
     RowBox[{"exp", "(", 
      RowBox[{"\[Epsilon]", " ", "h", " ", 
       SubsuperscriptBox["\[Sigma]", "j", "x"]}], ")"}], "]"}], 
    RowBox[{"i", ",", 
     RowBox[{"i", "+", "1"}]}]], "=", 
   RowBox[{
    RowBox[{"C", " ", 
     RowBox[{"exp", "(", 
      RowBox[{"\[ScriptCapitalJ]", " ", 
       SubscriptBox["s", 
        RowBox[{"i", ",", "j"}]], 
       SubscriptBox["s", 
        RowBox[{
         RowBox[{"i", "+", "1"}], ",", "j"}]]}], ")"}]}], "=", 
    RowBox[{"C", " ", 
     RowBox[{"(", GridBox[{
        {
         SuperscriptBox["\[ExponentialE]", "\[ScriptCapitalJ]"], 
         SuperscriptBox["\[ExponentialE]", 
          RowBox[{"-", "\[ScriptCapitalJ]"}]]},
        {
         SuperscriptBox["\[ExponentialE]", 
          RowBox[{"-", "\[ScriptCapitalJ]"}]], 
         SuperscriptBox["\[ExponentialE]", "\[ScriptCapitalJ]"]}
       }], ")"}]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.7849516470177507`*^9, 3.784951748759018*^9}, {
  3.784951792572089*^9, 3.784951811413925*^9}, {3.7849518556509438`*^9, 
  3.784951886083048*^9}, {3.784952205615295*^9, 
  3.784952205618928*^9}},ExpressionUUID->"b397e567-b4fd-484b-934c-\
8c7762290556"],

Cell[BoxData[
 FormBox[
  RowBox[{"\[DoubleLongRightArrow]", 
   RowBox[{"{", 
    RowBox[{GridBox[{
       {
        RowBox[{
         RowBox[{"cosh", "(", 
          RowBox[{"h", " ", "\[Epsilon]"}], ")"}], "=", 
         RowBox[{"C", " ", 
          RowBox[{"exp", "(", "\[ScriptCapitalJ]", ")"}]}]}]},
       {
        RowBox[{
         RowBox[{"sinh", "(", 
          RowBox[{"h", " ", "\[Epsilon]"}], ")"}], "=", 
         RowBox[{"C", " ", 
          RowBox[{"exp", "(", 
           RowBox[{"-", "\[ScriptCapitalJ]"}], ")"}]}]}]}
      }], "\[DoubleLongRightArrow]", 
     RowBox[{"{", GridBox[{
        {
         RowBox[{
          SuperscriptBox["C", "2"], "=", 
          RowBox[{"sinh", 
           RowBox[{"(", 
            RowBox[{"h", " ", "\[Epsilon]"}], ")"}], "cosh", 
           RowBox[{"(", 
            RowBox[{"h", " ", "\[Epsilon]"}], ")"}]}]}]},
        {
         RowBox[{
          RowBox[{"exp", 
           RowBox[{"(", 
            RowBox[{"2", "\[ScriptCapitalJ]"}], ")"}]}], "=", 
          RowBox[{"coth", 
           RowBox[{"(", 
            RowBox[{"h", " ", "\[Epsilon]"}], ")"}]}]}]}
       }]}]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.7849516470177507`*^9, 3.784951748759018*^9}, {
  3.784951792572089*^9, 3.784951811413925*^9}, {3.7849518556509438`*^9, 
  3.784951886083048*^9}, {3.784952205615295*^9, 
  3.784952271932198*^9}},ExpressionUUID->"25fefeeb-c987-4213-8905-\
366830564562"],

Cell["So we can establish the relation", "Text",
 CellChangeTimes->{{3.784951900523221*^9, 
  3.784951913739205*^9}},ExpressionUUID->"703d2d66-671f-496a-9cb1-\
499df4459639"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[LeftAngleBracket]", 
    RowBox[{
     RowBox[{"{", 
      SubscriptBox["s", 
       RowBox[{"i", "+", "1"}]], "}"}], "|", 
     RowBox[{
      RowBox[{"exp", "(", 
       RowBox[{
        RowBox[{"-", "\[Epsilon]"}], " ", 
        SubscriptBox["H", "2"]}], ")"}], 
      RowBox[{"exp", "(", 
       RowBox[{
        RowBox[{"-", "\[Epsilon]"}], " ", 
        SubscriptBox["H", "1"]}], ")"}]}], "|", 
     RowBox[{"{", 
      SubscriptBox["s", "i"], "}"}]}], "\[RightAngleBracket]"}], "=", 
   RowBox[{"C", " ", 
    RowBox[{"exp", "(", 
     RowBox[{"\[ScriptCapitalJ]", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"j", "=", "0"}], 
        RowBox[{"N", "-", "1"}]], 
       RowBox[{
        SubscriptBox["s", 
         RowBox[{"i", ",", "j"}]], 
        SubscriptBox["s", 
         RowBox[{
          RowBox[{"i", "+", "1"}], ",", "j"}]]}]}]}], ")"}], 
    RowBox[{"exp", "(", 
     RowBox[{"\[Epsilon]", " ", "J", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"j", "=", "0"}], 
        RowBox[{"N", "-", "1"}]], 
       RowBox[{
        SubscriptBox["s", 
         RowBox[{"i", ",", "j"}]], 
        SubscriptBox["s", 
         RowBox[{"i", ",", 
          RowBox[{"j", "+", "1"}]}]]}]}]}], ")"}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.784951900523221*^9, 3.784951960947091*^9}, {
  3.7849524071778097`*^9, 
  3.784952407725157*^9}},ExpressionUUID->"2f6b9ed9-dd49-42b7-a4d9-\
93e44eff8cf6"],

Cell[BoxData[
 FormBox[
  RowBox[{"=", 
   RowBox[{"C", " ", 
    RowBox[{"exp", "(", 
     RowBox[{
      RowBox[{"\[ScriptCapitalJ]", 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"j", "=", "0"}], 
         RowBox[{"N", "-", "1"}]], 
        RowBox[{
         SubscriptBox["s", 
          RowBox[{"i", ",", "j"}]], 
         SubscriptBox["s", 
          RowBox[{
           RowBox[{"i", "+", "1"}], ",", "j"}]]}]}]}], "+", 
      RowBox[{"\[Epsilon]", " ", "J", 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"j", "=", "0"}], 
         RowBox[{"N", "-", "1"}]], 
        RowBox[{
         SubscriptBox["s", 
          RowBox[{"i", ",", "j"}]], 
         SubscriptBox["s", 
          RowBox[{"i", ",", 
           RowBox[{"j", "+", "1"}]}]]}]}]}]}], ")"}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.784951900523221*^9, 3.784951979494404*^9}, {
  3.784952410162928*^9, 
  3.7849524104102182`*^9}},ExpressionUUID->"efb36feb-7c06-4477-8ebe-\
36e71e888720"],

Cell["Comparing with the classical transfer matrix", "Text",
 CellChangeTimes->{{3.784952123788185*^9, 3.7849521537800503`*^9}, {
  3.784952277937133*^9, 3.784952289653576*^9}, {3.784952418069817*^9, 
  3.784952424252355*^9}},ExpressionUUID->"95711892-c87d-42af-b3b3-\
45efd4a34086"],

Cell[BoxData[
 FormBox[
  RowBox[{" ", 
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      RowBox[{"{", 
       SubscriptBox["s", 
        RowBox[{"i", "+", "1"}]], "}"}], "|", "T", "|", 
      RowBox[{"{", 
       SubscriptBox["s", "i"], "}"}]}], "\[RightAngleBracket]"}], "=", 
    RowBox[{"exp", "[", 
     RowBox[{"\[Beta]", "(", 
      RowBox[{
       RowBox[{
        SubscriptBox["J", "int"], 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"j", "=", "0"}], 
          RowBox[{"N", "-", "1"}]], 
         RowBox[{
          SubscriptBox["s", 
           RowBox[{"i", ",", "j"}]], 
          SubscriptBox["s", 
           RowBox[{
            RowBox[{"i", "+", "1"}], ",", "j"}]]}]}]}], "+", 
       RowBox[{
        SubscriptBox["J", "row"], 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"j", "=", "0"}], 
          RowBox[{"N", "-", "1"}]], 
         RowBox[{
          SubscriptBox["s", 
           RowBox[{"i", ",", "j"}]], 
          SubscriptBox["s", 
           RowBox[{"i", ",", 
            RowBox[{"j", "+", "1"}]}]]}]}]}]}], ")"}], "]"}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.784952123788185*^9, 3.7849521900723753`*^9}, {
   3.784952264154871*^9, 3.784952270251547*^9}, 
   3.7849523017870483`*^9},ExpressionUUID->"f3edd79e-9899-4878-8d9d-\
e095912b984c"],

Cell["\<\
We establish the correspondence between the quantum and the classical models:\
\
\>", "Text",
 CellChangeTimes->{{3.784952307897839*^9, 
  3.784952325141816*^9}},ExpressionUUID->"f925f52b-f0f1-462f-8be3-\
6166285e13f2"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[ScriptCapitalJ]", "=", 
    RowBox[{"\[Beta]", " ", 
     SubscriptBox["J", "int"]}]}], ",", " ", 
   RowBox[{
    RowBox[{"\[Epsilon]", " ", "J"}], "=", 
    RowBox[{"\[Beta]", " ", 
     SubscriptBox["J", "row"]}]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.784952307897839*^9, 3.784952326356763*^9}, {
  3.7849524380529137`*^9, 
  3.784952462227665*^9}},ExpressionUUID->"69015c10-78e5-4094-a5b8-\
bac1ae05da7d"],

Cell[TextData[{
 "It is known that the classical Ising model (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["J", "int"], "=", 
    RowBox[{
     SubscriptBox["J", "row"], "=", "1"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "73a5630f-ccb1-41a3-9bcd-af6bf01faede"],
 ") undergoes a phase transition at the temperature"
}], "Text",
 CellChangeTimes->{{3.784952123788185*^9, 3.784952134310055*^9}, {
  3.7849524667763777`*^9, 
  3.784952507545063*^9}},ExpressionUUID->"10d79978-7d2d-4dd6-b542-\
9df56db14819"],

Cell[BoxData[
 FormBox[
  RowBox[{"\[Beta]", "=", 
   RowBox[{"\[ScriptCapitalJ]", "=", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"ln", "(", 
      RowBox[{"1", "+", 
       SqrtBox["2"]}], ")"}]}]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.784952123788185*^9, 3.784952134310055*^9}, {
  3.7849524667763777`*^9, 3.7849525309749928`*^9}, {3.7849679985224867`*^9, 
  3.7849680035114193`*^9}},ExpressionUUID->"138fcf10-d3eb-49ea-a9f0-\
543e802ee3d9"],

