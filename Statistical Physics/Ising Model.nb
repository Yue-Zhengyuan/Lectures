     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", "0"}], 
      RowBox[{"N", "-", "1"}]], 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"j", "=", "0"}], 
       RowBox[{"N", "-", "1"}]], 
      RowBox[{"(", 
       RowBox[{
        UnderscriptBox[
         UnderscriptBox[
          RowBox[{
           SubscriptBox["J", "int"], 
           SubscriptBox["s", 
            RowBox[{"i", ",", "j"}]], 
           SubscriptBox["s", 
            RowBox[{
             RowBox[{"i", "+", "1"}], ",", "j"}]]}], "\[UnderBrace]"], 
         RowBox[{"interaction", " ", "between", " ", "row"}]], "+", 
        UnderscriptBox[
         UnderscriptBox[
          RowBox[{
           SubscriptBox["J", "row"], 
           SubscriptBox["s", 
            RowBox[{"i", ",", "j"}]], 
           SubscriptBox["s", 
            RowBox[{"i", ",", 
             RowBox[{"j", "+", "1"}]}]]}], "\[UnderBrace]"], 
         RowBox[{"interaction", " ", "within", " ", "row"}]]}], ")"}]}]}]}]}],
   TraditionalForm]], "Equation",
