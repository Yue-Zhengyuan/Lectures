Cell[TextData[{
 "This gives us some hint about the emergence of ",
 StyleBox["fermions",
  FontSlant->"Italic"],
 ". However, we know that (e.g. on a lattice) the annihilation/creation \
operators ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"c", "(", "n", ")"}], "/", 
    RowBox[{
     SuperscriptBox["c", "\[Dagger]"], "(", "n", ")"}]}], TraditionalForm]],
  ExpressionUUID->"3793f8e4-16f3-4a3b-900e-f76050b1bd69"],
 " of fermions at ",
 StyleBox["different position",
  FontSlant->"Italic"],
 " should anti-commutes:"
}], "Text",
 CellChangeTimes->{{3.785624336416551*^9, 3.785624370701631*^9}, {
  3.7856247087037582`*^9, 3.7856247245164347`*^9}, {3.785624763221004*^9, 
  3.785624838539888*^9}, {3.785624934800687*^9, 3.7856249742082577`*^9}, {
  3.785625280713828*^9, 3.7856252817797527`*^9}, {3.787281875498412*^9, 
  3.787281882844296*^9}, {3.7873069670695047`*^9, 
  3.787306967552507*^9}},ExpressionUUID->"b3a8fd06-8351-4580-8ef5-\
84c151fb0f76"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"c", "(", "n", ")"}], ",", 
      RowBox[{
       SuperscriptBox["c", "\[Dagger]"], "(", "m", ")"}]}], "}"}], "=", 
    SubscriptBox["\[Delta]", 
     RowBox[{"n", " ", "m"}]]}], ",", " ", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"c", "(", "n", ")"}], ",", 
      RowBox[{"c", "(", "m", ")"}]}], "}"}], "=", 
    RowBox[{
     RowBox[{"{", 
