Cell[BoxData[
 FormBox[
  RowBox[{"H", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"-", "\[Lambda]"}], 
     RowBox[{
      UnderscriptBox["\[Sum]", "n"], 
      RowBox[{
       RowBox[{
        SubscriptBox["\[Sigma]", "1"], "(", "n", ")"}], 
       RowBox[{
        SubscriptBox["\[Sigma]", "1"], "(", 
        RowBox[{"n", "+", "1"}], ")"}]}]}]}], "-", 
    RowBox[{
     UnderscriptBox["\[Sum]", "n"], 
     RowBox[{
      SubscriptBox["\[Sigma]", "3"], "(", "n", ")"}]}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.785623962436076*^9, 3.785623974285254*^9}, {
  3.7856241156598663`*^9, 3.78562421569557*^9}, {3.787281120107073*^9, 
  3.787281126109671*^9}},ExpressionUUID->"b49703d7-3494-442b-8db1-\
cb3b3a049a4e"],

Cell[TextData[{
 "Here ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Sigma]", "i"], "(", 
    RowBox[{
     RowBox[{"i", "=", "1"}], ",", "2", ",", "3"}], ")"}], TraditionalForm]],
  ExpressionUUID->"99d8beca-f86b-4391-9d37-c4c1b76ac0a5"],
 " are the Pauli matrices. The 1D lattice will be labelled by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "=", 
    RowBox[{"-", "N"}]}], TraditionalForm]],ExpressionUUID->
  "46733c48-568c-4391-95b9-866894594aa4"],
 " to ",
 Cell[BoxData[
  FormBox["N", TraditionalForm]],ExpressionUUID->
  "74340957-ef61-471a-8dda-4f2c32ba5342"],
 ". We also define the ",
 StyleBox["raising and lowering operators",
  FontWeight->"Bold"]
}], "Text",
 CellChangeTimes->{{3.785624187603538*^9, 3.785624249177581*^9}, {
  3.785624297042055*^9, 3.7856243151508636`*^9}, {3.7872805158036423`*^9, 
