Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[LeftAngleBracket]", 
    RowBox[{
     RowBox[{"{", 
      SubscriptBox["s", 
       RowBox[{"i", "+", "1"}]], "}"}], "|", 
     RowBox[{
      RowBox[{"exp", "(", 
       RowBox[{
        RowBox[{"-", "\[Epsilon]"}], " ", 
        SubscriptBox["H", "2"]}], ")"}], 
      RowBox[{"exp", "(", 
