Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox[
    RowBox[{"[", 
     RowBox[{"exp", "(", 
      RowBox[{"\[Epsilon]", " ", "h", " ", 
       SubsuperscriptBox["\[Sigma]", "j", "x"]}], ")"}], "]"}], 
    RowBox[{"i", ",", 
     RowBox[{"i", "+", "1"}]}]], "=", 
   RowBox[{
    RowBox[{"C", " ", 
     RowBox[{"exp", "(", 
      RowBox[{"\[ScriptCapitalJ]", " ", 
       SubscriptBox["s", 
        RowBox[{"i", ",", "j"}]], 
       SubscriptBox["s", 
        RowBox[{
         RowBox[{"i", "+", "1"}], ",", "j"}]]}], ")"}]}], "=", 
    RowBox[{"C", " ", 
     RowBox[{"(", GridBox[{
        {
         SuperscriptBox["\[ExponentialE]", "\[ScriptCapitalJ]"], 
         SuperscriptBox["\[ExponentialE]", 
          RowBox[{"-", "\[ScriptCapitalJ]"}]]},
        {
         SuperscriptBox["\[ExponentialE]", 
          RowBox[{"-", "\[ScriptCapitalJ]"}]], 
         SuperscriptBox["\[ExponentialE]", "\[ScriptCapitalJ]"]}
       }], ")"}]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.7849516470177507`*^9, 3.784951748759018*^9}, {
  3.784951792572089*^9, 3.784951811413925*^9}, {3.7849518556509438`*^9, 
  3.784951886083048*^9}, {3.784952205615295*^9, 
  3.784952205618928*^9}},ExpressionUUID->"b397e567-b4fd-484b-934c-\
8c7762290556"],

Cell[BoxData[
 FormBox[
  RowBox[{"\[DoubleLongRightArrow]", 
   RowBox[{"{", 
    RowBox[{GridBox[{
       {
        RowBox[{
         RowBox[{"cosh", "(", 
          RowBox[{"h", " ", "\[Epsilon]"}], ")"}], "=", 
         RowBox[{"C", " ", 
          RowBox[{"exp", "(", "\[ScriptCapitalJ]", ")"}]}]}]},
       {
        RowBox[{
         RowBox[{"sinh", "(", 
          RowBox[{"h", " ", "\[Epsilon]"}], ")"}], "=", 
         RowBox[{"C", " ", 
          RowBox[{"exp", "(", 
           RowBox[{"-", "\[ScriptCapitalJ]"}], ")"}]}]}]}
      }], "\[DoubleLongRightArrow]", 
     RowBox[{"{", GridBox[{
        {
         RowBox[{
          SuperscriptBox["C", "2"], "=", 
          RowBox[{"sinh", 
           RowBox[{"(", 
            RowBox[{"h", " ", "\[Epsilon]"}], ")"}], "cosh", 
           RowBox[{"(", 
            RowBox[{"h", " ", "\[Epsilon]"}], ")"}]}]}]},
        {
         RowBox[{
          RowBox[{"exp", 
           RowBox[{"(", 
            RowBox[{"2", "\[ScriptCapitalJ]"}], ")"}]}], "=", 
          RowBox[{"coth", 
           RowBox[{"(", 
            RowBox[{"h", " ", "\[Epsilon]"}], ")"}]}]}]}
       }]}]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.7849516470177507`*^9, 3.784951748759018*^9}, {
  3.784951792572089*^9, 3.784951811413925*^9}, {3.7849518556509438`*^9, 
