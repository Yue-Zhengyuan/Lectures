 CellChangeTimes->{{3.784952538223287*^9, 
  3.7849525548499517`*^9}},ExpressionUUID->"39f78a90-9ef6-47d1-8054-\
c8403938cbe5"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"exp", "(", 
    RowBox[{"2", "\[ScriptCapitalJ]"}], ")"}], "=", 
   RowBox[{"coth", "(", 
    RowBox[{"h", " ", "\[Epsilon]"}], ")"}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.784952538223287*^9, 
  3.784952597114068*^9}},ExpressionUUID->"ea2eabca-dd60-411d-b52b-\
