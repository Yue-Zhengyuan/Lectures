    RowBox[{
     UnderscriptBox["\[Product]", "k"], 
     RowBox[{"[", 
      RowBox[{"1", "+", 
       RowBox[{"exp", "(", 
        RowBox[{"-", 
         RowBox[{"\[Beta]", "(", 
