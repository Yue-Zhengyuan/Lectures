  FormBox[
   RowBox[{"g", "\[Proportional]", 
    SuperscriptBox["m", "2"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a98da5bb-8c05-4fc2-9c8c-5263b1c690cf"],
 ". Recall ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["h", "\[Prime]"], "=", 
    RowBox[{
     SuperscriptBox["b", "x"], "h"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "fc7755e4-8ce0-40ed-881e-fe85a1253459"],
 ", then"
}], "Text",
 CellChangeTimes->{{3.797981995658319*^9, 
  3.797982081271267*^9}},ExpressionUUID->"716a3e46-b3b3-4426-bc80-\
1968d1f5776f"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"g", "(", 
    RowBox[{"r", ",", "h", ",", "t"}], ")"}], "=", 
   RowBox[{
    FractionBox["1", 
     SuperscriptBox["b", 
      RowBox[{"2", 
       RowBox[{"(", 
        RowBox[{"d", "-", "x"}], ")"}]}]]], 
    RowBox[{"g", "(", 
     RowBox[{
      FractionBox["r", "b"], ",", 
      SuperscriptBox["h", "\[Prime]"], ",", 
      SuperscriptBox["t", "\[Prime]"]}], ")"}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.797981995658319*^9, 
  3.797982111625533*^9}},ExpressionUUID->"4d495c31-647f-4297-b0dd-\
e06b0a6248fc"],

Cell[TextData[{
 "Setting ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"h", "=", "0"}], ",", 
    RowBox[{"t", "=", "0"}], ",", 
    RowBox[{"b", "=", "r"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "5d9912cf-664a-4989-8013-94404d8b49d8"],
 ", then"
}], "Text",
 CellChangeTimes->{{3.7979821155290413`*^9, 
  3.797982154913533*^9}},ExpressionUUID->"3a3c40b6-689a-47cb-b7b8-\
c50de79a33b7"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"g", "(", 
    RowBox[{"r", ",", "0", ",", "0"}], ")"}], "=", 
   RowBox[{
    RowBox[{
     FractionBox["1", 
      SuperscriptBox["b", 
       RowBox[{"2", 
        RowBox[{"(", 
         RowBox[{"d", "-", "x"}], ")"}]}]]], 
     RowBox[{"g", "(", 
      RowBox[{"1", ",", "0", ",", "0"}], ")"}]}], "\[Proportional]", 
    SuperscriptBox["r", 
     RowBox[{"-", 
      RowBox[{"(", 
       RowBox[{"d", "-", "2", "+", "\[Eta]"}], ")"}]}]]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.7979821155290413`*^9, 
  3.797982166253564*^9}},ExpressionUUID->"32e7491c-1b67-4afc-adbc-\
5878d99ec7ef"],

Cell["Thus", "Text",
 CellChangeTimes->{{3.79653478945647*^9, 3.79653479611532*^9}, {
  3.79798002649804*^9, 3.7979800264986153`*^9}, {3.797982174274683*^9, 
  3.797982174600326*^9}},ExpressionUUID->"e8a76691-91bc-4886-8616-\
55f43d99103d"],

Cell[BoxData[
 FormBox[
  RowBox[{"\[Eta]", "=", 
   RowBox[{"2", "+", "d", "-", 
    RowBox[{"2", 
     RowBox[{"x", " ", 
      OverscriptBox["\[LongRightArrow]", 
       RowBox[{"r", "=", 
        RowBox[{
         RowBox[{"-", "x"}], "/", "d"}]}]], " ", "2"}]}], "+", 
    RowBox[{"d", "(", 
     RowBox[{"1", "+", 
      RowBox[{"2", "r"}]}], ")"}]}]}], TraditionalForm]], "EquationNumbered",
