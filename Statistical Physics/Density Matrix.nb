 CellChangeTimes->{{3.789858249473689*^9, 
  3.78985825166363*^9}},ExpressionUUID->"c015ebfa-97ee-4456-a610-\
d11b1a3987fa"],

Cell[BoxData[{
 FormBox[
  RowBox[{
   RowBox[{"\[ImaginaryI]", " ", "\[HBar]", 
    FractionBox[
     RowBox[{"\[PartialD]", "\[Rho]"}], 
     RowBox[{"\[PartialD]", "t"}]]}], "=", 
   RowBox[{
    UnderscriptBox["\[Sum]", "k"], " ", 
    RowBox[{"\[ImaginaryI]", " ", "\[HBar]", " ", 
     SubscriptBox["p", "k"], 
     RowBox[{
      FractionBox["\[PartialD]", 
       RowBox[{"\[PartialD]", "t"}]], 
      RowBox[{"(", 
       RowBox[{
        TemplateBox[{RowBox[{" ", 
            SubscriptBox["\[Psi]", "k"]}]},
         "Ket"], 
        TemplateBox[{SubscriptBox["\[Psi]", "k"]},
         "Bra"]}], ")"}]}]}]}]}], TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{
     UnderscriptBox["\[Sum]", "k"], " ", 
     RowBox[{"\[ImaginaryI]", " ", "\[HBar]", " ", 
      SubscriptBox["p", "k"], 
      FractionBox[
       RowBox[{"\[PartialD]", 
        TemplateBox[{RowBox[{" ", 
            SubscriptBox["\[Psi]", "k"]}]},
         "Ket"]}], 
       RowBox[{"\[PartialD]", "t"}]], 
      TemplateBox[{SubscriptBox["\[Psi]", "k"]},
       "Bra"]}]}], "+", 
    RowBox[{
     UnderscriptBox["\[Sum]", "k"], " ", 
     RowBox[{"\[ImaginaryI]", " ", "\[HBar]", " ", 
      SubscriptBox["p", "k"], 
      TemplateBox[{RowBox[{" ", 
          SubscriptBox["\[Psi]", "k"]}]},
       "Ket"], 
      FractionBox[
       RowBox[{"\[PartialD]", 
        TemplateBox[{SubscriptBox["\[Psi]", "k"]},
         "Bra"]}], 
       RowBox[{"\[PartialD]", "t"}]]}]}]}]}], 
  TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{
     UnderscriptBox["\[Sum]", "k"], " ", 
     RowBox[{
      SubscriptBox["p", "k"], "H", 
      TemplateBox[{RowBox[{" ", 
          SubscriptBox["\[Psi]", "k"]}]},
       "Ket"], 
      TemplateBox[{SubscriptBox["\[Psi]", "k"]},
       "Bra"]}]}], "-", 
    RowBox[{
     UnderscriptBox["\[Sum]", "k"], " ", 
     RowBox[{
      SubscriptBox["p", "k"], 
      TemplateBox[{RowBox[{" ", 
          SubscriptBox["\[Psi]", "k"]}]},
       "Ket"], 
      TemplateBox[{SubscriptBox["\[Psi]", "k"]},
       "Bra"], "H"}]}]}]}], TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{
     RowBox[{"H", " ", "\[Rho]"}], "-", 
     RowBox[{"\[Rho]", " ", "H"}]}], "=", 
    RowBox[{
     RowBox[{"[", 
      RowBox[{"H", ",", "\[Rho]"}], "]"}], "      ", "\[FilledSquare]"}]}]}], 
  TraditionalForm]}], "Equation",
 CellChangeTimes->{{3.789858249473689*^9, 3.789858319214528*^9}, {
  3.789858458627125*^9, 
  3.7898585944162617`*^9}},ExpressionUUID->"dee77f27-4cea-40d2-9025-\
be448a68a468"]
