    RowBox[{
     UnderscriptBox["\[Sum]", 
      RowBox[{"k", ",", "n"}]], 
     RowBox[{
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{
        SubscriptBox["\[Psi]", "n"], "|", 
        RowBox[{"O", " ", 
         SubscriptBox["p", "k"]}], "|", 
        SubscriptBox["\[Psi]", "k"]}], "\[RightAngleBracket]"}], 
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{
        SubscriptBox["\[Psi]", "k"], "|", 
        SubscriptBox["\[Psi]", "n"]}], "\[RightAngleBracket]"}]}]}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.789681410177698*^9, 3.789681478638768*^9}, {
