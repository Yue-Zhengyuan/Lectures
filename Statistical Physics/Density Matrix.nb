Cell[BoxData[{
 FormBox[
  RowBox[{
   SubscriptBox["S", "A"], "=", 
   RowBox[{
    RowBox[{"-", 
     RowBox[{
      SubscriptBox["Tr", "A"], "(", 
      RowBox[{
       SubscriptBox["\[Rho]", "A"], "ln", " ", 
       SubscriptBox["\[Rho]", "A"]}], ")"}]}], "=", 
    RowBox[{"-", 
     RowBox[{
