b2175371386b"],

Cell[BoxData[{
 FormBox[
  RowBox[{
   SubscriptBox["\[Rho]", "A"], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["Tr", "B"], "\[Rho]"}], "=", 
    RowBox[{
     UnderscriptBox["\[Sum]", "i"], " ", 
     RowBox[{
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{"i", ",", 
        RowBox[{"B", "|", "\[Psi]"}]}], "\[RightAngleBracket]"}], 
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{
        RowBox[{"\[Psi]", "|", "i"}], ",", "B"}], 
       "\[RightAngleBracket]"}]}]}]}]}], TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    UnderscriptBox["\[Sum]", 
     RowBox[{"i", ",", "m", ",", "n"}]], " ", 
    RowBox[{
     SubscriptBox["\[Lambda]", "m"], 
     SubscriptBox["\[Lambda]", "n"], 
     TemplateBox[{RowBox[{"i", ",", "B"}],RowBox[{" ", 
         RowBox[{"m", ",", "B"}]}]},
      "BraKet"], 
     TemplateBox[{RowBox[{" ", 
         RowBox[{"m", ",", "A"}]}]},
      "Ket"], 
     TemplateBox[{RowBox[{"n", ",", "A"}]},
      "Bra"], 
     TemplateBox[{RowBox[{"n", ",", "B"}],RowBox[{" ", 
         RowBox[{"i", ",", "B"}]}]},
      "BraKet"]}]}]}], TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    UnderscriptBox["\[Sum]", "i"], " ", 
    RowBox[{
     SubsuperscriptBox["\[Lambda]", "i", "2"], 
     TemplateBox[{RowBox[{" ", 
         RowBox[{"i", ",", "A"}]}]},
      "Ket"], 
     TemplateBox[{RowBox[{"i", ",", "A"}]},
      "Bra"]}]}]}], TraditionalForm]}], "EquationNumbered",
