   SubsuperscriptBox["c", 
    RowBox[{"k", " ", "m"}], "\[Prime]"], "=", 
   RowBox[{
    UnderscriptBox["\[Sum]", "l"], 
    RowBox[{
     SqrtBox[
      FractionBox[
       SubscriptBox["p", "l"], 
       SubsuperscriptBox["p", "k", "\[Prime]"]]], 
     SubscriptBox["U", 
      RowBox[{"k", " ", "l"}]], 
     SubscriptBox["c", 
      RowBox[{"l", " ", "m"}]]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.7897081500924807`*^9, 3.789708175866349*^9}, {
  3.789709325980299*^9, 
  3.789709329370886*^9}},ExpressionUUID->"8e4c4eae-6ce1-469d-8a2d-\
274e896a26ea"],

Cell["Then, for the primed ensemble", "Text",
 CellChangeTimes->{{3.78968526319775*^9, 3.789685267226124*^9}, {
  3.789708096308947*^9, 3.7897081001691732`*^9}, {3.789709622429269*^9, 
  3.789709624117857*^9}},ExpressionUUID->"fc5d8847-3866-43ed-aaa1-\
7cc7f1ce5a8f"],

Cell[BoxData[{
 FormBox[
  RowBox[{
   SubsuperscriptBox["\[Rho]", 
    RowBox[{"m", " ", "n"}], "\[Prime]"], "=", 
   RowBox[{
    RowBox[{
     UnderscriptBox["\[Sum]", "k"], 
     RowBox[{
      SubsuperscriptBox["p", "k", "\[Prime]"], 
      SubsuperscriptBox["c", 
       RowBox[{"k", " ", "m"}], "\[Prime]"], 
      SubsuperscriptBox["c", 
       RowBox[{"k", " ", "n"}], 
       RowBox[{"\[Prime]", "*"}]]}]}], "=", 
    RowBox[{
     UnderscriptBox["\[Sum]", 
      RowBox[{"k", ",", "i", ",", "j"}]], 
     RowBox[{
      RowBox[{
       SubsuperscriptBox["p", "k", "\[Prime]"], "(", 
       RowBox[{
        SqrtBox[
         FractionBox[
          SubscriptBox["p", "i"], 
          SubsuperscriptBox["p", "k", "\[Prime]"]]], 
        SubscriptBox["U", 
         RowBox[{"k", " ", "i"}]], 
        SubscriptBox["c", 
         RowBox[{"i", " ", "m"}]]}], ")"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SqrtBox[
          FractionBox[
           SubscriptBox["p", "j"], 
           SubsuperscriptBox["p", "k", "\[Prime]"]]], 
         SubscriptBox["U", 
          RowBox[{"k", " ", "j"}]], 
         SubscriptBox["c", 
          RowBox[{"j", " ", "n"}]]}], ")"}], "*"]}]}]}]}], 
  TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{
     UnderscriptBox["\[Sum]", 
      RowBox[{"k", ",", "i", ",", "j"}]], 
     RowBox[{
      SqrtBox[
       RowBox[{
        SubscriptBox["p", "i"], 
        SubscriptBox["p", "j"]}]], 
      SubscriptBox["c", 
       RowBox[{"i", " ", "m"}]], 
      SubsuperscriptBox["c", 
       RowBox[{"j", " ", "n"}], "*"], 
      SubscriptBox["U", 
       RowBox[{"k", " ", "i"}]], 
      SubsuperscriptBox["U", 
       RowBox[{"k", " ", "j"}], "*"]}]}], "=", 
    RowBox[{
     UnderscriptBox["\[Sum]", 
      RowBox[{"k", ",", "i", ",", "j"}]], 
     RowBox[{
      SqrtBox[
       RowBox[{
        SubscriptBox["p", "i"], 
        SubscriptBox["p", "j"]}]], 
      SubscriptBox["c", 
       RowBox[{"i", " ", "m"}]], 
      SubscriptBox[
       RowBox[{
        SubsuperscriptBox["c", 
         RowBox[{"j", " ", "n"}], "*"], "(", 
        SuperscriptBox["U", "\[Dagger]"], ")"}], 
       RowBox[{"j", " ", "k"}]], 
      SubscriptBox["U", 
       RowBox[{"k", " ", "i"}]]}]}]}]}], TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{
     UnderscriptBox["\[Sum]", 
      RowBox[{"i", ",", "j"}]], 
     RowBox[{
      SqrtBox[
       RowBox[{
        SubscriptBox["p", "i"], 
        SubscriptBox["p", "j"]}]], 
      SubscriptBox["c", 
       RowBox[{"i", " ", "m"}]], 
      SubsuperscriptBox["c", 
       RowBox[{"j", " ", "n"}], "*"], 
      SubscriptBox["\[Delta]", 
       RowBox[{"i", " ", "j"}]]}]}], "=", 
    RowBox[{
     RowBox[{
      UnderscriptBox["\[Sum]", "i"], 
      RowBox[{
       SubscriptBox["p", "i"], 
       SubscriptBox["c", 
        RowBox[{"i", " ", "m"}]], 
       SubsuperscriptBox["c", 
        RowBox[{"i", " ", "n"}], "*"]}]}], "=", 
     RowBox[{
      SubscriptBox["\[Rho]", 
       RowBox[{"m", " ", "n"}]], "  ", "\[FilledSquare]"}]}]}]}], 
  TraditionalForm]}], "Equation",
 InitializationCell->True,
 CellChangeTimes->{{3.789685054931988*^9, 3.7896850558043337`*^9}, {
   3.7896851946740932`*^9, 3.7896852905280323`*^9}, {3.789685328015995*^9, 
   3.789685396045554*^9}, 3.789685435829791*^9, {3.789708105781722*^9, 
   3.789708126755472*^9}, {3.7897081870019693`*^9, 3.789708341197031*^9}, 
   3.7897084554604*^9, {3.789708992651347*^9, 3.789708993985017*^9}, {
   3.7897092217637653`*^9, 3.7897093101713877`*^9}, {3.7897094611551733`*^9, 
   3.7897094621918707`*^9}, {3.789709617359655*^9, 
   3.789709619569467*^9}},ExpressionUUID->"c6420881-2044-4de3-a906-\
e77413a57863"]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["Reduced Density Matrix and Entanglement", "Section",
 CellChangeTimes->{{3.78970632770348*^9, 
  3.789706339008264*^9}},ExpressionUUID->"2f43021e-667c-462c-8d8c-\
c319402010fa"],

Cell[CellGroupData[{

Cell[TextData[{
 
 CounterBox["Subsection"],
 ". Motivation for Definition"
}], "Subsection",
 CellChangeTimes->{{3.789719255670093*^9, 
  3.789719266412747*^9}},ExpressionUUID->"cac6db50-7c80-44b1-bcb1-\
0d6275320f98"],

Cell[TextData[{
 "Consider a system made of two subsystems ",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", ",", "B"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a4aa6960-3954-45ad-8e49-8207a244d6a9"],
 ", with basis states ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     TemplateBox[{RowBox[{" ", 
         RowBox[{"m", ",", "A"}]}]},
      "Ket"], "}"}], ",", 
    RowBox[{"{", 
     TemplateBox[{RowBox[{" ", 
         RowBox[{"n", ",", "B"}]}]},
      "Ket"], "}"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "3c2384eb-1ee0-48b8-a892-aede9e96b5fe"],
 " respectively. Then the whole system can be described by the basis ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     TemplateBox[{RowBox[{" ", 
         RowBox[{"m", ",", "A"}]}]},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{RowBox[{" ", 
         RowBox[{"n", ",", "B"}]}]},
      "Ket"]}], "}"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "77a3cc8b-ddf1-4090-8b74-c5f95a03fa85"],
 " (or simply written as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{RowBox[{" ", 
        RowBox[{"m", ",", "A"}]}]},
     "Ket"], 
    TemplateBox[{RowBox[{" ", 
        RowBox[{"n", ",", "B"}]}]},
     "Ket"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "02531483-339a-4b6b-b3ce-a439c3a7e161"],
 " or ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{RowBox[{" ", 
       RowBox[{"m", ",", "n"}]}]},
    "Ket"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "c1e0e888-c1ae-4245-8121-6b84ce1424c7"],
 "). "
}], "Text",
 CellChangeTimes->{{3.7898536961768436`*^9, 3.789853882917345*^9}, {
  3.789854444111496*^9, 
  3.789854473187264*^9}},ExpressionUUID->"b4b0f8a7-3c4a-4cfd-898d-\
53ebc7a7ecca"],

Cell[TextData[{
 "Let the density matrix of the system be ",
 Cell[BoxData[
  FormBox["\[Rho]", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d74e5b45-ed85-46fc-9620-0ca316daa73e"],
 ". If we measure a quantity corresponding to the operator ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["O", "A"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "dd7a2e2d-703d-4118-a52a-e397114cb6c8"],
 " that only acts on ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "3aaeb3e9-7590-4e99-80c6-8ae171691830"],
 ", the average value is"
}], "Text",
 CellChangeTimes->{{3.7898536961768436`*^9, 3.789853927592738*^9}, {
  3.789854055553162*^9, 
  3.789854071271778*^9}},ExpressionUUID->"af1433ab-c9e3-47eb-bb4f-\
f629ba67ce93"],

Cell[BoxData[{
 FormBox[
  RowBox[{
   RowBox[{"\[LeftAngleBracket]", 
    SubscriptBox["O", "A"], "\[RightAngleBracket]"}], "=", 
   RowBox[{
    RowBox[{"Tr", "(", 
     RowBox[{"\[Rho]", " ", 
      SubscriptBox["O", "A"]}], ")"}], "=", 
    RowBox[{
     UnderscriptBox["\[Sum]", 
      RowBox[{"m", ",", "n"}]], " ", 
     RowBox[{
      RowBox[{
       TemplateBox[{RowBox[{"n", ",", "B"}]},
        "Bra"], "\[CircleTimes]", 
       TemplateBox[{RowBox[{"m", ",", "A"}]},
        "Bra"]}], "\[Rho]", " ", 
      SubscriptBox["O", "A"], 
      RowBox[{
       TemplateBox[{RowBox[{" ", 
           RowBox[{"m", ",", "A"}]}]},
        "Ket"], "\[CircleTimes]", 
       TemplateBox[{RowBox[{" ", 
           RowBox[{"n", ",", "B"}]}]},
        "Ket"]}]}]}]}]}], TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    UnderscriptBox["\[Sum]", "m"], 
    RowBox[{
     RowBox[{
      TemplateBox[{RowBox[{"m", ",", "A"}]},
       "Bra"], "(", 
      RowBox[{
       UnderscriptBox["\[Sum]", "n"], 
       RowBox[{
        TemplateBox[{RowBox[{"n", ",", "B"}]},
         "Bra"], "\[Rho]", 
        TemplateBox[{RowBox[{" ", 
            RowBox[{"n", ",", "B"}]}]},
         "Ket"]}]}], ")"}], " ", 
     SubscriptBox["O", "A"], 
     TemplateBox[{RowBox[{" ", 
         RowBox[{"m", ",", "A"}]}]},
      "Ket"]}]}]}], TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    SubscriptBox["Tr", "A"], "[", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["Tr", "B"], "\[Rho]"}], ")"}], " ", 
     SubscriptBox["O", "A"]}], "]"}]}], TraditionalForm]}], "Equation",
 CellChangeTimes->{{3.7898536961768436`*^9, 3.78985402454025*^9}, {
  3.789854107378956*^9, 
  3.78985418491733*^9}},ExpressionUUID->"8cf3a3bc-dba1-4d86-b437-\
b48de3c99aca"],

Cell[TextData[{
 "Therefore, we introduce the ",
 StyleBox["reduced density matrix",
  FontWeight->"Bold"],
 " of ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "cc4777b5-477a-4da9-9f8e-f0d75f98c8b6"],
 ":"
}], "Text",
 CellChangeTimes->{{3.7898541917142267`*^9, 
  3.789854213294778*^9}},ExpressionUUID->"e5f08ba4-bee2-4591-993d-\
028e70478682"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[Rho]", "A"], "\[Congruent]", 
    RowBox[{
     SubscriptBox["Tr", "B"], "\[Rho]"}]}], "=", 
   RowBox[{
    RowBox[{
     UnderscriptBox["\[Sum]", "n"], 
     RowBox[{
      TemplateBox[{RowBox[{"n", ",", "B"}]},
       "Bra"], "\[Rho]", 
      RowBox[{
       TemplateBox[{RowBox[{" ", 
           RowBox[{"n", ",", "B"}]}]},
        "Ket"], " ", "\[DoubleLongRightArrow]", " ", 
       RowBox[{"\[LeftAngleBracket]", 
        SubscriptBox["O", "A"], "\[RightAngleBracket]"}]}]}]}], "=", 
    RowBox[{
     SubscriptBox["Tr", "A"], 
     SubscriptBox["\[Rho]", "A"]}]}]}], TraditionalForm]], "EquationNumbered",\

 CellChangeTimes->{{3.7898541917142267`*^9, 3.789854240477943*^9}, {
  3.789854395584641*^9, 
  3.789854412949398*^9}},ExpressionUUID->"2774aa04-1770-4df7-98b5-\
4c05d1ba3339"],

Cell[TextData[{
 "Similarly, if we want to measure a quantity ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["O", "B"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "dc3d507b-68ff-4ad4-848b-18bd3b338460"],
 " of ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "9aa0e5a2-cd3d-4f39-b261-6e7f5b3cdc0d"],
 ", we use"
}], "Text",
 CellChangeTimes->{{3.7898542445472803`*^9, 
  3.78985426955293*^9}},ExpressionUUID->"0b388956-92b2-4402-8431-\
d8f5084ecc80"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[Rho]", "B"], "\[Congruent]", 
    RowBox[{
     SubscriptBox["Tr", "A"], "\[Rho]"}]}], "=", 
   RowBox[{
    RowBox[{
     UnderscriptBox["\[Sum]", "m"], 
     RowBox[{
      TemplateBox[{RowBox[{"m", ",", "A"}]},
       "Bra"], "\[Rho]", 
      RowBox[{
       TemplateBox[{RowBox[{" ", 
           RowBox[{"m", ",", "A"}]}]},
        "Ket"], " ", "\[DoubleLongRightArrow]", " ", 
       RowBox[{"\[LeftAngleBracket]", 
        SubscriptBox["O", "B"], "\[RightAngleBracket]"}]}]}]}], "=", 
    RowBox[{
     SubscriptBox["Tr", "B"], 
     SubscriptBox["\[Rho]", "B"]}]}]}], TraditionalForm]], "EquationNumbered",\

 CellChangeTimes->{{3.78985427182089*^9, 3.789854300405698*^9}, {
  3.78985441996868*^9, 
  3.789854425972803*^9}},ExpressionUUID->"5e4bf607-8809-43aa-9ac0-\
397ea9d8c57c"]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[{
 
 CounterBox["Subsection"],
 ". Entanglement"
}], "Subsection",
 CellChangeTimes->{{3.789719255670093*^9, 3.789719266412747*^9}, 
   3.789853593369485*^9, {3.789856231551016*^9, 3.789856241393124*^9}, {
   3.789856660013363*^9, 
   3.789856660357196*^9}},ExpressionUUID->"2d38064e-8f06-4c04-a5c8-\
37108e2c88d4"],

Cell[TextData[{
 "If the state of the whole system ",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", " ", "B"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "be756735-bd86-46a5-b5a1-7155258c5465"],
 " ",
 StyleBox["cannot",
  FontSlant->"Italic"],
 " be written as the direct product of the states of the two subsystems ",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", ",", "B"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "2aead2f0-3fca-42d1-b187-8ace3f543f20"],
 ", i.e."
}], "Text",
 CellChangeTimes->{{3.789854769140587*^9, 3.789854865763906*^9}, {
  3.7898552249274178`*^9, 
  3.789855235040148*^9}},ExpressionUUID->"48dee441-8535-4314-8ec1-\
542989ab2913"],

Cell[BoxData[
 FormBox[
  RowBox[{
   TemplateBox[{RowBox[{" ", 
       RowBox[{"\[Psi]", "(", 
         RowBox[{"A", ",", "B"}], ")"}]}]},
    "Ket"], "\[NotEqual]", 
   RowBox[{
    TemplateBox[{RowBox[{" ", 
        RowBox[{
          SubscriptBox["\[Psi]", "A"], "(", "A", ")"}]}]},
     "Ket"], "\[CircleTimes]", 
    TemplateBox[{RowBox[{" ", 
        RowBox[{
          SubscriptBox["\[Psi]", "B"], "(", "B", ")"}]}]},
     "Ket"]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.789854769140587*^9, 
  3.789854901570404*^9}},ExpressionUUID->"8c8ec29c-0c56-4dff-8d34-\
a120e16d88db"],

Cell[TextData[{
 "we say that there is ",
 StyleBox["entanglement",
  FontWeight->"Bold"],
 " between the two subsystems, and the whole system is in an ",
 StyleBox["entangled state",
  FontWeight->"Bold"],
 ". "
}], "Text",
 CellChangeTimes->{{3.789854769140587*^9, 3.789854865305472*^9}, {
  3.789855186713484*^9, 
  3.789855195180657*^9}},ExpressionUUID->"1400a208-0bf9-4534-b409-\
1c9ed11bbc20"],

Cell["Examples: for a system consisting of two spin-1/2, ", "Text",
 CellChangeTimes->{{3.78985490818961*^9, 
  3.789854944906917*^9}},ExpressionUUID->"5e7233ff-66b2-4818-8fca-\
6306a4e691f3"],

Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    RowBox[{
     TemplateBox[{RowBox[{"\[UpArrow]", "\[DownArrow]"}]},
      "Ket"], "+", 
     TemplateBox[{RowBox[{"\[DownArrow]", "\[DownArrow]"}]},
      "Ket"]}], 
    SqrtBox["2"]], "=", 
   RowBox[{
    RowBox[{"(", 
     FractionBox[
      RowBox[{
       TemplateBox[{"\[UpArrow]"},
        "Ket"], "+", 
       TemplateBox[{"\[DownArrow]"},
        "Ket"]}], 
      SqrtBox["2"]], ")"}], "\[CircleTimes]", 
    TemplateBox[{"\[DownArrow]"},
     "Ket"]}]}], TraditionalForm]], "Equation",
 InitializationCell->True,
 CellChangeTimes->{{3.78985490818961*^9, 3.789854915495487*^9}, {
  3.7898549507556133`*^9, 3.7898550530046673`*^9}, {3.789855107506467*^9, 
  3.789855107511011*^9}},ExpressionUUID->"a044903f-caa2-4e15-a17e-\
0341e2cc085d"],

Cell["Thus this is a non-entangled state. But", "Text",
 CellChangeTimes->{{3.789855055262154*^9, 
  3.789855125607774*^9}},ExpressionUUID->"56dd429c-c009-4832-88a3-\
2088d7f2ae6a"],

Cell[BoxData[
 FormBox[
  FractionBox[
   RowBox[{
    TemplateBox[{RowBox[{"\[UpArrow]", "\[UpArrow]"}]},
     "Ket"], "+", 
    TemplateBox[{RowBox[{"\[DownArrow]", "\[DownArrow]"}]},
     "Ket"]}], 
   SqrtBox["2"]], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.789855055262154*^9, 
  3.7898550779140368`*^9}},ExpressionUUID->"a0d430c3-34c5-4429-b72e-\
5eb90053c456"],

Cell["is an entangled state. ", "Text",
 CellChangeTimes->{{3.789855079745558*^9, 
  3.789855084503806*^9}},ExpressionUUID->"8c5e4e35-f5e4-4910-9f90-\
12f5ecdc3125"]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[{
 
 CounterBox["Subsection"],
 ". Schmidt Decomposition"
}], "Subsection",
 CellChangeTimes->{{3.789719255670093*^9, 3.789719266412747*^9}, {
  3.789853597494631*^9, 3.789853603278513*^9}, {3.789856669462206*^9, 
  3.789856675489603*^9}, {3.789857177789259*^9, 
  3.789857178601734*^9}},ExpressionUUID->"22811e0e-f7a9-4004-b7c9-\
bd8ea1be8ec9"],

Cell[TextData[{
 "Suppose ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{RowBox[{" ", "\[Psi]"}]},
    "Ket"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "c514735e-b31a-41f1-a634-d5a3a8dc23d3"],
 " is a pure state of a composite system ",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", " ", "B"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d0499f5c-d6f3-468b-a699-c46a67bf1002"],
 ". Then there exist ",
 StyleBox["orthonormal",
  FontSlant->"Italic"],
 " states ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{RowBox[{" ", 
       RowBox[{"m", ",", "A"}]}]},
    "Ket"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "8fb12406-34e9-41ac-aa27-b645bccbfbfe"],
 " for ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "65b5b4f2-fd1e-46a4-802b-4fa5214c63b8"],
 ", and ",
 StyleBox["orthonormal",
  FontSlant->"Italic"],
 " states ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{RowBox[{" ", 
       RowBox[{"n", ",", "B"}]}]},
    "Ket"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "322bd695-8b27-48a2-a573-fc524b768ed1"],
 " for ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d1915d7d-4664-45d8-86cd-cd1fcadfebbe"],
 " (called the ",
 StyleBox["Schmidt bases",
  FontWeight->"Bold"],
 " for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", ",", "B"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "c26e0108-df39-4266-96da-533ae888171e"],
 ") such that"
}], "Text",
 CellChangeTimes->{{3.789855207088756*^9, 3.789855297462427*^9}, {
  3.789855745035129*^9, 
  3.7898557630587807`*^9}},ExpressionUUID->"dd7136f2-025d-494b-9ec1-\
eb16cc419e16"],

Cell[BoxData[
 FormBox[
  RowBox[{
   TemplateBox[{RowBox[{" ", "\[Psi]"}]},
    "Ket"], "=", 
   RowBox[{
    UnderscriptBox["\[Sum]", "n"], " ", 
    RowBox[{
     SubscriptBox["\[Lambda]", "n"], 
     RowBox[{
      TemplateBox[{RowBox[{" ", 
          RowBox[{"n", ",", "A"}]}]},
       "Ket"], "\[CircleTimes]", 
      TemplateBox[{RowBox[{" ", 
          RowBox[{"n", ",", "B"}]}]},
       "Ket"]}]}]}]}], TraditionalForm]], "EquationNumbered",
 InitializationCell->True,
 CellChangeTimes->{{3.789855207088756*^9, 
  3.789855345315226*^9}},ExpressionUUID->"7097bff7-d786-4abc-b8b0-\
ad61ac79b753"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
