Cell[BoxData[{
 FormBox[
  RowBox[{
   RowBox[{
    UnderscriptBox["\[Sum]", "k"], 
    RowBox[{
     SubsuperscriptBox["p", "k", "\[Prime]"], 
     SubsuperscriptBox["c", 
      RowBox[{"k", " ", "m"}], "\[Prime]"], 
     SubsuperscriptBox["c", 
      RowBox[{"k", " ", "n"}], 
      RowBox[{"\[Prime]", "*"}]]}]}], "=", 
   RowBox[{
    UnderscriptBox["\[Sum]", 
     RowBox[{"k", ",", "i", ",", "j"}]], " ", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["p", "k", "\[Prime]"], "(", 
      RowBox[{
       SubscriptBox["a", 
        RowBox[{"k", " ", "i"}]], 
       SubscriptBox["c", 
        RowBox[{"i", " ", "m"}]]}], ")"}], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["a", 
         RowBox[{"k", " ", "j"}]], 
        SubscriptBox["c", 
         RowBox[{"j", " ", "n"}]]}], ")"}], "*"]}]}]}], 
  TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{
     UnderscriptBox["\[Sum]", 
      RowBox[{"k", ",", "i", ",", "j"}]], " ", 
     RowBox[{
      SubsuperscriptBox["p", "k", "\[Prime]"], 
      SubscriptBox["a", 
       RowBox[{"k", " ", "i"}]], 
      SubsuperscriptBox["a", 
       RowBox[{"k", " ", "j"}], "*"], 
      SubscriptBox["c", 
       RowBox[{"i", " ", "m"}]], 
      SubsuperscriptBox["c", 
       RowBox[{"j", " ", "n"}], "*"]}]}], "=", 
    RowBox[{
     UnderscriptBox["\[Sum]", "k"], 
     RowBox[{
      SubscriptBox["p", "k"], 
      SubscriptBox["c", 
       RowBox[{"k", " ", "m"}]], 
      SubsuperscriptBox["c", 
       RowBox[{"k", " ", "n"}], "*"]}]}]}]}], TraditionalForm]}], "Equation",
 CellChangeTimes->{{3.789718255708164*^9, 3.789718344970374*^9}, {
  3.789718408312154*^9, 
  3.78971843382507*^9}},ExpressionUUID->"30035d29-fd8f-4b50-8763-\
