Cell[BoxData[
 FormBox[
  RowBox[{
   TemplateBox[{RowBox[{" ", 
       SubsuperscriptBox["\[Psi]", "k", "\[Prime]"]}]},
    "Ket"], "=", 
   RowBox[{
    RowBox[{
     UnderscriptBox["\[Sum]", "l"], " ", 
     RowBox[{
      SubscriptBox["a", 
       RowBox[{"k", " ", "l"}]], 
      TemplateBox[{RowBox[{" ", 
          SubscriptBox["\[Psi]", "l"]}]},
       "Ket"]}]}], "=", 
    RowBox[{
     RowBox[{
      UnderscriptBox["\[Sum]", 
       RowBox[{"l", ",", "m"}]], 
      RowBox[{
       SubscriptBox["a", 
        RowBox[{"k", " ", "l"}]], 
       SubscriptBox["c", 
        RowBox[{"l", " ", "m"}]], 
       TemplateBox[{RowBox[{" ", "m"}]},
        "Ket"]}]}], "\[Congruent]", 
     RowBox[{
      UnderscriptBox["\[Sum]", "m"], " ", 
      RowBox[{
       SubsuperscriptBox["c", 
        RowBox[{"k", " ", "m"}], "\[Prime]"], 
       TemplateBox[{RowBox[{" ", "m"}]},
        "Ket"]}]}]}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.789717989873088*^9, 3.789717995812714*^9}, {
