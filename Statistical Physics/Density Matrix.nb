Cell["The Schr\[ODoubleDot]dinger equation:", "Text",
 CellChangeTimes->{{3.7898581396234922`*^9, 
  3.789858163027225*^9}},ExpressionUUID->"cc8db5ae-9b38-478b-b375-\
a5c90e8a3b7d"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[ImaginaryI]", " ", "\[HBar]", " ", 
    FractionBox[
     RowBox[{"\[PartialD]", 
      TemplateBox[{RowBox[{" ", "\[Psi]"}]},
       "Ket"]}], 
     RowBox[{"\[PartialD]", "t"}]]}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"H", 
      TemplateBox[{RowBox[{" ", "\[Psi]"}]},
       "Ket"]}], 
     OverscriptBox["\[RightArrow]", 
      RowBox[{"Hermitian", " ", "Conjugate"}]], 
     RowBox[{
      RowBox[{"-", "\[ImaginaryI]"}], " ", "\[HBar]", " ", 
      FractionBox[
       RowBox[{"\[PartialD]", 
        TemplateBox[{"\[Psi]"},
         "Bra"]}], 
       RowBox[{"\[PartialD]", "t"}]]}]}], "=", 
    RowBox[{
     TemplateBox[{"\[Psi]"},
      "Bra"], "H"}]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.7898581396234922`*^9, 
  3.789858247082225*^9}},ExpressionUUID->"9341817b-06ce-48a9-b4b1-\
829d03675868"],

