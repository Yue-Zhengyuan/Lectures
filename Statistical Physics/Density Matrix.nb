 FormBox[
  RowBox[{
   RowBox[{"\[LeftAngleBracket]", "O", "\[RightAngleBracket]"}], "=", 
   RowBox[{
    RowBox[{
     UnderscriptBox["\[Sum]", 
      RowBox[{"k", ",", "n"}]], 
