Cell[TextData[{
 "To relate ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalOmega]", ",", "\[Mu]"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "f4718f75-f019-407f-b71c-b7d9d4d763a9"],
