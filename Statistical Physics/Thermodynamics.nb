Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[CapitalDelta]", " ", 
    SuperscriptBox["f", "2"]}], "\[Congruent]", 
   RowBox[{"\[LeftAngleBracket]", 
    SuperscriptBox[
