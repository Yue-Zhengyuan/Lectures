  3.790573136800706*^9}},ExpressionUUID->"4d8f26c4-0a20-4964-b65a-\
11ff8d5724f0"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    UnderscriptBox["\[Sum]", 
     RowBox[{"n", ",", "N"}]], " ", 
    SubscriptBox["w", 
     RowBox[{"n", " ", "N"}]]}], "=", 
   RowBox[{
    RowBox[{
     FractionBox["1", "\[CapitalXi]"], 
     RowBox[{
      UnderscriptBox["\[Sum]", 
       RowBox[{"n", ",", "N"}]], " ", 
